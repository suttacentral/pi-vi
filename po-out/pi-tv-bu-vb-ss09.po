#. lang="pi"><section class="sutta" id="pi-tv-bu-vb-ss9"><article><div
#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-ss9:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss9:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss9:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-ss9:4.1"
msgid "9. Dutiyaduṭṭhadosasikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.167"></a>
msgctxt "pi-tv-bu-vb-ss9:5.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

#. VAR: chagalakaṃ → chakalakaṃ (si, s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss9:5.2"
msgid "Tena kho pana samayena mettiyabhūmajakā bhikkhū gijjhakūṭā pabbatā orohantā addasaṃsu chagalakaṃ ajikāya vippaṭipajjantaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.3"
msgid "Disvāna evamāhaṃsu—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.4"
msgid "handa mayaṃ, āvuso, imaṃ chagalakaṃ dabbaṃ mallaputtaṃ nāma karoma."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.5"
msgid "Imaṃ ajikaṃ mettiyaṃ nāma bhikkhuniṃ karoma."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.6"
msgid "Evaṃ mayaṃ voharissāma."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.7"
msgid "Pubbe mayaṃ, āvuso, dabbaṃ mallaputtaṃ sutena avocumhā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.8"
msgid "Idāni pana amhehi sāmaṃ diṭṭho mettiyāya bhikkhuniyā vippaṭipajjanto”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.9"
msgid "Te taṃ chagalakaṃ dabbaṃ mallaputtaṃ nāma akaṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.10"
msgid "Taṃ ajikaṃ mettiyaṃ nāma bhikkhuniṃ akaṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.11"
msgid "Te bhikkhūnaṃ ārocesuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.12"
msgid "“pubbe mayaṃ, āvuso, dabbaṃ mallaputtaṃ sutena avocumhā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.13"
msgid "Idāni pana amhehi sāmaṃ diṭṭho mettiyāya bhikkhuniyā vippaṭipajjanto”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.14"
msgid "Bhikkhū evamāhaṃsu—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.15"
msgid "“māvuso, evaṃ avacuttha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:5.16"
msgid "Nāyasmā dabbo mallaputto evaṃ karissatī”ti."
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "pi-tv-bu-vb-ss9:6.1"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.2"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ dabbaṃ mallaputtaṃ paṭipucchi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.3"
msgid "sarasi tvaṃ, dabba, evarūpaṃ kattā yathayime bhikkhū āhaṃsū”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.4"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.5"
msgid "Dutiyampi kho bhagavā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.6"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.7"
msgid "tatiyampi kho bhagavā āyasmantaṃ dabbaṃ mallaputtaṃ etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.8"
msgid "sarasi tvaṃ, dabba, evarūpaṃ kattā yathayime bhikkhū āhaṃsū”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.9"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.10"
msgid "“Na kho, dabba, dabbā evaṃ nibbeṭhenti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.11"
msgid "Sace tayā kataṃ katanti vadehi, sace tayā akataṃ akatanti vadehī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.12"
msgid "“Yato ahaṃ, bhante, jāto nābhijānāmi supinantenapi methunadhammaṃ paṭisevitā, pageva jāgaro”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.13"
msgid "Atha kho bhagavā bhikkhū āmantesi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.14"
msgid "tena hi, bhikkhave, ime bhikkhū anuyuñjathā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:6.15"
msgid "Idaṃ vatvā bhagavā uṭṭhāyāsanā vihāraṃ pāvisi."
msgstr ""

#. </p><p><a class="sc" id="3"></a>
msgctxt "pi-tv-bu-vb-ss9:7.1"
msgid "Atha kho te bhikkhū mettiyabhūmajake bhikkhū anuyuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:7.2"
msgid "Te bhikkhūhi anuyuñjīyamānā bhikkhūnaṃ etamatthaṃ ārocesuṃ—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:7.3"
msgid "kiṃ pana tumhe, āvuso, āyasmantaṃ dabbaṃ mallaputtaṃ aññabhāgiyassa adhikaraṇassa kiñci desaṃ lesamattaṃ upādāya pārājikena dhammena anuddhaṃsethā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:7.4"
msgid "“Evamāvuso”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:7.5"
msgid "Ye te bhikkhū appicchā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:7.6"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:7.7"
msgid "te ujjhāyanti khiyyanti vipācenti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:7.8"
msgid "kathañhi nāma mettiyabhūmajakā bhikkhū āyasmantaṃ dabbaṃ mallaputtaṃ aññabhāgiyassa adhikaraṇassa kiñci desaṃ lesamattaṃ upādāya pārājikena dhammena anuddhaṃsessantī”ti."
msgstr ""

#. </p><p><a class="sc" id="4"></a>
msgctxt "pi-tv-bu-vb-ss9:8.1"
msgid "Atha kho te bhikkhū mettiyabhūmajake bhikkhū anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.3"
msgid "“saccaṃ kira tumhe, bhikkhave, dabbaṃ mallaputtaṃ aññabhāgiyassa adhikaraṇassa kiñci desaṃ lesamattaṃ upādāya pārājikena dhammena anuddhaṃsethā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.5"
msgid "Vigarahi buddho bhagavā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.6"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.7"
msgid "“kathañhi nāma tumhe, moghapurisā, dabbaṃ mallaputtaṃ aññabhāgiyassa adhikaraṇassa kiñci desaṃ lesamattaṃ upādāya pārājikena dhammena anuddhaṃsessatha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.8"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:8.10"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="5"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.168"></a>
msgctxt "pi-tv-bu-vb-ss9:9.1"
msgid "<strong class=\"rule\">“Yo pana bhikkhu bhikkhuṃ duṭṭho doso appatīto aññabhāgiyassa adhikaraṇassa kiñci desaṃ lesamattaṃ upādāya pārājikena dhammena anuddhaṃseyya—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:9.2"
msgid "appeva nāma naṃ imamhā brahmacariyā cāveyyan’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:9.3"
msgid "Tato aparena samayena samanuggāhīyamāno vā asamanuggāhīyamāno vā aññabhāgiyañceva taṃ adhikaraṇaṃ hoti koci deso lesamatto upādinno, bhikkhu ca dosaṃ patiṭṭhāti, saṃghādiseso”</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:9.4"
msgid "(<em>9</em>:<strong>13</strong>)"
msgstr ""

#. </p><p><a class="sc" id="6"></a>
msgctxt "pi-tv-bu-vb-ss9:10.1"
msgid "<strong>Yo panā</strong>ti yo yādiso …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:10.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:10.3"
msgid "<strong>bhikkhū</strong>ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:10.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:10.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. </p><p><a class="sc" id="7"></a>
msgctxt "pi-tv-bu-vb-ss9:11.1"
msgid "<strong>Bhikkhun</strong>ti aññaṃ bhikkhuṃ."
msgstr ""

#. </p><p><a class="sc" id="8"></a>
msgctxt "pi-tv-bu-vb-ss9:12.1"
msgid "<strong>Duṭṭho doso</strong>ti kupito anattamano anabhiraddho āhatacitto khilajāto."
msgstr ""

#. </p><p><a class="sc" id="9"></a>
msgctxt "pi-tv-bu-vb-ss9:13.1"
msgid "<strong>Appatīto</strong>ti tena ca kopena, tena ca dosena, tāya ca anattamanatāya, tāya ca anabhiraddhiyā appatīto hoti."
msgstr ""

#. </p><p><a class="sc" id="10"></a>
msgctxt "pi-tv-bu-vb-ss9:14.1"
msgid "<strong>Aññabhāgiyassa adhikaraṇassā</strong>ti āpattaññabhāgiyaṃ vā hoti adhikaraṇaññabhāgiyaṃ vā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:14.2"
msgid "Kathaṃ adhikaraṇaṃ adhikaraṇassa aññabhāgiyaṃ?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:14.3"
msgid "Vivādādhikaraṇaṃ anuvādādhikaraṇassa āpattādhikaraṇassa kiccādhikaraṇassa aññabhāgiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:14.4"
msgid "Anuvādādhikaraṇaṃ āpattādhikaraṇassa kiccādhikaraṇassa vivādādhikaraṇassa aññabhāgiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:14.5"
msgid "Āpattādhikaraṇaṃ kiccādhikaraṇassa vivādādhikaraṇassa anuvādādhikaraṇassa aññabhāgiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:14.6"
msgid "Kiccādhikaraṇaṃ vivādādhikaraṇassa anuvādādhikaraṇassa āpattādhikaraṇassa aññabhāgiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:14.7"
msgid "Evaṃ adhikaraṇaṃ adhikaraṇassa aññabhāgiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="11"></a>
msgctxt "pi-tv-bu-vb-ss9:15.1"
msgid "Kathaṃ adhikaraṇaṃ adhikaraṇassa tabbhāgiyaṃ?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:15.2"
msgid "Vivādādhikaraṇaṃ vivādādhikaraṇassa tabbhāgiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:15.3"
msgid "Anuvādādhikaraṇaṃ anuvādādhikaraṇassa tabbhāgiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:15.4"
msgid "Āpattādhikaraṇaṃ āpattādhikaraṇassa siyā tabbhāgiyaṃ siyā aññabhāgiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="12"></a>
msgctxt "pi-tv-bu-vb-ss9:16.1"
msgid "Kathaṃ āpattādhikaraṇaṃ āpattādhikaraṇassa aññabhāgiyaṃ?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:16.2"
msgid "Methunadhammapārājikāpatti adinnādānapārājikāpattiyā manussaviggahapārājikāpattiyā uttarimanussadhammapārājikāpattiyā aññabhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:16.3"
msgid "Adinnādānapārājikāpatti manussaviggahapārājikāpattiyā uttarimanussadhammapārājikāpattiyā methunadhammapārājikāpattiyā aññabhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:16.4"
msgid "Manussaviggahapārājikāpatti uttarimanussadhammapārājikāpattiyā methunadhammapārājikāpattiyā adinnādānapārājikāpattiyā aññabhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:16.5"
msgid "Uttarimanussadhammapārājikāpatti methunadhammapārājikāpattiyā adinnādānapārājikāpattiyā manussaviggahapārājikāpattiyā aññabhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:16.6"
msgid "Evaṃ āpattādhikaraṇaṃ āpattādhikaraṇassa aññabhāgiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="13"></a>
msgctxt "pi-tv-bu-vb-ss9:17.1"
msgid "Kathaṃ āpattādhikaraṇaṃ āpattādhikaraṇassa tabbhāgiyaṃ?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:17.2"
msgid "Methunadhammapārājikāpatti methunadhammapārājikāpattiyā tabbhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:17.3"
msgid "Adinnādānapārājikāpatti adinnādānapārājikāpattiyā tabbhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:17.4"
msgid "Manussaviggahapārājikāpatti manussaviggahapārājikāpattiyā tabbhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:17.5"
msgid "Uttarimanussadhammapārājikāpatti uttarimanussadhammapārājikāpattiyā tabbhāgiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:17.6"
msgid "Evaṃ āpattādhikaraṇaṃ āpattādhikaraṇassa tabbhāgiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="14"></a>
msgctxt "pi-tv-bu-vb-ss9:18.1"
msgid "Kiccādhikaraṇaṃ kiccādhikaraṇassa tabbhāgiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:18.2"
msgid "Evaṃ adhikaraṇaṃ adhikaraṇassa tabbhāgiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="15"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.169"></a>
msgctxt "pi-tv-bu-vb-ss9:19.1"
msgid "<strong>Kiñci desaṃ lesamattaṃ upādāyā</strong>ti leso nāma dasa lesā—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:19.2"
msgid "jātileso, nāmaleso, gottaleso, liṅgaleso, āpattileso, pattaleso, cīvaraleso, upajjhāyaleso, ācariyaleso, senāsanaleso."
msgstr ""

#. </p><p><a class="sc" id="16"></a>
#. VAR: diṭṭho → ettha \
msgctxt "pi-tv-bu-vb-ss9:20.1"
msgid "<strong>Jātileso</strong> nāma khattiyo diṭṭho hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:20.2"
msgid "Aññaṃ khattiyaṃ passitvā codeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:20.3"
msgid "khattiyo mayā diṭṭho."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:20.4"
msgid "Pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi, asakyaputtiyosi, natthi tayā saddhiṃ uposatho vā pavāraṇā vā saṃghakammaṃ vā”ti, āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="17"></a>
msgctxt "pi-tv-bu-vb-ss9:21.1"
msgid "Brāhmaṇo diṭṭho hoti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.3"
msgid "vesso diṭṭho hoti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.5"
msgid "suddo diṭṭho hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.6"
msgid "Aññaṃ suddaṃ passitvā codeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.7"
msgid "suddo mayā diṭṭho."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.8"
msgid "Pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi, asakyaputtiyosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:21.10"
msgid "āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="18"></a>
msgctxt "pi-tv-bu-vb-ss9:22.1"
msgid "<strong>Nāmaleso</strong> nāma buddharakkhito diṭṭho hoti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.3"
msgid "dhammarakkhito diṭṭho hoti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.5"
msgid "saṃgharakkhito diṭṭho hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.6"
msgid "Aññaṃ saṃgharakkhitaṃ passitvā codeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.7"
msgid "saṃgharakkhito mayā diṭṭho."
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.8"
msgid "Pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi, asakyaputtiyosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:22.10"
msgid "āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="19"></a>
msgctxt "pi-tv-bu-vb-ss9:23.1"
msgid "<strong>Gottaleso</strong> nāma gotamo diṭṭho hoti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:23.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:23.3"
msgid "moggallāno diṭṭho hoti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:23.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:23.5"
msgid "kaccāyano diṭṭho hoti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:23.6"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss9:23.7"
msgid "vāsiṭṭho diṭṭho hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
msgstr ""

