#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-np13:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np13:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np13:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np13:4.1"
msgid "Kosiyavagga"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-np13:5.1"
msgid "13. Dvebhāgasikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a>
msgctxt "pi-tv-bu-vb-np13:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū—“"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.3"
msgid "bhagavatā paṭikkhittaṃ suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpetun”ti, te thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpenti."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.4"
msgid "Ye te bhikkhū appicchā …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.5"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.6"
msgid "te ujjhāyanti khiyyanti vipācenti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.7"
msgid "kathañhi nāma chabbaggiyā bhikkhū thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpessantī”ti."
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "pi-tv-bu-vb-np13:7.1"
msgid "Atha kho te bhikkhū chabbaggiye bhikkhū anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.3"
msgid "“saccaṃ kira tumhe, bhikkhave, thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpethā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.5"
msgid "Vigarahi buddho bhagavā …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.6"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.7"
msgid "kathañhi nāma tumhe, moghapurisā, thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpessatha."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.8"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.10"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="3"></a>
msgctxt "pi-tv-bu-vb-np13:8.1"
msgid "<strong class=\"rule\">“Navaṃ pana bhikkhunā santhataṃ kārayamānena dve bhāgā suddhakāḷakānaṃ eḷakalomānaṃ ādātabbā tatiyaṃ odātānaṃ catutthaṃ gocariyānaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:8.2"
msgid "Anādā ce bhikkhu dve bhāge suddhakāḷakānaṃ eḷakalomānaṃ tatiyaṃ odātānaṃ catutthaṃ gocariyānaṃ navaṃ santhataṃ kārāpeyya, nissaggiyaṃ pācittiyan”</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:8.3"
msgid "(<em>13</em>:<strong>32</strong>)"
msgstr ""

#. </p><p><a class="sc" id="4"></a>
msgctxt "pi-tv-bu-vb-np13:9.1"
msgid "<strong>Navaṃ</strong> nāma karaṇaṃ upādāya vuccati."
msgstr ""

#. </p><p><a class="sc" id="5"></a>
msgctxt "pi-tv-bu-vb-np13:10.1"
msgid "<strong>Santhataṃ</strong> nāma santharitvā kataṃ hoti avāyimaṃ."
msgstr ""

#. </p><p><a class="sc" id="6"></a>
msgctxt "pi-tv-bu-vb-np13:11.1"
msgid "<strong>Kārayamānenā</strong>ti karonto vā kārāpento vā."
msgstr ""

#. </p><p><a class="sc" id="7"></a>
msgctxt "pi-tv-bu-vb-np13:12.1"
msgid "<strong>Dve bhāgā suddhakāḷakānaṃ eḷakalomānaṃ ādātabbā</strong>ti dhārayitvā dve tulā ādātabbā."
msgstr ""

#. </p><p><a class="sc" id="8"></a>
msgctxt "pi-tv-bu-vb-np13:13.1"
msgid "<strong>Tatiyaṃ odātānan</strong>ti tulaṃ odātānaṃ."
msgstr ""

#. </p><p><a class="sc" id="9"></a>
msgctxt "pi-tv-bu-vb-np13:14.1"
msgid "<strong>Catutthaṃ gocariyānan</strong>ti tulaṃ gocariyānaṃ."
msgstr ""

#. </p><p><a class="sc" id="10"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.227"></a>
msgctxt "pi-tv-bu-vb-np13:15.1"
msgid "<strong>Anādā ce bhikkhu dve bhāge suddhakāḷakānaṃ eḷakalomānaṃ tatiyaṃ odātānaṃ catutthaṃ gocariyānan</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.2"
msgid "Anādiyitvā dve tule suddhakāḷakānaṃ eḷakalomānaṃ tulaṃ odātānaṃ tulaṃ gocariyānaṃ navaṃ santhataṃ karoti vā kārāpeti vā payoge dukkaṭaṃ, paṭilābhena nissaggiyaṃ hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.3"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā puggalassa vā."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.4"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.5"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.6"
msgid "“idaṃ me, bhante, santhataṃ anādiyitvā dve tule suddhakāḷakānaṃ eḷakalomānaṃ tulaṃ odātānaṃ tulaṃ gocariyānaṃ kārāpitaṃ nissaggiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.7"
msgid "Imāhaṃ saṃghassa nissajjāmī”ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.8"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.9"
msgid "dadeyyāti …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.10"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.11"
msgid "dadeyyunti …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.12"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.13"
msgid "āyasmato dammīti."
msgstr ""

#. </p><p><a class="sc" id="11"></a>
msgctxt "pi-tv-bu-vb-np13:16.1"
msgid "Attanā vippakataṃ attanā pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:16.2"
msgid "Attanā vippakataṃ parehi pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:16.3"
msgid "Parehi vippakataṃ attanā pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:16.4"
msgid "Parehi vippakataṃ parehi pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="12"></a>
msgctxt "pi-tv-bu-vb-np13:17.1"
msgid "Aññassatthāya karoti vā kārāpeti vā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:17.2"
msgid "Aññena kataṃ paṭilabhitvā paribhuñjati, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="13"></a>
msgctxt "pi-tv-bu-vb-np13:18.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:18.2"
msgid "tulaṃ odātānaṃ tulaṃ gocariyānaṃ ādiyitvā karoti, bahutaraṃ odātānaṃ bahutaraṃ gocariyānaṃ ādiyitvā karoti, suddhaṃ odātānaṃ suddhaṃ gocariyānaṃ ādiyitvā karoti, vitānaṃ vā bhūmattharaṇaṃ vā sāṇipākāraṃ vā bhisiṃ vā bibbohanaṃ vā karoti, ummattakassa, ādikammikassāti."
msgstr ""

#. </p><p class="endsutta"><a class="sc" id="14"></a>
msgctxt "pi-tv-bu-vb-np13:19.1"
msgid "Dvebhāgasikkhāpadaṃ niṭṭhitaṃ tatiyaṃ."
msgstr ""

