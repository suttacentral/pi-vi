#. lang="pi"><section class="sutta" id="pi-tv-bu-vb-ss1"><article><div
#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-ss1:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss1:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss1:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-ss1:4.1"
msgid "1. Sukkavissaṭṭhisikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.110"></a>
msgctxt "pi-tv-bu-vb-ss1:5.1"
msgid "Ime kho panāyasmanto terasa saṃghādisesā"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-ss1:5.2"
msgid "dhammā uddesaṃ āgacchanti."
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "pi-tv-bu-vb-ss1:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.2"
msgid "Tena kho pana samayena āyasmā seyyasako anabhirato brahmacariyaṃ carati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.3"
msgid "So tena kiso hoti lūkho dubbaṇṇo uppaṇḍuppaṇḍukajāto dhamanisanthatagatto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.4"
msgid "Addasa kho āyasmā udāyī āyasmantaṃ seyyasakaṃ kisaṃ lūkhaṃ dubbaṇṇaṃ uppaṇḍuppaṇḍukajātaṃ dhamanisanthatagattaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.5"
msgid "Disvāna āyasmantaṃ seyyasakaṃ etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.6"
msgid "kissa tvaṃ, āvuso seyyasaka, kiso lūkho dubbaṇṇo uppaṇḍuppaṇḍukajāto dhamanisanthatagatto?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.7"
msgid "Kacci no tvaṃ, āvuso seyyasaka, anabhirato brahmacariyaṃ carasī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.8"
msgid "“Evamāvuso”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.9"
msgid "“Tena hi tvaṃ, āvuso seyyasaka, yāvadatthaṃ bhuñja yāvadatthaṃ supa yāvadatthaṃ nhāya."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.10"
msgid "Yāvadatthaṃ bhuñjitvā yāvadatthaṃ supitvā yāvadatthaṃ nhāyitvā yadā te anabhirati uppajjati rāgo cittaṃ anuddhaṃseti tadā hatthena upakkamitvā asuciṃ mocehī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.11"
msgid "“Kiṃ nu kho, āvuso, kappati evarūpaṃ kātun”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.12"
msgid "“Āma, āvuso."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:6.13"
msgid "Ahampi evaṃ karomī”ti."
msgstr ""

#. </p><p><a class="sc" id="3"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.111"></a>
msgctxt "pi-tv-bu-vb-ss1:7.1"
msgid "Atha kho āyasmā seyyasako yāvadatthaṃ bhuñji yāvadatthaṃ supi yāvadatthaṃ nhāyi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.2"
msgid "Yāvadatthaṃ bhuñjitvā yāvadatthaṃ supitvā yāvadatthaṃ nhāyitvā yadā anabhirati uppajjati rāgo cittaṃ anuddhaṃseti tadā hatthena upakkamitvā asuciṃ mocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.3"
msgid "Atha kho āyasmā seyyasako aparena samayena vaṇṇavā ahosi pīṇindriyo pasannamukhavaṇṇo vippasannachavivaṇṇo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.4"
msgid "Atha kho āyasmato seyyasakassa sahāyakā bhikkhū āyasmantaṃ seyyasakaṃ etadavocuṃ—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.5"
msgid "pubbe kho tvaṃ, āvuso seyyasaka, kiso ahosi lūkho dubbaṇṇo uppaṇḍuppaṇḍukajāto dhamanisanthatagatto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.6"
msgid "So dāni tvaṃ etarahi vaṇṇavā pīṇindriyo pasannamukhavaṇṇo vippasannachavivaṇṇo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.7"
msgid "Kiṃ nu kho tvaṃ, āvuso seyyasaka, bhesajjaṃ karosī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.8"
msgid "“Na kho ahaṃ, āvuso, bhesajjaṃ karomi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.9"
msgid "Api cāhaṃ yāvadatthaṃ bhuñjāmi yāvadatthaṃ supāmi yāvadatthaṃ nhāyāmi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.10"
msgid "Yāvadatthaṃ bhuñjitvā yāvadatthaṃ supitvā yāvadatthaṃ nhāyitvā yadā me anabhirati uppajjati rāgo cittaṃ anuddhaṃseti tadā hatthena upakkamitvā asuciṃ mocemī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.11"
msgid "“Kiṃ pana tvaṃ, āvuso seyyasaka, yeneva hatthena saddhādeyyaṃ bhuñjasi teneva hatthena upakkamitvā asuciṃ mocesī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.12"
msgid "“Evamāvuso”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.13"
msgid "Ye te bhikkhū appicchā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.14"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.15"
msgid "te ujjhāyanti khiyyanti vipācenti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:7.16"
msgid "kathañhi nāma āyasmā seyyasako hatthena upakkamitvā asuciṃ mocessatī”ti."
msgstr ""

#. </p><p><a class="sc" id="4"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.112"></a>
msgctxt "pi-tv-bu-vb-ss1:8.1"
msgid "Atha kho te bhikkhū āyasmantaṃ seyyasakaṃ anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.2"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ seyyasakaṃ paṭipucchi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.3"
msgid "saccaṃ kira tvaṃ, seyyasaka, hatthena upakkamitvā asuciṃ mocesī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.5"
msgid "Vigarahi buddho bhagavā—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.6"
msgid "ananucchavikaṃ, moghapurisa, ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.7"
msgid "Kathañhi nāma tvaṃ, moghapurisa, hatthena upakkamitvā asuciṃ mocessasi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.8"
msgid "Nanu mayā, moghapurisa, anekapariyāyena virāgāya dhammo desito no sarāgāya, visaññogāya dhammo desito no saññogāya, anupādānāya dhammo desito no saupādānāya."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.9"
msgid "Tattha nāma tvaṃ, moghapurisa, mayā virāgāya dhamme desite sarāgāya cetessasi, visaññogāya dhamme desite saññogāya cetessasi, anupādānāya dhamme desite saupādānāya cetessasi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.10"
msgid "Nanu mayā, moghapurisa, anekapariyāyena rāgavirāgāya dhammo desito, madanimmadanāya pipāsavinayāya ālayasamugghātāya vaṭṭupacchedāya taṇhākkhayāya virāgāya nirodhāya nibbānāya dhammo desito?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.11"
msgid "Nanu mayā, moghapurisa, anekapariyāyena kāmānaṃ pahānaṃ akkhātaṃ, kāmasaññānaṃ pariññā akkhātā, kāmapipāsānaṃ paṭivinayo akkhāto, kāmavitakkānaṃ samugghāto akkhāto, kāmapariḷāhānaṃ vūpasamo akkhāto?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.12"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya, pasannānaṃ vā bhiyyobhāvāya."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.13"
msgid "Atha khvetaṃ, moghapurisa, appasannānañceva appasādāya, pasannānañca ekaccānaṃ aññathattāyā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.14"
msgid "Atha kho bhagavā āyasmantaṃ seyyasakaṃ anekapariyāyena vigarahitvā dubbharatāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.15"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:8.16"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="5"></a>
#. VAR: sukkavissaṭṭhi → sukkavisaṭṭhi (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss1:9.1"
msgid "<strong class=\"rule\">“Sañcetanikā sukkavissaṭṭhi saṃghādiseso”</strong>ti."
msgstr ""

#. </p><p><a class="sc" id="6"></a>
msgctxt "pi-tv-bu-vb-ss1:10.1"
msgid "Evañcidaṃ bhagavatā bhikkhūnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="7"></a>
msgctxt "pi-tv-bu-vb-ss1:11.1"
msgid "Tena kho pana samayena bhikkhū paṇītabhojanāni bhuñjitvā muṭṭhassatī asampajānā niddaṃ okkamanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.2"
msgid "Tesaṃ muṭṭhassatīnaṃ asampajānānaṃ niddaṃ okkamantānaṃ supinantena asuci muccati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.3"
msgid "Tesaṃ kukkuccaṃ ahosi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.4"
msgid "bhagavatā sikkhāpadaṃ paññattaṃ—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.5"
msgid "sañcetanikā sukkavissaṭṭhi saṃghādiseso’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.6"
msgid "Amhākañca supinantena asuci muccati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.7"
msgid "Atthi cettha cetanā labbhati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.8"
msgid "Kacci nu kho mayaṃ saṃghādisesaṃ āpattiṃ āpannā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.9"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.10"
msgid "“Atthesā, bhikkhave, cetanā;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.11"
msgid "sā ca kho abbohārikāti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:11.12"
msgid "Evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="8"></a>
#. VAR: sukkavissaṭṭhi → sukkavisaṭṭhi (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss1:12.1"
msgid "<strong class=\"rule\">“Sañcetanikā sukkavissaṭṭhi aññatra supinantā saṃghādiseso”</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:12.2"
msgid "(<em>1</em>:<strong>5</strong>)"
msgstr ""

#. </p><p><a class="sc" id="9"></a>
msgctxt "pi-tv-bu-vb-ss1:13.1"
msgid "<strong>Sañcetanikā</strong>ti jānanto sañjānanto cecca abhivitaritvā vītikkamo."
msgstr ""

#. </p><p><a class="sc" id="10"></a>
msgctxt "pi-tv-bu-vb-ss1:14.1"
msgid "<strong>Sukkan</strong>ti dasa sukkāni—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:14.2"
msgid "nīlaṃ pītakaṃ lohitakaṃ odātaṃ takkavaṇṇaṃ dakavaṇṇaṃ telavaṇṇaṃ khīravaṇṇaṃ dadhivaṇṇaṃ sappivaṇṇaṃ."
msgstr ""

#. </p><p><a class="sc" id="11"></a>
msgctxt "pi-tv-bu-vb-ss1:15.1"
msgid "<strong>Vissaṭṭhī</strong>ti ṭhānato cāvanā vuccati vissaṭṭhīti."
msgstr ""

#. </p><p><a class="sc" id="12"></a>
msgctxt "pi-tv-bu-vb-ss1:16.1"
msgid "<strong>Aññatra supinantā</strong>ti ṭhapetvā supinantaṃ."
msgstr ""

#. </p><p><a class="sc" id="13"></a>
msgctxt "pi-tv-bu-vb-ss1:17.1"
msgid "<strong>Saṃghādiseso</strong>ti saṃghova tassā āpattiyā parivāsaṃ deti, mūlāya paṭikassati, mānattaṃ deti, abbheti;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:17.2"
msgid "na sambahulā, na ekapuggalo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:17.3"
msgid "Tena vuccati—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:17.4"
msgid "saṃghādiseso”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:17.5"
msgid "Tasseva āpattinikāyassa nāmakammaṃ adhivacanaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:17.6"
msgid "Tenapi vuccati—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:17.7"
msgid "saṃghādiseso”ti."
msgstr ""

#. </p><p><a class="sc" id="14"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.113"></a>
msgctxt "pi-tv-bu-vb-ss1:18.1"
msgid "Ajjhattarūpe moceti, bahiddhārūpe moceti, ajjhattabahiddhārūpe moceti, ākāse kaṭiṃ kampento moceti;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:18.2"
msgid "rāgūpatthambhe moceti, vaccūpatthambhe moceti, passāvūpatthambhe moceti, vātūpatthambhe moceti, uccāliṅgapāṇakadaṭṭhūpatthambhe moceti;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:18.3"
msgid "ārogyatthāya moceti, sukhatthāya moceti, bhesajjatthāya moceti, dānatthāya moceti, puññatthāya moceti, yaññatthāya moceti, saggatthāya moceti, bījatthāya moceti, vīmaṃsatthāya moceti, davatthāya moceti;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:18.4"
msgid "nīlaṃ moceti, pītakaṃ moceti, lohitakaṃ moceti, odātaṃ moceti, takkavaṇṇaṃ moceti, dakavaṇṇaṃ moceti, telavaṇṇaṃ moceti, khīravaṇṇaṃ moceti, dadhivaṇṇaṃ moceti, sappivaṇṇaṃ moceti."
msgstr ""

#. </p><p><a class="sc" id="15"></a>
#. VAR: upādinne → upādinna (s1-3, pts1) | upādiṇṇe (mr)
msgctxt "pi-tv-bu-vb-ss1:19.1"
msgid "<strong>Ajjhattarūpe</strong>ti ajjhattaṃ upādinne rūpe."
msgstr ""

#. </p><p><a class="sc" id="16"></a>
msgctxt "pi-tv-bu-vb-ss1:20.1"
msgid "<strong>Bahiddhārūpe</strong>ti bahiddhā upādinne vā anupādinne vā."
msgstr ""

#. </p><p><a class="sc" id="17"></a>
msgctxt "pi-tv-bu-vb-ss1:21.1"
msgid "<strong>Ajjhattabahiddhārūpe</strong>ti tadubhaye."
msgstr ""

#. </p><p><a class="sc" id="18"></a>
msgctxt "pi-tv-bu-vb-ss1:22.1"
msgid "<strong>Ākāse kaṭiṃ kampento</strong>ti ākāse vāyamantassa aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="19"></a>
msgctxt "pi-tv-bu-vb-ss1:23.1"
msgid "<strong>Rāgūpatthambhe</strong>ti rāgena pīḷitassa aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="20"></a>
msgctxt "pi-tv-bu-vb-ss1:24.1"
msgid "<strong>Vaccūpatthambhe</strong>ti vaccena pīḷitassa aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="21"></a>
msgctxt "pi-tv-bu-vb-ss1:25.1"
msgid "<strong>Passāvūpatthambhe</strong>ti passāvena pīḷitassa aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="22"></a>
msgctxt "pi-tv-bu-vb-ss1:26.1"
msgid "<strong>Vātūpatthambhe</strong>ti vātena pīḷitassa aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="23"></a>
msgctxt "pi-tv-bu-vb-ss1:27.1"
msgid "<strong>Uccāliṅgapāṇakadaṭṭhūpatthambhe</strong>ti uccāliṅgapāṇakadaṭṭhena aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="24"></a>
msgctxt "pi-tv-bu-vb-ss1:28.1"
msgid "<strong>Ārogyatthāyā</strong>ti arogo bhavissāmi."
msgstr ""

#. </p><p><a class="sc" id="25"></a>
msgctxt "pi-tv-bu-vb-ss1:29.1"
msgid "<strong>Sukhatthāyā</strong>ti sukhaṃ vedanaṃ uppādessāmi."
msgstr ""

#. </p><p><a class="sc" id="26"></a>
msgctxt "pi-tv-bu-vb-ss1:30.1"
msgid "<strong>Bhesajjatthāyā</strong>ti bhesajjaṃ bhavissati."
msgstr ""

#. </p><p><a class="sc" id="27"></a>
msgctxt "pi-tv-bu-vb-ss1:31.1"
msgid "<strong>Dānatthāyā</strong>ti dānaṃ dassāmi."
msgstr ""

#. </p><p><a class="sc" id="28"></a>
msgctxt "pi-tv-bu-vb-ss1:32.1"
msgid "<strong>Puññatthāyā</strong>ti puññaṃ bhavissati."
msgstr ""

#. </p><p><a class="sc" id="29"></a>
msgctxt "pi-tv-bu-vb-ss1:33.1"
msgid "<strong>Yaññatthāyā</strong>ti yaññaṃ yajissāmi."
msgstr ""

#. </p><p><a class="sc" id="30"></a>
msgctxt "pi-tv-bu-vb-ss1:34.1"
msgid "<strong>Saggatthāyā</strong>ti saggaṃ gamissāmi."
msgstr ""

#. </p><p><a class="sc" id="31"></a>
msgctxt "pi-tv-bu-vb-ss1:35.1"
msgid "<strong>Bījatthāyā</strong>ti bījaṃ bhavissati."
msgstr ""

#. </p><p><a class="sc" id="32"></a>
msgctxt "pi-tv-bu-vb-ss1:36.1"
msgid "<strong>Vīmaṃsatthāyā</strong>ti nīlaṃ bhavissati, pītakaṃ bhavissati, lohitakaṃ bhavissati, odātaṃ bhavissati, takkavaṇṇaṃ bhavissati, dakavaṇṇaṃ bhavissati, telavaṇṇaṃ bhavissati, khīravaṇṇaṃ bhavissati, dadhivaṇṇaṃ bhavissati, sappivaṇṇaṃ bhavissatīti."
msgstr ""

#. </p><p><a class="sc" id="33"></a>
msgctxt "pi-tv-bu-vb-ss1:37.1"
msgid "<strong>Davatthāyā</strong>ti khiḍḍādhippāyo."
msgstr ""

#. </p><p><a class="sc" id="34"></a>
msgctxt "pi-tv-bu-vb-ss1:38.1"
msgid "Ajjhattarūpe ceteti upakkamati muccati, āpatti saṃghādisesassa."
