#. lang="pi"><section class="sutta" id="pi-tv-bu-vb-ss8"><article><div
#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-ss8:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss8:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss8:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-ss8:4.1"
msgid "8. Duṭṭhadosasikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.158"></a>
msgctxt "pi-tv-bu-vb-ss8:5.1"
msgid "Tena samayena buddho bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:5.2"
msgid "Tena kho pana samayena āyasmatā dabbena mallaputtena jātiyā sattavassena arahattaṃ sacchikataṃ hoti."
msgstr ""

#. VAR: Yaṃ kiñci → yañca kiñci (si, mr)
msgctxt "pi-tv-bu-vb-ss8:5.3"
msgid "Yaṃ kiñci sāvakena pattabbaṃ sabbaṃ tena anuppattaṃ hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:5.4"
msgid "Natthi cassa kiñci uttari karaṇīyaṃ, katassa vā paticayo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:5.5"
msgid "Atha kho āyasmato dabbassa mallaputtassa rahogatassa paṭisallīnassa evaṃ cetaso parivitakko udapādi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:5.6"
msgid "mayā kho jātiyā sattavassena arahattaṃ sacchikataṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:5.7"
msgid "Yaṃ kiñci sāvakena pattabbaṃ sabbaṃ mayā anuppattaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:5.8"
msgid "Natthi ca me kiñci uttari karaṇīyaṃ, katassa vā paticayo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:5.9"
msgid "Kiṃ nu kho ahaṃ saṃghassa veyyāvaccaṃ kareyyan”ti?"
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "pi-tv-bu-vb-ss8:6.1"
msgid "Atha kho āyasmato dabbassa mallaputtassa etadahosi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.2"
msgid "yannūnāhaṃ saṃghassa senāsanañca paññapeyyaṃ bhattāni ca uddiseyyan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.3"
msgid "Atha kho āyasmā dabbo mallaputto sāyanhasamayaṃ paṭisallānā vuṭṭhito yena bhagavā tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.4"
msgid "upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.5"
msgid "Ekamantaṃ nisinno kho āyasmā dabbo mallaputto bhagavantaṃ etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.6"
msgid "idha mayhaṃ, bhante, rahogatassa paṭisallīnassa evaṃ cetaso parivitakko udapādi mayā kho jātiyā sattavassena arahattaṃ sacchikataṃ, yaṃ kiñci sāvakena pattabbaṃ, sabbaṃ mayā anupattaṃ, natthi ca me kiñci uttari karaṇīyaṃ, katassa vā paticayo, kiṃ nu kho ahaṃ saṃghassa veyyāvaccaṃ kareyyanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.7"
msgid "Tassa mayhaṃ, bhante, etadahosi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.8"
msgid "‘yannūnāhaṃ saṃghassa senāsanañca paññapeyyaṃ bhattāni ca uddiseyyan’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.9"
msgid "Icchāmahaṃ, bhante, saṃghassa senāsanañca paññapetuṃ bhattāni ca uddisitun”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.10"
msgid "“Sādhu sādhu, dabba."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.11"
msgid "Tena hi tvaṃ, dabba, saṃghassa senāsanañca paññapehi bhattāni ca uddisā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:6.12"
msgid "“Evaṃ, bhante”ti kho āyasmā dabbo mallaputto bhagavato paccassosi."
msgstr ""

#. </p><p><a class="sc" id="3"></a>
msgctxt "pi-tv-bu-vb-ss8:7.1"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe dhammiṃ kathaṃ katvā bhikkhū āmantesi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:7.2"
msgid "tena hi, bhikkhave, saṃgho dabbaṃ mallaputtaṃ senāsanapaññāpakañca bhattuddesakañca sammannatu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:7.3"
msgid "Evañca pana, bhikkhave, sammannitabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:7.4"
msgid "Paṭhamaṃ dabbo mallaputto yācitabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:7.5"
msgid "Yācitvā byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. </p><p><a class="sc" id="4"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.159"></a>
msgctxt "pi-tv-bu-vb-ss8:8.1"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:8.2"
msgid "Yadi saṃghassa pattakallaṃ saṃgho āyasmantaṃ dabbaṃ mallaputtaṃ senāsanapaññāpakañca bhattuddesakañca sammanneyya."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:8.3"
msgid "Esā ñatti."
msgstr ""

#. </p><p><a class="sc" id="5"></a>
msgctxt "pi-tv-bu-vb-ss8:9.1"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:9.2"
msgid "Saṃgho āyasmantaṃ dabbaṃ mallaputtaṃ senāsanapaññāpakañca bhattuddesakañca sammannati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:9.3"
msgid "Yassāyasmato khamati āyasmato dabbassa mallaputtassa senāsanapaññāpakassa ca bhattuddesakassa ca sammuti, so tuṇhassa;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:9.4"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. </p><p><a class="sc" id="6"></a>
msgctxt "pi-tv-bu-vb-ss8:10.1"
msgid "Sammato saṃghena āyasmā dabbo mallaputto senāsanapaññāpako ca bhattuddesako ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:10.2"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. </p><p><a class="sc" id="7"></a>
msgctxt "pi-tv-bu-vb-ss8:11.1"
msgid "Sammato ca panāyasmā dabbo mallaputto sabhāgānaṃ bhikkhūnaṃ ekajjhaṃ senāsanaṃ paññapeti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.2"
msgid "Ye te bhikkhū suttantikā tesaṃ ekajjhaṃ senāsanaṃ paññapeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.3"
msgid "te aññamaññaṃ suttantaṃ saṅgāyissantī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.4"
msgid "Ye te bhikkhū vinayadharā tesaṃ ekajjhaṃ senāsanaṃ paññapeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.5"
msgid "te aññamaññaṃ vinayaṃ vinicchinissantī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.6"
msgid "Ye te bhikkhū dhammakathikā tesaṃ ekajjhaṃ senāsanaṃ paññapeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.7"
msgid "te aññamaññaṃ dhammaṃ sākacchissantī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.8"
msgid "Ye te bhikkhū jhāyino tesaṃ ekajjhaṃ senāsanaṃ paññapeti—"
msgstr ""

#. VAR: na byābādhissantī”ti → na byā­bāhis­santīti (mr)
msgctxt "pi-tv-bu-vb-ss8:11.9"
msgid "“te aññamaññaṃ na byābādhissantī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.10"
msgid "Ye te bhikkhū tiracchānakathikā kāyadaḷhibahulā viharanti tesampi ekajjhaṃ senāsanaṃ paññapeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.11"
msgid "imāyapime āyasmanto ratiyā acchissantī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.12"
msgid "Yepi te bhikkhū vikāle āgacchanti tesampi tejodhātuṃ samāpajjitvā teneva ālokena senāsanaṃ paññapeti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.13"
msgid "Apisu bhikkhū sañcicca vikāle āgacchanti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:11.14"
msgid "mayaṃ āyasmato dabbassa mallaputtassa iddhipāṭihāriyaṃ passissāmā”ti."
msgstr ""

#. </p><p><a class="sc" id="8"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.160"></a>
msgctxt "pi-tv-bu-vb-ss8:12.1"
msgid "Te āyasmantaṃ dabbaṃ mallaputtaṃ upasaṅkamitvā evaṃ vadanti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.2"
msgid "amhākaṃ, āvuso dabba, senāsanaṃ paññapehī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.3"
msgid "Te āyasmā dabbo mallaputto evaṃ vadeti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.4"
msgid "“katthāyasmantā icchanti, kattha paññapemī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.5"
msgid "Te sañcicca dūre apadisanti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.6"
msgid "“amhākaṃ, āvuso dabba, gijjhakūṭe pabbate senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.7"
msgid "Amhākaṃ, āvuso, corapapāte senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.8"
msgid "Amhākaṃ, āvuso, isigilipasse kāḷasilāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.9"
msgid "Amhākaṃ, āvuso, vebhārapasse sattapaṇṇiguhāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.10"
msgid "Amhākaṃ, āvuso, sītavane sappasoṇḍikapabbhāre senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.11"
msgid "Amhākaṃ, āvuso, gotamakakandarāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.12"
msgid "Amhākaṃ, āvuso, tindukakandarāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.13"
msgid "Amhākaṃ, āvuso, tapodakandarāyaṃ senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.14"
msgid "Amhākaṃ, āvuso, tapodārāme senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.15"
msgid "Amhākaṃ, āvuso, jīvakambavane senāsanaṃ paññapehi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:12.16"
msgid "Amhākaṃ, āvuso, maddakucchismiṃ migadāye senāsanaṃ paññapehī”ti."
msgstr ""

#. </p><p><a class="sc" id="9"></a>
msgctxt "pi-tv-bu-vb-ss8:13.1"
msgid "Tesaṃ āyasmā dabbo mallaputto tejodhātuṃ samāpajjitvā aṅguliyā jalamānāya purato purato gacchati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:13.2"
msgid "Tepi teneva ālokena āyasmato dabbassa mallaputtassa piṭṭhito piṭṭhito gacchanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:13.3"
msgid "Tesaṃ āyasmā dabbo mallaputto evaṃ senāsanaṃ paññapeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:13.4"
msgid "ayaṃ mañco, idaṃ pīṭhaṃ, ayaṃ bhisi, idaṃ bibbohanaṃ, idaṃ vaccaṭṭhānaṃ, idaṃ passāvaṭṭhānaṃ, idaṃ pānīyaṃ, idaṃ paribhojanīyaṃ, ayaṃ kattaradaṇḍo, idaṃ saṃghassa katikasaṇṭhānaṃ, imaṃ kālaṃ pavisitabbaṃ, imaṃ kālaṃ nikkhamitabban”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:13.5"
msgid "Tesaṃ āyasmā dabbo mallaputto evaṃ senāsanaṃ paññapetvā punadeva veḷuvanaṃ paccāgacchati."
msgstr ""

#. </p><p><a class="sc" id="10"></a>
msgctxt "pi-tv-bu-vb-ss8:14.1"
msgid "Tena kho pana samayena mettiyabhūmajakā bhikkhū navakā ceva honti appapuññā ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.2"
msgid "Yāni saṃghassa lāmakāni senāsanāni tāni tesaṃ pāpuṇanti lāmakāni ca bhattāni."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.3"
msgid "Tena kho pana samayena rājagahe manussā icchanti therānaṃ bhikkhūnaṃ abhisaṅkhārikaṃ piṇḍapātaṃ dātuṃ sappimpi telampi uttaribhaṅgampi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.4"
msgid "Mettiyabhūmajakānaṃ pana bhikkhūnaṃ pākatikaṃ denti yathārandhaṃ kaṇājakaṃ bilaṅgadutiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.5"
msgid "Te pacchābhattaṃ piṇḍapātapaṭikkantā there bhikkhū pucchanti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.6"
msgid "“tumhākaṃ, āvuso, bhattagge kiṃ ahosi?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.7"
msgid "Tumhākaṃ, āvuso, bhattagge kiṃ ahosī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.8"
msgid "Ekacce therā evaṃ vadanti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.9"
msgid "amhākaṃ, āvuso, sappi ahosi telaṃ ahosi uttaribhaṅgaṃ ahosī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.10"
msgid "Mettiyabhūmajakā pana bhikkhū evaṃ vadanti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:14.11"
msgid "amhākaṃ, āvuso, na kiñci ahosi, pākatikaṃ yathārandhaṃ kaṇājakaṃ bilaṅgadutiyan”ti."
msgstr ""

#. </p><p><a class="sc" id="11"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.161"></a>
msgctxt "pi-tv-bu-vb-ss8:15.1"
msgid "Tena kho pana samayena kalyāṇabhattiko gahapati saṃghassa catukkabhattaṃ deti niccabhattaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.2"
msgid "So bhattagge saputtadāro upatiṭṭhitvā parivisati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.3"
msgid "Aññe odanena pucchanti, aññe sūpena pucchanti, aññe telena pucchanti, aññe uttaribhaṅgena pucchanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.4"
msgid "Tena kho pana samayena kalyāṇabhattikassa gahapatino bhattaṃ svātanāya mettiyabhūmajakānaṃ bhikkhūnaṃ uddiṭṭhaṃ hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.5"
msgid "Atha kho kalyāṇabhattiko gahapati ārāmaṃ agamāsi kenacideva karaṇīyena."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.6"
msgid "So yenāyasmā dabbo mallaputto tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.7"
msgid "upasaṅkamitvā āyasmantaṃ dabbaṃ mallaputtaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.8"
msgid "Ekamantaṃ nisinnaṃ kho kalyāṇabhattikaṃ gahapatiṃ āyasmā dabbo mallaputto dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṃsesi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.9"
msgid "Atha kho kalyāṇabhattiko gahapati āyasmatā dabbena mallaputtena dhammiyā kathāya sandassito samādapito samuttejito sampahaṃsito āyasmantaṃ dabbaṃ mallaputtaṃ etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.10"
msgid "kassa, bhante, amhākaṃ ghare svātanāya bhattaṃ uddiṭṭhan”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.11"
msgid "“Mettiyabhūmajakānaṃ kho, gahapati, bhikkhūnaṃ tumhākaṃ ghare svātanāya bhattaṃ uddiṭṭhan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.12"
msgid "Atha kho kalyāṇabhattiko gahapati anattamano ahosi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.13"
msgid "kathañhi nāma pāpabhikkhū amhākaṃ ghare bhuñjissantī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.14"
msgid "Gharaṃ gantvā dāsiṃ āṇāpesi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.15"
msgid "ye, je, sve bhattikā āgacchanti te koṭṭhake āsanaṃ paññapetvā kaṇājakena bilaṅgadutiyena parivisā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:15.16"
msgid "“Evaṃ, ayyā”ti kho sā dāsī kalyāṇabhattikassa gahapatino paccassosi."
msgstr ""

#. </p><p><a class="sc" id="12"></a>
msgctxt "pi-tv-bu-vb-ss8:16.1"
msgid "Atha kho mettiyabhūmajakā bhikkhū—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:16.2"
msgid "hiyyo kho, āvuso, amhākaṃ kalyāṇabhattikassa gahapatino bhattaṃ uddiṭṭhaṃ, sve amhe kalyāṇabhattiko gahapati saputtadāro upatiṭṭhitvā parivisissati;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:16.3"
msgid "aññe odanena pucchissanti, aññe sūpena pucchissanti, aññe telena pucchissanti, aññe uttaribhaṅgena pucchissantī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:16.4"
msgid "Te teneva somanassena na cittarūpaṃ rattiyā supiṃsu."
msgstr ""

#. </p><p><a class="sc" id="13"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.162"></a>
msgctxt "pi-tv-bu-vb-ss8:17.1"
msgid "Atha kho mettiyabhūmajakā bhikkhū pubbaṇhasamayaṃ nivāsetvā pattacīvaraṃ ādāya yena kalyāṇabhattikassa gahapatino nivesanaṃ tenupasaṅkamiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.2"
msgid "Addasā kho sā dāsī mettiyabhūmajake bhikkhū dūratova āgacchante."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.3"
msgid "Disvāna koṭṭhake āsanaṃ paññapetvā mettiyabhūmajake bhikkhū etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.4"
msgid "nisīdatha, bhante”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.5"
msgid "Atha kho mettiyabhūmajakānaṃ bhikkhūnaṃ etadahosi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.6"
msgid "nissaṃsayaṃ kho na tāva bhattaṃ siddhaṃ bhavissati."
msgstr ""

#. VAR: mayaṃ koṭṭhake nisīdeyyāmā”ti → nisī­dāpi­yey­yāmāti (bj) |
#. nisīdāpiyāmāti (s1-3) | nisī­dāpeyyā­māti (pts1)
#. VAR: Yathā → yāva (bj)
msgctxt "pi-tv-bu-vb-ss8:17.7"
msgid "Yathā mayaṃ koṭṭhake nisīdeyyāmā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.8"
msgid "Atha kho sā dāsī kaṇājakena bilaṅgadutiyena upagacchi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.9"
msgid "bhuñjatha, bhante”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.10"
msgid "“Mayaṃ kho, bhagini, niccabhattikā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.11"
msgid "“Jānāmi, ayyā, niccabhattikāti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.12"
msgid "Api cāhaṃ hiyyova gahapatinā āṇattā—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.13"
msgid "ye, je, sve bhattikā āgacchanti te koṭṭhake āsanaṃ paññapetvā kaṇājakena bilaṅgadutiyena parivisā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.14"
msgid "Bhuñjatha, bhante”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.15"
msgid "Atha kho mettiyabhūmajakā bhikkhū—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.16"
msgid "hiyyo kho, āvuso, kalyāṇabhattiko gahapati ārāmaṃ agamāsi dabbassa mallaputtassa santike."
msgstr ""

#. VAR: antare → santike (bj, s1-3, pts1, mr)
msgctxt "pi-tv-bu-vb-ss8:17.17"
msgid "Nissaṃsayaṃ kho mayaṃ dabbena mallaputtena gahapatino antare paribhinnā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.18"
msgid "Te teneva domanassena na cittarūpaṃ bhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:17.19"
msgid "Atha kho mettiyabhūmajakā bhikkhū pacchābhattaṃ piṇḍapātapaṭikkantā ārāmaṃ gantvā pattacīvaraṃ paṭisāmetvā bahārāmakoṭṭhake saṅghāṭipallatthikāya nisīdiṃsu tuṇhībhūtā maṅkubhūtā pattakkhandhā adhomukhā pajjhāyantā appaṭibhānā."
msgstr ""

#. </p><p><a class="sc" id="14"></a>
msgctxt "pi-tv-bu-vb-ss8:18.1"
msgid "Atha kho mettiyā bhikkhunī yena mettiyabhūmajakā bhikkhū tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.2"
msgid "upasaṅkamitvā mettiyabhūmajake bhikkhū etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.3"
msgid "vandāmi, ayyā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.4"
msgid "Evaṃ vutte, mettiyabhūmajakā bhikkhū nālapiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.5"
msgid "Dutiyampi kho …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.6"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.7"
msgid "tatiyampi kho mettiyā bhikkhunī mettiyabhūmajake bhikkhū etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.8"
msgid "vandāmi, ayyā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.9"
msgid "Tatiyampi kho mettiyabhūmajakā bhikkhū nālapiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.10"
msgid "“Kyāhaṃ ayyānaṃ aparajjhāmi?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.11"
msgid "Kissa maṃ ayyā nālapantī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.12"
msgid "“Tathā hi pana tvaṃ, bhagini, amhe dabbena mallaputtena viheṭhīyamāne ajjhupekkhasī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.13"
msgid "“Kyāhaṃ, ayyā, karomī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.14"
msgid "“Sace kho tvaṃ, bhagini, iccheyyāsi ajjeva bhagavā dabbaṃ mallaputtaṃ nāsāpeyyā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.15"
msgid "“Kyāhaṃ, ayyā, karomi, kiṃ mayā sakkā kātun”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.16"
msgid "“Ehi tvaṃ, bhagini, yena bhagavā tenupasaṅkama;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.17"
msgid "upasaṅkamitvā bhagavantaṃ evaṃ vadehi—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.18"
msgid "idaṃ, bhante, nacchannaṃ nappatirūpaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.19"
msgid "Yāyaṃ, bhante, disā abhayā anītikā anupaddavā sāyaṃ disā sabhayā saītikā saupaddavā."
msgstr ""

#. VAR: savātaṃ → pavātaṃ (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss8:18.20"
msgid "Yato nivātaṃ tato savātaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.21"
msgid "Udakaṃ maññe ādittaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.22"
msgid "Ayyenamhi dabbena mallaputtena dūsitā’”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.23"
msgid "“Evaṃ, ayyā”ti kho mettiyā bhikkhunī mettiyabhūmajakānaṃ bhikkhūnaṃ paṭissutvā yena bhagavā tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.24"
msgid "upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.25"
msgid "Ekamantaṃ ṭhitā kho sā mettiyā bhikkhunī bhagavantaṃ etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.26"
msgid "idaṃ, bhante, nacchannaṃ nappatirūpaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.27"
msgid "Yāyaṃ, bhante, disā abhayā anītikā anupaddavā sāyaṃ disā sabhayā saītikā saupaddavā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.28"
msgid "Yato nivātaṃ tato savātaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.29"
msgid "Udakaṃ maññe ādittaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:18.30"
msgid "Ayyenamhi dabbena mallaputtena dūsitā”ti."
msgstr ""

#. </p><p><a class="sc" id="15"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.163"></a>
msgctxt "pi-tv-bu-vb-ss8:19.1"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ dabbaṃ mallaputtaṃ paṭipucchi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.2"
msgid "sarasi tvaṃ, dabba, evarūpaṃ kattā yathāyaṃ bhikkhunī āhā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.3"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.4"
msgid "Dutiyampi kho bhagavā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.5"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.6"
msgid "tatiyampi kho bhagavā āyasmantaṃ dabbaṃ mallaputtaṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.7"
msgid "“sarasi tvaṃ, dabba, evarūpaṃ kattā yathāyaṃ bhikkhunī āhā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.8"
msgid "“Yathā maṃ, bhante, bhagavā jānātī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.9"
msgid "“Na kho, dabba, dabbā evaṃ nibbeṭhenti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.10"
msgid "Sace tayā kataṃ katanti vadehi, sace tayā akataṃ akatanti vadehī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.11"
msgid "“Yato ahaṃ, bhante, jāto nābhijānāmi supinantenapi methunaṃ dhammaṃ paṭisevitā, pageva jāgaro”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.12"
msgid "Atha kho bhagavā bhikkhū āmantesi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.13"
msgid "tena hi, bhikkhave, mettiyaṃ bhikkhuniṃ nāsetha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.14"
msgid "Ime ca bhikkhū anuyuñjathā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:19.15"
msgid "Idaṃ vatvā bhagavā uṭṭhāyāsanā vihāraṃ pāvisi."
msgstr ""

#. </p><p><a class="sc" id="16"></a>
msgctxt "pi-tv-bu-vb-ss8:20.1"
msgid "Atha kho te bhikkhū mettiyaṃ bhikkhuniṃ nāsesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.2"
msgid "Atha kho mettiyabhūmajakā bhikkhū te bhikkhū etadavocuṃ—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.3"
msgid "māvuso, mettiyaṃ bhikkhuniṃ nāsetha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.4"
msgid "Na sā kiñci aparajjhati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.5"
msgid "Amhehi sā ussāhitā kupitehi anattamanehi cāvanādhippāyehī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.6"
msgid "“Kiṃ pana tumhe, āvuso, āyasmantaṃ dabbaṃ mallaputtaṃ amūlakena pārājikena dhammena anuddhaṃsethā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.7"
msgid "“Evamāvuso”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.8"
msgid "Ye te bhikkhū appicchā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.10"
msgid "te ujjhāyanti khiyyanti vipācenti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:20.11"
msgid "kathañhi nāma mettiyabhūmajakā bhikkhū āyasmantaṃ dabbaṃ mallaputtaṃ amūlakena pārājikena dhammena anuddhaṃsessantī”ti."
msgstr ""

#. </p><p><a class="sc" id="17"></a>
msgctxt "pi-tv-bu-vb-ss8:21.1"
msgid "Atha kho te bhikkhū mettiyabhūmajake bhikkhū anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.3"
msgid "“saccaṃ kira tumhe, bhikkhave, dabbaṃ mallaputtaṃ amūlakena pārājikena dhammena anuddhaṃsethā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.5"
msgid "Vigarahi buddho bhagavā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.6"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.7"
msgid "“kathañhi nāma tumhe, moghapurisā, dabbaṃ mallaputtaṃ amūlakena pārājikena dhammena anuddhaṃsessatha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.8"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:21.10"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="18"></a>
msgctxt "pi-tv-bu-vb-ss8:22.1"
msgid "<strong class=\"rule\">“Yo pana bhikkhu bhikkhuṃ duṭṭho doso appatīto amūlakena pārājikena dhammena anuddhaṃseyya—‘"
msgstr ""

#. VAR: appeva nāma → appevanāma (bj, s1-3)
msgctxt "pi-tv-bu-vb-ss8:22.2"
msgid "appeva nāma naṃ imamhā brahmacariyā cāveyyan’ti, tato aparena samayena samanuggāhīyamāno vā asamanuggāhīyamāno vā amūlakañceva taṃ adhikaraṇaṃ hoti bhikkhu ca dosaṃ patiṭṭhāti, saṃghādiseso”</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:22.3"
msgid "(<em>8</em>:<strong>12</strong>)"
msgstr ""

#. </p><p><a class="sc" id="19"></a>
msgctxt "pi-tv-bu-vb-ss8:23.1"
msgid "<strong>Yo panā</strong>ti yo yādiso …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:23.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:23.3"
msgid "<strong>bhikkhū</strong>ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:23.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:23.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. </p><p><a class="sc" id="20"></a>
msgctxt "pi-tv-bu-vb-ss8:24.1"
msgid "<strong>Bhikkhun</strong>ti aññaṃ bhikkhuṃ."
msgstr ""

#. </p><p><a class="sc" id="21"></a>
msgctxt "pi-tv-bu-vb-ss8:25.1"
msgid "<strong>Duṭṭho doso</strong>ti kupito anattamano anabhiraddho āhatacitto khilajāto."
msgstr ""

#. </p><p><a class="sc" id="22"></a>
msgctxt "pi-tv-bu-vb-ss8:26.1"
msgid "<strong>Appatīto</strong>ti tena ca kopena tena ca dosena tāya ca anattamanatāya tāya ca anabhiraddhiyā appatīto hoti."
msgstr ""

#. </p><p><a class="sc" id="23"></a>
msgctxt "pi-tv-bu-vb-ss8:27.1"
msgid "<strong>Amūlakaṃ</strong> nāma adiṭṭhaṃ asutaṃ aparisaṅkitaṃ."
msgstr ""

#. </p><p><a class="sc" id="24"></a>
msgctxt "pi-tv-bu-vb-ss8:28.1"
msgid "<strong>Pārājikena dhammenā</strong>ti catunnaṃ aññatarena."
msgstr ""

#. </p><p><a class="sc" id="25"></a>
msgctxt "pi-tv-bu-vb-ss8:29.1"
msgid "<strong>Anuddhaṃseyyā</strong>ti codeti vā codāpeti vā."
msgstr ""

#. </p><p><a class="sc" id="26"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.164"></a>
msgctxt "pi-tv-bu-vb-ss8:30.1"
msgid "<strong>Appeva nāma naṃ imamhā brahmacariyā cāveyyan</strong>ti bhikkhubhāvā cāveyyaṃ, samaṇadhammā cāveyyaṃ, sīlakkhandhā cāveyyaṃ, tapoguṇā cāveyyaṃ."
msgstr ""

#. </p><p><a class="sc" id="27"></a>
msgctxt "pi-tv-bu-vb-ss8:31.1"
msgid "<strong>Tato aparena samayenā</strong>ti yasmiṃ khaṇe anuddhaṃsito hoti taṃ khaṇaṃ taṃ layaṃ taṃ muhuttaṃ vītivatte."
msgstr ""

#. </p><p><a class="sc" id="28"></a>
msgctxt "pi-tv-bu-vb-ss8:32.1"
msgid "<strong>Samanuggāhīyamāno</strong>ti yena vatthunā anuddhaṃsito hoti tasmiṃ vatthusmiṃ samanuggāhīyamāno."
msgstr ""

#. </p><p><a class="sc" id="29"></a>
msgctxt "pi-tv-bu-vb-ss8:33.1"
msgid "<strong>Asamanuggāhīyamāno</strong>ti na kenaci vuccamāno."
msgstr ""

#. </p><p><a class="sc" id="30"></a>
msgctxt "pi-tv-bu-vb-ss8:34.1"
msgid "<strong>Adhikaraṇaṃ</strong> nāma cattāri adhikaraṇāni—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:34.2"
msgid "vivādādhikaraṇaṃ, anuvādādhikaraṇaṃ, āpattādhikaraṇaṃ, kiccādhikaraṇaṃ."
msgstr ""

#. </p><p><a class="sc" id="31"></a>
msgctxt "pi-tv-bu-vb-ss8:35.1"
msgid "<strong>Bhikkhu ca dosaṃ patiṭṭhātī</strong>ti tucchakaṃ mayā bhaṇitaṃ, musā mayā bhaṇitaṃ, abhūtaṃ mayā bhaṇitaṃ, ajānantena mayā bhaṇitaṃ."
msgstr ""

#. </p><p><a class="sc" id="32"></a>
msgctxt "pi-tv-bu-vb-ss8:36.1"
msgid "<strong>Saṃghādiseso</strong>ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:36.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:36.3"
msgid "tenapi vuccati saṃghādisesoti."
msgstr ""

#. </p><p><a class="sc" id="33"></a>
msgctxt "pi-tv-bu-vb-ss8:37.1"
msgid "Adiṭṭhassa hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:37.2"
msgid "Tañce codeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:37.3"
msgid "diṭṭho mayā, pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi, asakyaputtiyosi, natthi tayā saddhiṃ uposatho vā pavāraṇā vā saṃghakammaṃ vā”ti, āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="34"></a>
msgctxt "pi-tv-bu-vb-ss8:38.1"
msgid "Asutassa hoti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:38.2"
msgid "pārājikaṃ dhammaṃ ajjhāpanno”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:38.3"
msgid "Tañce codeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:38.4"
msgid "suto mayā, pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi, asakyaputtiyosi, natthi tayā saddhiṃ uposatho vā pavāraṇā vā saṃghakammaṃ vā”ti, āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="35"></a>
msgctxt "pi-tv-bu-vb-ss8:39.1"
msgid "Aparisaṅkitassa hoti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:39.2"
msgid "pārājikaṃ dhammaṃ ajjhāpanno”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:39.3"
msgid "Tañce codeti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:39.4"
msgid "“parisaṅkito mayā, pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi, asakyaputtiyosi, natthi tayā saddhiṃ uposatho vā pavāraṇā vā saṃghakammaṃ vā”ti, āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="36"></a>
msgctxt "pi-tv-bu-vb-ss8:40.1"
msgid "Adiṭṭhassa hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:40.2"
msgid "Tañce codeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:40.3"
msgid "diṭṭho mayā suto ca, pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:40.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:40.5"
msgid "āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="37"></a>
msgctxt "pi-tv-bu-vb-ss8:41.1"
msgid "Adiṭṭhassa hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:41.2"
msgid "Tañce codeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:41.3"
msgid "diṭṭho mayā parisaṅkito ca, pārājikaṃ dhammaṃ ajjhāpannosi, assamaṇosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:41.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss8:41.5"
msgid "āpatti vācāya, vācāya saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="38"></a>
msgctxt "pi-tv-bu-vb-ss8:42.1"
msgid "Adiṭṭhassa hoti pārājikaṃ dhammaṃ ajjhāpajjanto."
