#. lang="pi"><section class="sutta" id="pi-tv-bu-vb-pj4"><article><div
#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-pj4:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj4:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj4:3.1"
msgid "Pārājikakaṇḍa"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-pj4:4.1"
msgid "4. Catutthapārājikasikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.87"></a>
msgctxt "pi-tv-bu-vb-pj4:5.1"
msgid "Tena samayena buddho bhagavā vesāliyaṃ viharati mahāvane kūṭāgārasālāyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:5.2"
msgid "Tena kho pana samayena sambahulā sandiṭṭhā sambhattā bhikkhū vaggumudāya nadiyā tīre vassaṃ upagacchiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:5.3"
msgid "Tena kho pana samayena vajjī dubbhikkhā hoti dvīhitikā setaṭṭhikā salākāvuttā, na sukarā uñchena paggahena yāpetuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:5.4"
msgid "Atha kho tesaṃ bhikkhūnaṃ etadahosi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:5.5"
msgid "etarahi kho vajjī dubbhikkhā dvīhitikā setaṭṭhikā salākāvuttā, na sukarā uñchena paggahena yāpetuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:5.6"
msgid "Kena nu kho mayaṃ upāyena samaggā sammodamānā avivadamānā phāsukaṃ vassaṃ vaseyyāma, na ca piṇḍakena kilameyyāmā”ti?"
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "pi-tv-bu-vb-pj4:6.1"
msgid "Ekacce evamāhaṃsu—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:6.2"
msgid "handa mayaṃ, āvuso, gihīnaṃ kammantaṃ adhiṭṭhema, evaṃ te amhākaṃ dātuṃ maññissanti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:6.3"
msgid "Evaṃ mayaṃ samaggā sammodamānā avivadamānā phāsukaṃ vassaṃ vasissāma, na ca piṇḍakena kilamissāmā”ti."
msgstr ""

#. </p><p><a class="sc" id="3"></a>
msgctxt "pi-tv-bu-vb-pj4:7.1"
msgid "Ekacce evamāhaṃsu—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:7.2"
msgid "alaṃ, āvuso, kiṃ gihīnaṃ kammantaṃ adhiṭṭhitena."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:7.3"
msgid "Handa mayaṃ, āvuso, gihīnaṃ dūteyyaṃ harāma, evaṃ te amhākaṃ dātuṃ maññissanti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:7.4"
msgid "Evaṃ mayaṃ samaggā sammodamānā avivadamānā phāsukaṃ vassaṃ vasissāma, na ca piṇḍakena kilamissāmā”ti."
msgstr ""

#. </p><p><a class="sc" id="4"></a><a class="pts-vp-pi" id="pts-vp-pi3.88"></a>
msgctxt "pi-tv-bu-vb-pj4:8.1"
msgid "Ekacce evamāhaṃsu—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:8.2"
msgid "alaṃ, āvuso, kiṃ gihīnaṃ kammantaṃ adhiṭṭhitena."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:8.3"
msgid "Kiṃ gihīnaṃ dūteyyaṃ haṭena."
msgstr ""

#. VAR: bhāsissāma → bhāsāma (bj)
msgctxt "pi-tv-bu-vb-pj4:8.4"
msgid "Handa mayaṃ, āvuso, gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇaṃ bhāsissāma—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:8.5"
msgid "asuko bhikkhu paṭhamassa jhānassa lābhī, asuko bhikkhu dutiyassa jhānassa lābhī, asuko bhikkhu tatiyassa jhānassa lābhī, asuko bhikkhu catutthassa jhānassa lābhī, asuko bhikkhu sotāpanno, asuko bhikkhu sakadāgāmī, asuko bhikkhu anāgāmī, asuko bhikkhu arahā, asuko bhikkhu tevijjo, asuko bhikkhu chaḷabhiñño’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:8.6"
msgid "Evaṃ te amhākaṃ dātuṃ maññissanti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:8.7"
msgid "Evaṃ mayaṃ samaggā sammodamānā avivadamānā phāsukaṃ vassaṃ vasissāma, na ca piṇḍakena kilamissāmā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:8.8"
msgid "“Esoyeva kho, āvuso, seyyo yo amhākaṃ gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇo bhāsito”ti."
msgstr ""

#. </p><p><a class="sc" id="5"></a>
msgctxt "pi-tv-bu-vb-pj4:9.1"
msgid "Atha kho te bhikkhū gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇaṃ bhāsiṃsu—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:9.2"
msgid "asuko bhikkhu paṭhamassa jhānassa lābhī …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:9.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:9.4"
msgid "asuko bhikkhu chaḷabhiñño”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:9.5"
msgid "Atha kho te manussā—“"
msgstr ""

#. VAR: yesaṃ no → vata no (c1)
msgctxt "pi-tv-bu-vb-pj4:9.6"
msgid "lābhā vata no, suladdhaṃ vata no, yesaṃ no evarūpā bhikkhū vassaṃ upagatā;"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:9.7"
msgid "na vata no ito pubbe evarūpā bhikkhū vassaṃ upagatā yathayime bhikkhū sīlavanto kalyāṇadhammā”ti, te na tādisāni bhojanāni attanā paribhuñjanti mātāpitūnaṃ denti puttadārassa denti dāsakammakaraporisassa denti mittāmaccānaṃ denti ñātisālohitānaṃ denti, yādisāni bhikkhūnaṃ denti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:9.8"
msgid "Te na tādisāni khādanīyāni sāyanīyāni pānāni attanā khādanti sāyanti pivanti mātāpitūnaṃ denti puttadārassa denti dāsakammakaraporisassa denti mittāmaccānaṃ denti ñātisālohitānaṃ denti, yādisāni bhikkhūnaṃ denti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:9.9"
msgid "Atha kho te bhikkhū vaṇṇavā ahesuṃ pīṇindriyā pasannamukhavaṇṇā vippasannachavivaṇṇā."
msgstr ""

#. </p><p><a class="sc" id="6"></a>
msgctxt "pi-tv-bu-vb-pj4:10.1"
msgid "Āciṇṇaṃ kho panetaṃ vassaṃvuṭṭhānaṃ bhikkhūnaṃ bhagavantaṃ dassanāya upasaṅkamituṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:10.2"
msgid "Atha kho te bhikkhū vassaṃvuṭṭhā temāsaccayena senāsanaṃ saṃsāmetvā pattacīvaraṃ ādāya yena vesālī tena pakkamiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:10.3"
msgid "Anupubbena yena vesālī mahāvanaṃ kūṭāgārasālā yena bhagavā tenupasaṅkamiṃsu;"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:10.4"
msgid "upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdiṃsu."
msgstr ""

#. </p><p><a class="sc" id="7"></a><a class="pts-vp-pi" id="pts-vp-pi3.89"></a>
msgctxt "pi-tv-bu-vb-pj4:11.1"
msgid "Tena kho pana samayena disāsu vassaṃvuṭṭhā bhikkhū kisā honti lūkhā dubbaṇṇā uppaṇḍuppaṇḍukajātā dhamanisanthatagattā;"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.2"
msgid "vaggumudātīriyā pana bhikkhū vaṇṇavā honti pīṇindriyā pasannamukhavaṇṇā vippasannachavivaṇṇā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.3"
msgid "Āciṇṇaṃ kho panetaṃ buddhānaṃ bhagavantānaṃ āgantukehi bhikkhūhi saddhiṃ paṭisammodituṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.4"
msgid "Atha kho bhagavā vaggumudātīriye bhikkhū etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.5"
msgid "kacci, bhikkhave, khamanīyaṃ kacci yāpanīyaṃ kacci samaggā sammodamānā avivadamānā phāsukaṃ vassaṃ vasittha, na ca piṇḍakena kilamitthā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.6"
msgid "“Khamanīyaṃ, bhagavā, yāpanīyaṃ, bhagavā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.7"
msgid "Samaggā ca mayaṃ, bhante, sammodamānā avivadamānā phāsukaṃ vassaṃ vasimhā, na ca piṇḍakena kilamimhā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.8"
msgid "Jānantāpi tathāgatā pucchanti, jānantāpi na pucchanti …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.10"
msgid "dvīhākārehi buddhā bhagavanto bhikkhū paṭipucchanti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:11.11"
msgid "dhammaṃ vā desessāma, sāvakānaṃ vā sikkhāpadaṃ paññapessāmā”ti."
msgstr ""

#. </p><p><a class="sc" id="8"></a>
msgctxt "pi-tv-bu-vb-pj4:12.1"
msgid "Atha kho bhagavā vaggumudātīriye bhikkhū etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.2"
msgid "yathā kathaṃ pana tumhe, bhikkhave, samaggā sammodamānā avivadamānā phāsukaṃ vassaṃ vasittha na ca piṇḍakena kilamitthā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.3"
msgid "Atha kho te bhikkhū bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.4"
msgid "“Kacci pana vo, bhikkhave, bhūtan”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.5"
msgid "“Abhūtaṃ, bhagavā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.6"
msgid "Vigarahi buddho bhagavā—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.7"
msgid "ananucchavikaṃ, moghapurisā, ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.8"
msgid "Kathañhi nāma tumhe, moghapurisā, udarassa kāraṇā gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇaṃ bhāsissatha."
msgstr ""

#. VAR: govikantanena → govikattanena (bj, pts1, mr) | govikatthanena (si)
msgctxt "pi-tv-bu-vb-pj4:12.9"
msgid "Varaṃ tumhehi, moghapurisā, tiṇhena govikantanena kucchi parikanto, na tveva udarassa kāraṇā gihīnaṃ aññamaññassa uttarimanussadhammassa vaṇṇo bhāsito."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.10"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.11"
msgid "Tatonidānañhi, moghapurisā, maraṇaṃ vā nigaccheyya maraṇamattaṃ vā dukkhaṃ, na tveva tappaccayā kāyassa bhedā paraṃ maraṇā apāyaṃ duggatiṃ vinipātaṃ nirayaṃ upapajjeyya."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.12"
msgid "Itonidānañca kho, moghapurisā, kāyassa bhedā paraṃ maraṇā apāyaṃ duggatiṃ vinipātaṃ nirayaṃ upapajjeyya."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.13"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.14"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:12.15"
msgid "vigarahitvā dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

#. </p><p><a class="sc" id="9"></a><a class="pts-vp-pi" id="pts-vp-pi3.90"></a>
msgctxt "pi-tv-bu-vb-pj4:13.1"
msgid "“Pañcime, bhikkhave, mahācorā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.3"
msgid "Idha, bhikkhave, ekaccassa mahācorassa evaṃ hoti—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.4"
msgid "kudāssu nāmāhaṃ satena vā sahassena vā parivuto gāmanigamarājadhānīsu āhiṇḍissāmi hananto ghātento chindanto chedāpento pacanto pācento’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.5"
msgid "So aparena samayena satena vā sahassena vā parivuto gāmanigamarājadhānīsu āhiṇḍati hananto ghātento chindanto chedāpento pacanto pācento."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.6"
msgid "Evameva kho, bhikkhave, idhekaccassa pāpabhikkhuno evaṃ hoti—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.7"
msgid "kudāssu nāmāhaṃ satena vā sahassena vā parivuto gāmanigamarājadhānīsu cārikaṃ carissāmi sakkato garukato mānito pūjito apacito gahaṭṭhānañceva pabbajitānañca, lābhī cīvarapiṇḍapātasenāsanagilānappaccayabhesajjaparikkhārānan’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.8"
msgid "So aparena samayena satena vā sahassena vā parivuto gāmanigamarājadhānīsu cārikaṃ carati sakkato garukato mānito pūjito apacito gahaṭṭhānañceva pabbajitānañca, lābhī cīvarapiṇḍapātasenāsanagilānappaccayabhesajjaparikkhārānaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.9"
msgid "Ayaṃ, bhikkhave, paṭhamo mahācoro santo saṃvijjamāno lokasmiṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:13.10"
msgid "(1)"
msgstr ""

#. </p><p><a class="sc" id="10"></a>
msgctxt "pi-tv-bu-vb-pj4:14.1"
msgid "Puna caparaṃ, bhikkhave, idhekacco pāpabhikkhu tathāgatappaveditaṃ dhammavinayaṃ pariyāpuṇitvā attano dahati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:14.2"
msgid "Ayaṃ, bhikkhave, dutiyo mahācoro santo saṃvijjamāno lokasmiṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:14.3"
msgid "(2)"
msgstr ""

#. </p><p><a class="sc" id="11"></a>
msgctxt "pi-tv-bu-vb-pj4:15.1"
msgid "Puna caparaṃ, bhikkhave, idhekacco pāpabhikkhu suddhaṃ brahmacāriṃ parisuddhaṃ brahmacariyaṃ carantaṃ amūlakena abrahmacariyena anuddhaṃseti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:15.2"
msgid "Ayaṃ, bhikkhave, tatiyo mahācoro santo saṃvijjamāno lokasmiṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:15.3"
msgid "(3)"
msgstr ""

#. </p><p><a class="sc" id="12"></a>
msgctxt "pi-tv-bu-vb-pj4:16.1"
msgid "Puna caparaṃ, bhikkhave, idhekacco pāpabhikkhu yāni tāni saṃghassa garubhaṇḍāni garuparikkhārāni, seyyathidaṃ—"
msgstr ""

#. VAR: bibbohanaṃ → bimbohanaṃ (bj, s1-3, pts1)
#. VAR: parasu → pharasu (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-pj4:16.2"
msgid "ārāmo ārāmavatthu vihāro vihāravatthu mañco pīṭhaṃ bhisi bibbohanaṃ lohakumbhī lohabhāṇakaṃ lohavārako lohakaṭāhaṃ vāsi parasu kuṭhārī kudālo nikhādanaṃ valli veḷu muñjaṃ pabbajaṃ tiṇaṃ mattikā dārubhaṇḍaṃ mattikābhaṇḍaṃ, tehi gihiṃ saṅgaṇhāti upalāpeti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:16.3"
msgid "Ayaṃ, bhikkhave, catuttho mahācoro santo saṃvijjamāno lokasmiṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:16.4"
msgid "(4)"
msgstr ""

#. </p><p><a class="sc" id="13"></a>
msgctxt "pi-tv-bu-vb-pj4:17.1"
msgid "Sadevake, bhikkhave, loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya ayaṃ aggo mahācoro yo asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:17.2"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:17.3"
msgid "Theyyāya vo, bhikkhave, raṭṭhapiṇḍo bhutto”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:17.4"
msgid "(5)"
msgstr ""

#. </p><blockquote class="gatha"><p><a class="sc" id="14"></a>
msgctxt "pi-tv-bu-vb-pj4:18.1"
msgid "Aññathā santamattānaṃ,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:18.2"
msgid "aññathā yo pavedaye;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:18.3"
msgid "Nikacca kitavasseva,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:18.4"
msgid "bhuttaṃ theyyena tassa taṃ."
msgstr ""

#. </p><p><a class="sc" id="15"></a>
msgctxt "pi-tv-bu-vb-pj4:19.1"
msgid "Kāsāvakaṇṭhā bahavo,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:19.2"
msgid "pāpadhammā asaññatā;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:19.3"
msgid "Pāpā pāpehi kammehi,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:19.4"
msgid "nirayaṃ te upapajjare."
msgstr ""

#. </p><p><a class="sc" id="16"></a>
msgctxt "pi-tv-bu-vb-pj4:20.1"
msgid "Seyyo ayoguḷo bhutto,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:20.2"
msgid "tatto aggisikhūpamo;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:20.3"
msgid "Yañce bhuñjeyya dussīlo,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj4:20.4"
msgid "raṭṭhapiṇḍaṃ asaññatoti."
msgstr ""

#. </p></blockquote><p><a class="sc" id="17"></a>
#. VAR: bhagavā → bhagavā te (c1)
msgctxt "pi-tv-bu-vb-pj4:21.1"
msgid "Atha kho bhagavā vaggumudātīriye bhikkhū anekapariyāyena vigarahitvā dubbharatāya dupposatāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:21.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:21.3"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="18"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.91"></a>
msgctxt "pi-tv-bu-vb-pj4:22.1"
msgid "<strong class=\"rule\">“Yo pana bhikkhu anabhijānaṃ uttarimanussadhammaṃ attupanāyikaṃ alamariyañāṇadassanaṃ samudācareyya—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:22.2"
msgid "iti jānāmi iti passāmī’ti, tato aparena samayena samanuggāhīyamāno vā asamanuggāhīyamāno vā āpanno visuddhāpekkho evaṃ vadeyya—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:22.3"
msgid "ajānamevaṃ, āvuso, avacaṃ jānāmi</strong>, <strong>apassaṃ passāmi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:22.4"
msgid "Tucchaṃ musā vilapin’ti, ayampi pārājiko hoti asaṃvāso”</strong>ti."
msgstr ""

#. </p><p><a class="sc" id="19"></a>
msgctxt "pi-tv-bu-vb-pj4:23.1"
msgid "Evañcidaṃ bhagavatā bhikkhūnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. </p><p><a class="sc" id="20"></a>
msgctxt "pi-tv-bu-vb-pj4:24.1"
msgid "Tena kho pana samayena sambahulā bhikkhū adiṭṭhe diṭṭhasaññino apatte pattasaññino anadhigate adhigatasaññino asacchikate sacchikatasaññino adhimānena aññaṃ byākariṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.2"
msgid "Tesaṃ aparena samayena rāgāyapi cittaṃ namati dosāyapi cittaṃ namati mohāyapi cittaṃ namati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.3"
msgid "Tesaṃ kukkuccaṃ ahosi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.4"
msgid "“bhagavatā sikkhāpadaṃ paññattaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.5"
msgid "Mayañcamha adiṭṭhe diṭṭhasaññino apatte pattasaññino anadhigate adhigatasaññino asacchikate sacchikatasaññino, adhimānena aññaṃ byākarimhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.6"
msgid "Kacci nu kho mayaṃ pārājikaṃ āpattiṃ āpannā”ti?"
msgstr ""

#. VAR: ānanda → honti te ānanda (bj) | honti yevānanda (s1) | hontiyevānanda (s2, s3)
msgctxt "pi-tv-bu-vb-pj4:24.7"
msgid "Te āyasmato ānandassa etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.8"
msgid "Āyasmā ānando bhagavato etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.9"
msgid "“Honti ye te, ānanda, bhikkhū adiṭṭhe diṭṭhasaññino apatte pattasaññino anadhigate adhigatasaññino asacchikate sacchikatasaññino adhimānena aññaṃ byākaronti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:24.10"
msgid "Tañca kho etaṃ abbohārikan”ti."
msgstr ""

#. </p><p><a class="sc" id="21"></a>
msgctxt "pi-tv-bu-vb-pj4:25.1"
msgid "“Evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="22"></a>
#. VAR: attupanāyikaṃ → attūpanāyikaṃ (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-pj4:26.1"
msgid "<strong class=\"rule\">“Yo pana bhikkhu anabhijānaṃ uttarimanussadhammaṃ attupanāyikaṃ alamariyañāṇadassanaṃ samudācareyya—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:26.2"
msgid "iti jānāmi iti passāmī’ti, tato aparena samayena samanuggāhīyamāno vā asamanuggāhīyamāno vā āpanno visuddhāpekkho evaṃ vadeyya—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:26.3"
msgid "ajānamevaṃ, āvuso, avacaṃ jānāmi, apassaṃ passāmi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:26.4"
msgid "Tucchaṃ musā vilapin’ti, aññatra adhimānā, ayampi pārājiko hoti asaṃvāso”</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:26.5"
msgid "(<em>4</em>:<strong>4</strong>)"
msgstr ""

#. </p><p><a class="sc" id="23"></a>
msgctxt "pi-tv-bu-vb-pj4:27.1"
msgid "<strong>Yo panā</strong>ti yo yādiso …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:27.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:27.3"
msgid "<strong>bhikkhū</strong>ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:27.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:27.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. </p><p><a class="sc" id="24"></a>
msgctxt "pi-tv-bu-vb-pj4:28.1"
msgid "<strong>Anabhijānan</strong>ti asantaṃ abhūtaṃ asaṃvijjamānaṃ ajānanto apassanto attani kusalaṃ dhammaṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:28.2"
msgid "atthi me kusalo dhammoti."
msgstr ""

#. </p><p><a class="sc" id="25"></a>
#. VAR: vimokkho → vimokkhaṃ (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-pj4:29.1"
msgid "<strong>Uttarimanussadhammo</strong> nāma jhānaṃ vimokkho samādhi samāpatti ñāṇadassanaṃ maggabhāvanā phalasacchikiriyā kilesappahānaṃ vinīvaraṇatā cittassa suññāgāre abhirati."
msgstr ""

#. </p><p><a class="sc" id="26"></a>
msgctxt "pi-tv-bu-vb-pj4:30.1"
msgid "<strong>Attupanāyikan</strong>ti te vā kusale dhamme attani upaneti attānaṃ vā tesu kusalesu dhammesu upaneti."
msgstr ""

#. </p><p><a class="sc" id="27"></a>
msgctxt "pi-tv-bu-vb-pj4:31.1"
msgid "<strong>Ñāṇan</strong>ti tisso vijjā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:31.2"
msgid "<strong>Dassanan</strong>ti yaṃ ñāṇaṃ taṃ dassanaṃ, yaṃ dassanaṃ taṃ ñāṇaṃ."
msgstr ""

#. </p><p><a class="sc" id="28"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.92"></a>
msgctxt "pi-tv-bu-vb-pj4:32.1"
msgid "<strong>Samudācareyyā</strong>ti āroceyya itthiyā vā purisassa vā gahaṭṭhassa vā pabbajitassa vā."
msgstr ""

#. </p><p><a class="sc" id="29"></a>
msgctxt "pi-tv-bu-vb-pj4:33.1"
msgid "<strong>Iti jānāmi iti passāmī</strong>ti jānāmahaṃ ete dhamme, passāmahaṃ ete dhamme atthi ca ete dhammā mayi, ahañca etesu dhammesu sandissāmīti."
msgstr ""

#. </p><p><a class="sc" id="30"></a>
msgctxt "pi-tv-bu-vb-pj4:34.1"
msgid "<strong>Tato aparena samayenā</strong>ti yasmiṃ khaṇe samudāciṇṇaṃ hoti taṃ khaṇaṃ taṃ layaṃ taṃ muhuttaṃ vītivatte."
msgstr ""

#. </p><p><a class="sc" id="31"></a>
msgctxt "pi-tv-bu-vb-pj4:35.1"
msgid "<strong>Samanuggāhīyamāno</strong>ti yaṃ vatthu paṭiññātaṃ hoti tasmiṃ vatthusmiṃ samanuggāhīyamāno—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:35.2"
msgid "kinte adhigataṃ, kinti te adhigataṃ, kadā te adhigataṃ, kattha te adhigataṃ, katame te kilesā pahīnā, katamesaṃ tvaṃ dhammānaṃ lābhī”ti."
msgstr ""

#. </p><p><a class="sc" id="32"></a>
msgctxt "pi-tv-bu-vb-pj4:36.1"
msgid "<strong>Asamanuggāhīyamāno</strong>ti na kenaci vuccamāno."
msgstr ""

#. </p><p><a class="sc" id="33"></a>
msgctxt "pi-tv-bu-vb-pj4:37.1"
msgid "<strong>Āpanno</strong>ti pāpiccho icchāpakato asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapitvā pārājikaṃ āpattiṃ āpanno hoti."
msgstr ""

#. </p><p><a class="sc" id="34"></a>
msgctxt "pi-tv-bu-vb-pj4:38.1"
msgid "<strong>Visuddhāpekkho</strong>ti gihī vā hotukāmo upāsako vā hotukāmo ārāmiko vā hotukāmo sāmaṇero vā hotukāmo."
msgstr ""

#. </p><p><a class="sc" id="35"></a>
msgctxt "pi-tv-bu-vb-pj4:39.1"
msgid "<strong>Ajānamevaṃ, āvuso, avacaṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:39.2"
msgid "jānāmi, apassaṃ passāmī</strong>ti nāhaṃ ete dhamme jānāmi, nāhaṃ ete dhamme passāmi, natthi ca ete dhammā mayi, na cāhaṃ etesu dhammesu sandissāmīti."
msgstr ""

#. </p><p><a class="sc" id="36"></a>
msgctxt "pi-tv-bu-vb-pj4:40.1"
msgid "<strong>Tucchaṃ musā vilapin</strong>ti tucchakaṃ mayā bhaṇitaṃ, musā mayā bhaṇitaṃ, abhūtaṃ mayā bhaṇitaṃ, ajānantena mayā bhaṇitaṃ."
msgstr ""

#. </p><p><a class="sc" id="37"></a>
msgctxt "pi-tv-bu-vb-pj4:41.1"
msgid "<strong>Aññatra adhimānā</strong>ti ṭhapetvā adhimānaṃ."
msgstr ""

#. </p><p><a class="sc" id="38"></a>
msgctxt "pi-tv-bu-vb-pj4:42.1"
msgid "<strong>Ayampī</strong>ti purime upādāya vuccati."
msgstr ""

#. </p><p><a class="sc" id="39"></a>
msgctxt "pi-tv-bu-vb-pj4:43.1"
msgid "<strong>Pārājiko hotī</strong>ti seyyathāpi nāma tālo matthakacchinno abhabbo puna virūḷhiyā;"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:43.2"
msgid "evameva bhikkhu pāpiccho icchāpakato asantaṃ abhūtaṃ uttarimanussadhammaṃ ullapitvā assamaṇo hoti asakyaputtiyo."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:43.3"
msgid "Tena vuccati—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:43.4"
msgid "pārājiko hotī”ti."
msgstr ""

#. </p><p><a class="sc" id="40"></a>
msgctxt "pi-tv-bu-vb-pj4:44.1"
msgid "<strong>Asaṃvāso</strong>ti saṃvāso nāma ekakammaṃ ekuddeso samasikkhatā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:44.2"
msgid "eso saṃvāso nāma."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:44.3"
msgid "So tena saddhiṃ natthi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:44.4"
msgid "Tena vuccati—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:44.5"
msgid "asaṃvāso”ti."
msgstr ""

#. </p><p><a class="sc" id="41"></a>
msgctxt "pi-tv-bu-vb-pj4:45.1"
msgid "<strong>Uttarimanussadhammo</strong> nāma jhānaṃ vimokkho samādhi samāpatti ñāṇadassanaṃ maggabhāvanā phalasacchikiriyā kilesappahānaṃ vinīvaraṇatā cittassa suññāgāre abhirati."
msgstr ""

#. </p><p><a class="sc" id="42"></a>
msgctxt "pi-tv-bu-vb-pj4:46.1"
msgid "<strong>Jhānan</strong>ti paṭhamaṃ jhānaṃ dutiyaṃ jhānaṃ tatiyaṃ jhānaṃ catutthaṃ jhānaṃ."
msgstr ""

#. </p><p><a class="sc" id="43"></a>
msgctxt "pi-tv-bu-vb-pj4:47.1"
msgid "<strong>Vimokkho</strong>ti suññato vimokkho animitto vimokkho appaṇihito vimokkho."
msgstr ""

#. </p><p><a class="sc" id="44"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.93"></a>
msgctxt "pi-tv-bu-vb-pj4:48.1"
msgid "<strong>Samādhī</strong>ti suññato samādhi animitto samādhi appaṇihito samādhi."
msgstr ""

#. </p><p><a class="sc" id="45"></a>
msgctxt "pi-tv-bu-vb-pj4:49.1"
msgid "<strong>Samāpattī</strong>ti suññatā samāpatti animittā samāpatti appaṇihitā samāpatti."
msgstr ""

#. </p><p><a class="sc" id="46"></a>
msgctxt "pi-tv-bu-vb-pj4:50.1"
msgid "<strong>Ñāṇadassanan</strong>ti tisso vijjā."
msgstr ""

#. </p><p><a class="sc" id="47"></a>
msgctxt "pi-tv-bu-vb-pj4:51.1"
msgid "<strong>Maggabhāvanā</strong>ti cattāro satipaṭṭhānā, cattāro sammappadhānā, cattāro iddhipādā, pañcindriyāni, pañca balāni, satta bojjhaṅgā, ariyo aṭṭhaṅgiko maggo."
msgstr ""

#. </p><p><a class="sc" id="48"></a>
#. VAR: arahattassa → arahat­ta­phalassa (s1-3, pts1)
msgctxt "pi-tv-bu-vb-pj4:52.1"
msgid "<strong>Phalasacchikiriyā</strong>ti sotāpattiphalassa sacchikiriyā, sakadāgāmiphalassa sacchikiriyā, anāgāmiphalassa sacchikiriyā, arahattassa sacchikiriyā."
msgstr ""

#. </p><p><a class="sc" id="49"></a>
msgctxt "pi-tv-bu-vb-pj4:53.1"
msgid "<strong>Kilesappahānan</strong>ti rāgassa pahānaṃ dosassa pahānaṃ mohassa pahānaṃ."
msgstr ""

#. </p><p><a class="sc" id="50"></a>
msgctxt "pi-tv-bu-vb-pj4:54.1"
msgid "<strong>Vinīvaraṇatā cittassā</strong>ti rāgā cittaṃ vinīvaraṇatā, dosā cittaṃ vinīvaraṇatā, mohā cittaṃ vinīvaraṇatā."
msgstr ""

#. </p><p><a class="sc" id="51"></a>
msgctxt "pi-tv-bu-vb-pj4:55.1"
msgid "<strong>Suññāgāre abhiratī</strong>ti paṭhamena jhānena suññāgāre abhirati, dutiyena jhānena suññāgāre abhirati, tatiyena jhānena suññāgāre abhirati, catutthena jhānena suññāgāre abhirati."
msgstr ""

#. </p><p><a class="sc" id="52"></a>
msgctxt "pi-tv-bu-vb-pj4:56.1"
msgid "Tīhākārehi paṭhamaṃ jhānaṃ samāpajjinti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:56.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti."
msgstr ""

#. </p><p><a class="sc" id="53"></a>
msgctxt "pi-tv-bu-vb-pj4:57.1"
msgid "Catūhākārehi paṭhamaṃ jhānaṃ samāpajjinti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:57.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ."
msgstr ""

#. </p><p><a class="sc" id="54"></a>
msgctxt "pi-tv-bu-vb-pj4:58.1"
msgid "Pañcahākārehi paṭhamaṃ jhānaṃ samāpajjinti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:58.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ vinidhāya khantiṃ."
msgstr ""

#. </p><p><a class="sc" id="55"></a>
msgctxt "pi-tv-bu-vb-pj4:59.1"
msgid "Chahākārehi paṭhamaṃ jhānaṃ samāpajjinti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:59.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ, vinidhāya khantiṃ, vinidhāya ruciṃ."
msgstr ""

#. </p><p><a class="sc" id="56"></a>
msgctxt "pi-tv-bu-vb-pj4:60.1"
msgid "Sattahākārehi paṭhamaṃ jhānaṃ samāpajjinti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:60.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ, vinidhāya khantiṃ, vinidhāya ruciṃ, vinidhāya bhāvaṃ."
msgstr ""

#. </p><p><a class="sc" id="57"></a>
msgctxt "pi-tv-bu-vb-pj4:61.1"
msgid "Tīhākārehi paṭhamaṃ jhānaṃ samāpajjāmīti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:61.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti."
msgstr ""

#. </p><p><a class="sc" id="58"></a>
msgctxt "pi-tv-bu-vb-pj4:62.1"
msgid "Catūhākārehi paṭhamaṃ jhānaṃ samāpajjāmīti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:62.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ."
msgstr ""

#. </p><p><a class="sc" id="59"></a>
msgctxt "pi-tv-bu-vb-pj4:63.1"
msgid "Pañcahākārehi paṭhamaṃ jhānaṃ samāpajjāmīti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:63.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ, vinidhāya khantiṃ."
msgstr ""

#. </p><p><a class="sc" id="60"></a>
msgctxt "pi-tv-bu-vb-pj4:64.1"
msgid "Chahākārehi paṭhamaṃ jhānaṃ samāpajjāmīti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:64.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ, vinidhāya khantiṃ, vinidhāya ruciṃ."
msgstr ""

#. </p><p><a class="sc" id="61"></a>
msgctxt "pi-tv-bu-vb-pj4:65.1"
msgid "Sattahākārehi paṭhamaṃ jhānaṃ samāpajjāmīti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:65.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ, vinidhāya khantiṃ, vinidhāya ruciṃ, vinidhāya bhāvaṃ."
msgstr ""

#. </p><p><a class="sc" id="62"></a>
msgctxt "pi-tv-bu-vb-pj4:66.1"
msgid "Tīhākārehi paṭhamaṃ jhānaṃ samāpannoti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:66.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti."
msgstr ""

#. </p><p><a class="sc" id="63"></a>
msgctxt "pi-tv-bu-vb-pj4:67.1"
msgid "Catūhākārehi paṭhamaṃ jhānaṃ samāpannoti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:67.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ."
msgstr ""

#. </p><p><a class="sc" id="64"></a>
msgctxt "pi-tv-bu-vb-pj4:68.1"
msgid "Pañcahākārehi paṭhamaṃ jhānaṃ samāpannoti sampajānamusā bhaṇantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj4:68.2"
msgid "pubbevassa hoti musā bhaṇissanti, bhaṇantassa hoti musā bhaṇāmīti, bhaṇitassa hoti musā mayā bhaṇitanti, vinidhāya diṭṭhiṃ, vinidhāya khantiṃ."
msgstr ""

