msgctxt "pi-tv-bu-vb-pj2:144.10"
msgid "pure sāmikā passantī”ti, theyyacittā paribhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:144.11"
msgid "Sāmikā te bhikkhū codesuṃ—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:144.12"
msgid "assamaṇāttha tumhe”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:144.13"
msgid "Tesaṃ kukkuccaṃ ahosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:144.14"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:144.15"
msgid "“āpattiṃ tumhe, bhikkhave, āpannā pārājikan”ti."
