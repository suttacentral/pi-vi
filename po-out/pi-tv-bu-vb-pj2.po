msgstr ""

msgctxt "pi-tv-bu-vb-pj2:57.5"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:57.6"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="54"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.51"></a>
msgctxt "pi-tv-bu-vb-pj2:58.1"
msgid "<strong>Araññaṃ</strong> nāma yaṃ manussānaṃ pariggahitaṃ hoti, taṃ araññaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.2"
msgid "<strong>Araññaṭṭhaṃ</strong> nāma bhaṇḍaṃ araññe catūhi ṭhānehi nikkhittaṃ hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.3"
msgid "bhūmaṭṭhaṃ, thalaṭṭhaṃ, ākāsaṭṭhaṃ, vehāsaṭṭhaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.4"
msgid "Araññaṭṭhaṃ bhaṇḍaṃ avaharissāmīti theyyacitto dutiyaṃ vā pariyesati gacchati vā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.5"
msgid "Āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.6"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.7"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.8"
msgid "Tattha jātakaṃ kaṭṭhaṃ vā lataṃ vā tiṇaṃ vā pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.9"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:58.10"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="55"></a>
msgctxt "pi-tv-bu-vb-pj2:59.1"
msgid "<strong>Udakaṃ</strong> nāma bhājanagataṃ vā hoti pokkharaṇiyā vā taḷāke vā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.2"
msgid "Theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.3"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.4"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.5"
msgid "Attano bhājanaṃ pavesetvā pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ udakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.6"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.7"
msgid "Attano bhājanagataṃ karoti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.8"
msgid "Mariyādaṃ bhindati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.9"
msgid "Mariyādaṃ bhinditvā pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ udakaṃ nikkhāmeti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.10"
msgid "Atirekamāsakaṃ vā ūnapañcamāsakaṃ vā agghanakaṃ udakaṃ nikkhāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:59.11"
msgid "Māsakaṃ vā ūnamāsakaṃ vā agghanakaṃ udakaṃ nikkhāmeti, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="56"></a>
msgctxt "pi-tv-bu-vb-pj2:60.1"
msgid "<strong>Dantapoṇaṃ</strong> nāma chinnaṃ vā acchinnaṃ vā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:60.2"
msgid "Pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:60.3"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:60.4"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="57"></a>
msgctxt "pi-tv-bu-vb-pj2:61.1"
msgid "<strong>Vanappati</strong> nāma yo manussānaṃ pariggahito hoti rukkho paribhogo."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:61.2"
msgid "Theyyacitto chindati, pahāre pahāre āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:61.3"
msgid "Ekaṃ pahāraṃ anāgate, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:61.4"
msgid "Tasmiṃ pahāre āgate, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="58"></a>
msgctxt "pi-tv-bu-vb-pj2:62.1"
msgid "<strong>Haraṇakaṃ</strong> nāma aññassa haraṇakaṃ bhaṇḍaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.2"
msgid "Theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.3"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.4"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.5"
msgid "Sahabhaṇḍahārakaṃ padasā nessāmīti paṭhamaṃ pādaṃ saṅkāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.6"
msgid "Dutiyaṃ pādaṃ saṅkāmeti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.7"
msgid "Patitaṃ bhaṇḍaṃ gahessāmīti pātāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.8"
msgid "Patitaṃ bhaṇḍaṃ pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.9"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:62.10"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="59"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.52"></a>
msgctxt "pi-tv-bu-vb-pj2:63.1"
msgid "<strong>Upanidhi</strong> nāma upanikkhittaṃ bhaṇḍaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:63.2"
msgid "Dehi me bhaṇḍanti vuccamāno nāhaṃ gaṇhāmīti bhaṇati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:63.3"
msgid "Sāmikassa vimatiṃ uppādeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:63.4"
msgid "Sāmiko na mayhaṃ dassatīti dhuraṃ nikkhipati, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:63.5"
msgid "Dhammaṃ caranto sāmikaṃ parājeti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:63.6"
msgid "Dhammaṃ caranto parajjati, āpatti thullaccayassa."
msgstr ""

#. </p><p><a class="sc" id="60"></a>
msgctxt "pi-tv-bu-vb-pj2:64.1"
msgid "<strong>Suṅkaghātaṃ</strong> nāma raññā ṭhapitaṃ hoti pabbatakhaṇḍe vā nadītitthe vā gāmadvāre vā—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:64.2"
msgid "atra paviṭṭhassa suṅkaṃ gaṇhantū”ti."
msgstr ""

#. VAR: rājaggaṃ → rājagghaṃ (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-pj2:64.3"
msgid "Tatra pavisitvā rājaggaṃ bhaṇḍaṃ pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:64.4"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:64.5"
msgid "Paṭhamaṃ pādaṃ suṅkaghātaṃ atikkāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:64.6"
msgid "Dutiyaṃ pādaṃ atikkāmeti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:64.7"
msgid "Antosuṅkaghāte ṭhito bahisuṅkaghātaṃ pāteti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:64.8"
msgid "Suṅkaṃ pariharati, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="61"></a>
msgctxt "pi-tv-bu-vb-pj2:65.1"
msgid "<strong>Pāṇo</strong> nāma manussapāṇo vuccati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:65.2"
msgid "Theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:65.3"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:65.4"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:65.5"
msgid "Padasā nessāmīti paṭhamaṃ pādaṃ saṅkāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:65.6"
msgid "Dutiyaṃ pādaṃ saṅkāmeti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="62"></a>
msgctxt "pi-tv-bu-vb-pj2:66.1"
msgid "<strong>Apadaṃ</strong> nāma ahi macchā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:66.2"
msgid "Pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:66.3"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:66.4"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="63"></a>
msgctxt "pi-tv-bu-vb-pj2:67.1"
msgid "<strong>Dvipadaṃ</strong> nāma manussā, pakkhajātā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:67.2"
msgid "Theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:67.3"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:67.4"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:67.5"
msgid "Padasā nessāmīti paṭhamaṃ pādaṃ saṅkāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:67.6"
msgid "Dutiyaṃ pādaṃ saṅkāmeti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="64"></a>
msgctxt "pi-tv-bu-vb-pj2:68.1"
msgid "<strong>Catuppadaṃ</strong> nāma—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.2"
msgid "hatthī assā oṭṭhā goṇā gadrabhā pasukā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.3"
msgid "Theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.4"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.5"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.6"
msgid "Padasā nessāmīti paṭhamaṃ pādaṃ saṅkāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.7"
msgid "Dutiyaṃ pādaṃ saṅkāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.8"
msgid "Tatiyaṃ pādaṃ saṅkāmeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:68.9"
msgid "Catutthaṃ pādaṃ saṅkāmeti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="65"></a>
msgctxt "pi-tv-bu-vb-pj2:69.1"
msgid "<strong>Bahuppadaṃ</strong> nāma—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:69.2"
msgid "vicchikā satapadī uccāliṅgapāṇakā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:69.3"
msgid "Pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:69.4"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:69.5"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:69.6"
msgid "Padasā nessāmīti saṅkāmeti, pade pade āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:69.7"
msgid "Pacchimaṃ pādaṃ saṅkāmeti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="66"></a>
msgctxt "pi-tv-bu-vb-pj2:70.1"
msgid "<strong>Ocarako</strong> nāma bhaṇḍaṃ ocaritvā ācikkhati—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:70.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:70.3"
msgid "So taṃ bhaṇḍaṃ avaharati, āpatti ubhinnaṃ pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="67"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.53"></a>
msgctxt "pi-tv-bu-vb-pj2:71.1"
msgid "<strong>Oṇirakkho</strong> nāma āhaṭaṃ bhaṇḍaṃ gopento pañcamāsakaṃ vā atirekapañcamāsakaṃ vā agghanakaṃ theyyacitto āmasati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:71.2"
msgid "Phandāpeti, āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:71.3"
msgid "Ṭhānā cāveti, āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="68"></a>
msgctxt "pi-tv-bu-vb-pj2:72.1"
msgid "<strong>Saṃvidāvahāro</strong> nāma sambahulā saṃvidahitvā eko bhaṇḍaṃ avaharati, āpatti sabbesaṃ pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="69"></a>
msgctxt "pi-tv-bu-vb-pj2:73.1"
msgid "<strong>Saṅketakammaṃ</strong> nāma saṅketaṃ karoti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:73.2"
msgid "purebhattaṃ vā pacchābhattaṃ vā rattiṃ vā divā vā tena saṅketena taṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:73.3"
msgid "Tena saṅketena taṃ bhaṇḍaṃ avaharati, āpatti ubhinnaṃ pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:73.4"
msgid "Taṃ saṅketaṃ pure vā pacchā vā taṃ bhaṇḍaṃ avaharati, mūlaṭṭhassa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:73.5"
msgid "Avahārakassa āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="70"></a>
msgctxt "pi-tv-bu-vb-pj2:74.1"
msgid "<strong>Nimittakammaṃ</strong> nāma nimittaṃ karoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:74.2"
msgid "Akkhiṃ vā nikhaṇissāmi bhamukaṃ vā ukkhipissāmi sīsaṃ vā ukkhipissāmi, tena nimittena taṃ bhaṇḍaṃ avaharāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:74.3"
msgid "Tena nimittena taṃ bhaṇḍaṃ avaharati, āpatti ubhinnaṃ pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:74.4"
msgid "Taṃ nimittaṃ pure vā pacchā vā taṃ bhaṇḍaṃ avaharati, mūlaṭṭhassa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:74.5"
msgid "Avahārakassa āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="71"></a>
msgctxt "pi-tv-bu-vb-pj2:75.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:75.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:75.3"
msgid "So taṃ maññamāno taṃ avaharati, āpatti ubhinnaṃ pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="72"></a>
msgctxt "pi-tv-bu-vb-pj2:76.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:76.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:76.3"
msgid "So taṃ maññamāno aññaṃ avaharati, mūlaṭṭhassa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:76.4"
msgid "Avahārakassa āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="73"></a>
msgctxt "pi-tv-bu-vb-pj2:77.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:77.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:77.3"
msgid "So aññaṃ maññamāno taṃ avaharati, āpatti ubhinnaṃ pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="74"></a>
msgctxt "pi-tv-bu-vb-pj2:78.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:78.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:78.3"
msgid "So aññaṃ maññamāno aññaṃ avaharati, mūlaṭṭhassa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:78.4"
msgid "Avahārakassa āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="75"></a>
msgctxt "pi-tv-bu-vb-pj2:79.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:79.2"
msgid "itthannāmassa pāvada—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:79.3"
msgid "itthannāmo itthannāmassa pāvadatu—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:79.4"
msgid "itthannāmo itthannāmaṃ bhaṇḍaṃ avaharatū’”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:79.5"
msgid "So itarassa āroceti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:79.6"
msgid "Avahārako paṭiggaṇhāti, mūlaṭṭhassa āpatti thullaccayassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:79.7"
msgid "So taṃ bhaṇḍaṃ avaharati, āpatti sabbesaṃ pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="76"></a>
msgctxt "pi-tv-bu-vb-pj2:80.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:80.2"
msgid "itthannāmassa pāvada—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:80.3"
msgid "itthannāmo itthannāmassa pāvadatu—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:80.4"
msgid "itthannāmo itthannāmaṃ bhaṇḍaṃ avaharatū’”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:80.5"
msgid "So aññaṃ āṇāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:80.6"
msgid "Avahārako paṭiggaṇhāti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:80.7"
msgid "So taṃ bhaṇḍaṃ avaharati, mūlaṭṭhassa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:80.8"
msgid "Āṇāpakassa ca avahārakassa ca āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="77"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.54"></a>
msgctxt "pi-tv-bu-vb-pj2:81.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:81.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:81.3"
msgid "So gantvā puna paccāgacchati—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:81.4"
msgid "nāhaṃ sakkomi taṃ bhaṇḍaṃ avaharitun”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:81.5"
msgid "So puna āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:81.6"
msgid "yadā sakkosi tadā taṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:81.7"
msgid "So taṃ bhaṇḍaṃ avaharati, āpatti ubhinnaṃ pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="78"></a>
msgctxt "pi-tv-bu-vb-pj2:82.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:82.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:82.3"
msgid "So āṇāpetvā vippaṭisārī na sāveti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:82.4"
msgid "mā avaharī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:82.5"
msgid "So taṃ bhaṇḍaṃ avaharati, āpatti ubhinnaṃ pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="79"></a>
msgctxt "pi-tv-bu-vb-pj2:83.1"
msgid "Bhikkhu bhikkhuṃ āṇāpeti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:83.2"
msgid "itthannāmaṃ bhaṇḍaṃ avaharā”ti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:83.3"
msgid "So āṇāpetvā vippaṭisārī sāveti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:83.4"
msgid "mā avaharī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:83.5"
msgid "So “āṇatto ahaṃ tayā”ti, taṃ bhaṇḍaṃ avaharati, mūlaṭṭhassa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:83.6"
msgid "Avahārakassa āpatti pārājikassa."
msgstr ""

#. </p><p><a class="sc" id="80"></a>
