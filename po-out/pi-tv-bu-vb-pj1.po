msgid "(50)"
msgstr ""

#. </p><p><a class="sc" id="181"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.39"></a>
msgctxt "pi-tv-bu-vb-pj1:191.1"
msgid "Tena kho pana samayena aññataro bhikkhu vesāliyaṃ mahāvane kūṭāgārasālāyaṃ divāvihāragato dvāraṃ vivaritvā nipanno hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.2"
msgid "Tassa aṅgamaṅgāni vātūpatthaddhāni honti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.3"
msgid "Tena kho pana samayena sambahulā itthiyo gandhañca mālañca ādāya ārāmaṃ āgamaṃsu vihārapekkhikāyo."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.4"
msgid "Atha kho tā itthiyo taṃ bhikkhuṃ passitvā aṅgajāte abhinisīditvā yāvadatthaṃ katvā, purisūsabho vatāyanti vatvā gandhañca mālañca āropetvā pakkamiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.5"
msgid "Bhikkhū kilinnaṃ passitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.6"
msgid "“Pañcahi, bhikkhave, ākārehi aṅgajātaṃ kammaniyaṃ hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.7"
msgid "rāgena, vaccena, passāvena, vātena, uccāliṅgapāṇakadaṭṭhena."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.8"
msgid "Imehi kho, bhikkhave, pañcahākārehi aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.9"
msgid "Aṭṭhānametaṃ, bhikkhave, anavakāso, yaṃ tassa bhikkhuno rāgena aṅgajātaṃ kammaniyaṃ assa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.10"
msgid "Arahaṃ so, bhikkhave, bhikkhu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.11"
msgid "Anāpatti, bhikkhave, tassa bhikkhuno."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.12"
msgid "Anujānāmi, bhikkhave, divā paṭisallīyantena dvāraṃ saṃvaritvā paṭisallīyitun”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:191.13"
msgid "(51)"
msgstr ""

#. </p><p><a class="sc" id="182"></a>
#. VAR: supinante → supinantena (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-pj1:192.1"
msgid "Tena kho pana samayena aññataro bhārukacchako bhikkhu supinante purāṇadutiyikāya methunaṃ dhammaṃ paṭisevitvā—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:192.2"
msgid "assamaṇo ahaṃ, vibbhamissāmī”ti, bhārukacchaṃ gacchanto antarāmagge āyasmantaṃ upāliṃ passitvā etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:192.3"
msgid "Āyasmā upāli evamāha—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:192.4"
msgid "anāpatti, āvuso, supinantenā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:192.5"
msgid "(52)"
msgstr ""

#. </p><p><a class="sc" id="183"></a>
#. VAR: mudhappasannā → muddhappasannā (bj) | muduppasannā (s1-3) | buddhappasannā (pts1)
msgctxt "pi-tv-bu-vb-pj1:193.1"
msgid "Tena kho pana samayena rājagahe supabbā nāma upāsikā mudhappasannā hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.2"
msgid "Sā evaṃdiṭṭhikā hoti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.3"
msgid "yā methunaṃ dhammaṃ deti sā aggadānaṃ detī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.4"
msgid "Sā bhikkhuṃ passitvā etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.5"
msgid "ehi, bhante, methunaṃ dhammaṃ paṭisevā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.6"
msgid "“Alaṃ, bhagini, netaṃ kappatī”ti."
msgstr ""

#. VAR: ūruntarikāya → ūrantarikāya (bj) | urantarikāya (pts1)
msgctxt "pi-tv-bu-vb-pj1:193.7"
msgid "“Ehi, bhante, ūruntarikāya ghaṭṭehi, evaṃ te anāpatti bhavissatī”ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.8"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.9"
msgid "ehi, bhante, nābhiyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.10"
msgid "ehi, bhante, udaravaṭṭiyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.11"
msgid "ehi, bhante, upakacchake ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.12"
msgid "ehi, bhante, gīvāyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.13"
msgid "ehi, bhante, kaṇṇacchidde ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.14"
msgid "ehi, bhante, kesavaṭṭiyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.15"
msgid "ehi, bhante, aṅgulantarikāya ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.16"
msgid "“ehi, bhante, hatthena upakkamitvā mocessāmi, evaṃ te anāpatti bhavissatī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.17"
msgid "So bhikkhu tathā akāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.18"
msgid "Tassa kukkuccaṃ ahosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.19"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.20"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.21"
msgid "Āpatti saṃghādisesassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:193.22"
msgid "(53–‍61)"
msgstr ""

#. </p><p><a class="sc" id="184"></a>
msgctxt "pi-tv-bu-vb-pj1:194.1"
msgid "Tena kho pana samayena sāvatthiyaṃ saddhā nāma upāsikā mudhappasannā hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.2"
msgid "Sā evaṃdiṭṭhikā hoti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.3"
msgid "yā methunaṃ dhammaṃ deti sā aggadānaṃ detī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.4"
msgid "Sā bhikkhuṃ passitvā etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.5"
msgid "ehi, bhante, methunaṃ dhammaṃ paṭisevā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.6"
msgid "“Alaṃ, bhagini, netaṃ kappatī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.7"
msgid "“Ehi, bhante, ūruntarikāya ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.8"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.9"
msgid "ehi, bhante, hatthena upakkamitvā mocessāmi, evaṃ te anāpatti bhavissatī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.10"
msgid "So bhikkhu tathā akāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.11"
msgid "Tassa kukkuccaṃ ahosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.12"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.13"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.14"
msgid "Āpatti saṃghādisesassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:194.15"
msgid "(62–‍70)"
msgstr ""

#. </p><p><a class="sc" id="185"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.40"></a>
msgctxt "pi-tv-bu-vb-pj1:195.1"
msgid "Tena kho pana samayena vesāliyaṃ licchavikumārakā bhikkhuṃ gahetvā bhikkhuniyā vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:195.2"
msgid "sikkhamānāya vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:195.3"
msgid "sāmaṇeriyā vippaṭipādesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:195.4"
msgid "Ubho sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:195.5"
msgid "Ubho nāsetabbā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:195.6"
msgid "Ubho na sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:195.7"
msgid "Ubhinnaṃ anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:195.8"
msgid "(71–‍76)"
msgstr ""

#. </p><p><a class="sc" id="186"></a>
msgctxt "pi-tv-bu-vb-pj1:196.1"
msgid "Tena kho pana samayena vesāliyaṃ licchavikumārakā bhikkhuṃ gahetvā vesiyā vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:196.2"
msgid "paṇḍake vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:196.3"
msgid "gihiniyā vippaṭipādesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:196.4"
msgid "Bhikkhu sādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:196.5"
msgid "Bhikkhu nāsetabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:196.6"
msgid "Bhikkhu na sādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:196.7"
msgid "Bhikkhussa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:196.8"
msgid "(77–‍82)"
msgstr ""

#. </p><p><a class="sc" id="187"></a>
msgctxt "pi-tv-bu-vb-pj1:197.1"
msgid "Tena kho pana samayena vesāliyaṃ licchavikumārakā bhikkhū gahetvā aññamaññaṃ vippaṭipādesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:197.2"
msgid "Ubho sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:197.3"
msgid "Ubho nāsetabbā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:197.4"
msgid "Ubho na sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:197.5"
msgid "Ubhinnaṃ anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:197.6"
msgid "(83–‍84)"
msgstr ""

#. </p><p><a class="sc" id="188"></a>
msgctxt "pi-tv-bu-vb-pj1:198.1"
msgid "Tena kho pana samayena aññataro vuḍḍhapabbajito bhikkhu purāṇadutiyikāya dassanaṃ agamāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.2"
msgid "Sā—“"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.3"
msgid "ehi, bhante, vibbhamā”ti aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.4"
msgid "So bhikkhu paṭikkamanto uttāno paripati."
msgstr ""

#. VAR: ubbhajitvā → ubbhujitvā (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-pj1:198.5"
msgid "Sā ubbhajitvā aṅgajāte abhinisīdi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.6"
msgid "Tassa kukkuccaṃ ahosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.7"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.8"
msgid "“sādiyi tvaṃ, bhikkhū”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.9"
msgid "“Nāhaṃ, bhagavā, sādiyin”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.10"
msgid "“Anāpatti, bhikkhu, asādiyantassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:198.11"
msgid "(85)"
msgstr ""

#. </p><p><a class="sc" id="189"></a>
msgctxt "pi-tv-bu-vb-pj1:199.1"
msgid "Tena kho pana samayena aññataro bhikkhu araññe viharati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:199.2"
msgid "Migapotako tassa passāvaṭṭhānaṃ āgantvā passāvaṃ pivanto mukhena aṅgajātaṃ aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:199.3"
msgid "So bhikkhu sādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:199.4"
msgid "Tassa kukkuccaṃ ahosi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:199.5"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:199.6"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:199.7"
msgid "(86)"
msgstr ""

#. </p><p class="endsutta"><a class="sc" id="190"></a>
msgctxt "pi-tv-bu-vb-pj1:200.1"
msgid "Paṭhamapārājikaṃ samattaṃ."
msgstr ""

