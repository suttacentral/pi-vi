#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-ss2:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss2:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss2:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-ss2:4.1"
msgid "2. Kāyasaṃsaggasikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a>
msgctxt "pi-tv-bu-vb-ss2:5.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:5.2"
msgid "Tena kho pana samayena āyasmā udāyī araññe viharati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:5.3"
msgid "Tassāyasmato vihāro abhirūpo hoti dassanīyo pāsādiko, majjhegabbho, samantāpariyāgāro, supaññattaṃ mañcapīṭhaṃ bhisibibbohanaṃ, pānīyaṃ paribhojanīyaṃ sūpaṭṭhitaṃ, pariveṇaṃ susammaṭṭhaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:5.4"
msgid "Bahū manussā āyasmato udāyissa vihārapekkhakā āgacchanti."
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "pi-tv-bu-vb-ss2:6.1"
msgid "Aññataropi brāhmaṇo sapajāpatiko yenāyasmā udāyī tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.2"
msgid "upasaṅkamitvā āyasmantaṃ udāyiṃ etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.3"
msgid "icchāma mayaṃ bhoto udāyissa vihāraṃ pekkhitun”ti."
msgstr ""

#. VAR: avāpuraṇaṃ → apāpuraṇaṃ (s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss2:6.4"
msgid "“Tena hi, brāhmaṇa, pekkhassū”ti, avāpuraṇaṃ ādāya ghaṭikaṃ ugghāṭetvā kavāṭaṃ paṇāmetvā vihāraṃ pāvisi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.5"
msgid "Sopi kho brāhmaṇo āyasmato udāyissa piṭṭhito pāvisi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.6"
msgid "Sāpi kho brāhmaṇī tassa brāhmaṇassa piṭṭhito pāvisi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.7"
msgid "Atha kho āyasmā udāyī ekacce vātapāne vivaranto ekacce vātapāne thakento gabbhaṃ anuparigantvā piṭṭhito āgantvā tassā brāhmaṇiyā aṅgamaṅgāni parāmasi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.8"
msgid "Atha kho so brāhmaṇo āyasmatā udāyinā saddhiṃ paṭisammoditvā agamāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.9"
msgid "Atha kho so brāhmaṇo attamano attamanavācaṃ nicchāresi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.10"
msgid "uḷārā ime samaṇā sakyaputtiyā ye ime evarūpe araññe viharanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:6.11"
msgid "Bhavampi udāyī uḷāro yo evarūpe araññe viharatī”ti."
msgstr ""

#. </p><p><a class="sc" id="3"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.120"></a>
msgctxt "pi-tv-bu-vb-ss2:7.1"
msgid "Evaṃ vutte, sā brāhmaṇī taṃ brāhmaṇaṃ etadavoca—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.2"
msgid "kuto tassa uḷārattatā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.3"
msgid "Yatheva me tvaṃ aṅgamaṅgāni parāmasi evameva me samaṇo udāyī aṅgamaṅgāni parāmasī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.4"
msgid "Atha kho so brāhmaṇo ujjhāyati khiyyati vipāceti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.5"
msgid "alajjino ime samaṇā sakyaputtiyā dussīlā musāvādino."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.6"
msgid "Ime hi nāma dhammacārino samacārino brahmacārino saccavādino sīlavanto kalyāṇadhammā paṭijānissanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.7"
msgid "Natthi imesaṃ sāmaññaṃ natthi imesaṃ brahmaññaṃ, naṭṭhaṃ imesaṃ sāmaññaṃ naṭṭhaṃ imesaṃ brahmaññaṃ, kuto imesaṃ sāmaññaṃ kuto imesaṃ brahmaññaṃ, apagatā ime sāmaññā apagatā ime brahmaññā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.8"
msgid "Kathañhi nāma samaṇo udāyī mama bhariyāya aṅgamaṅgāni parāmasissati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:7.9"
msgid "Na hi sakkā kulitthīhi kuladhītāhi kulakumārīhi kulasuṇhāhi kuladāsīhi ārāmaṃ vā vihāraṃ vā gantuṃ."
msgstr ""

#. VAR: Sace → sace hi (s1-3)
#. VAR: kuladhītaro → kuladhītayo (bj) | kuladhītāyo (si, s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss2:7.10"
msgid "Sace kulitthiyo kuladhītaro kulakumāriyo kulasuṇhāyo kuladāsiyo ārāmaṃ vā vihāraṃ vā gaccheyyuṃ, tāpi samaṇā sakyaputtiyā dūseyyun”ti."
msgstr ""

#. </p><p><a class="sc" id="4"></a>
msgctxt "pi-tv-bu-vb-ss2:8.1"
msgid "Assosuṃ kho bhikkhu tassa brāhmaṇassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:8.2"
msgid "Ye te bhikkhū appicchā …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:8.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:8.4"
msgid "te ujjhāyanti khiyyanti vipācenti—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:8.5"
msgid "kathañhi nāma āyasmā udāyī mātugāmena saddhiṃ kāyasaṃsaggaṃ samāpajjissatī”ti."
msgstr ""

#. </p><p><a class="sc" id="5"></a>
msgctxt "pi-tv-bu-vb-ss2:9.1"
msgid "Atha kho te bhikkhū āyasmantaṃ udāyiṃ anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.2"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ udāyiṃ paṭipucchi—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.3"
msgid "saccaṃ kira tvaṃ, udāyi, mātugāmena saddhiṃ kāyasaṃsaggaṃ samāpajjasī”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.5"
msgid "Vigarahi buddho bhagavā—“"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.6"
msgid "ananucchavikaṃ, moghapurisa, ananulomikaṃ appatirūpaṃ assāmaṇakaṃ akappiyaṃ akaraṇīyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.7"
msgid "Kathañhi nāma tvaṃ, moghapurisa, mātugāmena saddhiṃ kāyasaṃsaggaṃ samāpajjissasi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.8"
msgid "Nanu mayā, moghapurisa, anekapariyāyena virāgāya dhammo desito no sarāgāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.9"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.10"
msgid "kāmapariḷāhānaṃ vūpasamo akkhāto."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.11"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.12"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:9.13"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="6"></a>
#. VAR: veṇiggāhaṃ → veṇigāhaṃ (bj, s1-3, pts1)
#. VAR: hatthaggāhaṃ → hatthagāhaṃ (bj, s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss2:10.1"
msgid "<strong class=\"rule\">“Yo pana bhikkhu otiṇṇo vipariṇatena cittena mātugāmena saddhiṃ kāyasaṃsaggaṃ samāpajjeyya hatthaggāhaṃ vā veṇiggāhaṃ vā aññatarassa vā aññatarassa vā aṅgassa parāmasanaṃ, saṃghādiseso”</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:10.2"
msgid "(<em>2</em>:<strong>6</strong>)"
msgstr ""

#. </p><p><a class="sc" id="7"></a>
msgctxt "pi-tv-bu-vb-ss2:11.1"
msgid "<strong>Yo panā</strong>ti yo yādiso …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:11.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:11.3"
msgid "<strong>bhikkhū</strong>ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:11.4"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:11.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. </p><p><a class="sc" id="8"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.121"></a>
msgctxt "pi-tv-bu-vb-ss2:12.1"
msgid "<strong>Otiṇṇo</strong> nāma sāratto apekkhavā paṭibaddhacitto."
msgstr ""

#. </p><p><a class="sc" id="9"></a>
msgctxt "pi-tv-bu-vb-ss2:13.1"
msgid "<strong>Vipariṇatan</strong>ti rattampi cittaṃ vipariṇataṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:13.2"
msgid "Duṭṭhampi cittaṃ vipariṇataṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:13.3"
msgid "Mūḷhampi cittaṃ vipariṇataṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:13.4"
msgid "Api ca rattaṃ cittaṃ imasmiṃ atthe adhippetaṃ vipariṇatanti."
msgstr ""

#. </p><p><a class="sc" id="10"></a>
msgctxt "pi-tv-bu-vb-ss2:14.1"
msgid "<strong>Mātugāmo</strong> nāma manussitthī, na yakkhī na petī, na tiracchānagatā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:14.2"
msgid "Antamaso tadahujātāpi dārikā, pageva mahattarī."
msgstr ""

#. </p><p><a class="sc" id="11"></a>
msgctxt "pi-tv-bu-vb-ss2:15.1"
msgid "<strong>Saddhin</strong>ti ekato."
msgstr ""

#. </p><p><a class="sc" id="12"></a>
msgctxt "pi-tv-bu-vb-ss2:16.1"
msgid "<strong>Kāyasaṃsaggaṃ samāpajjeyyā</strong>ti ajjhācāro vuccati."
msgstr ""

#. </p><p><a class="sc" id="13"></a>
msgctxt "pi-tv-bu-vb-ss2:17.1"
msgid "<strong>Hattho</strong> nāma kapparaṃ upādāya yāva agganakhā."
msgstr ""

#. </p><p><a class="sc" id="14"></a>
msgctxt "pi-tv-bu-vb-ss2:18.1"
msgid "<strong>Veṇī</strong> nāma suddhakesā vā, suttamissā vā, mālāmissā vā, hiraññamissā vā, suvaṇṇamissā vā, muttāmissā vā, maṇimissā vā."
msgstr ""

#. </p><p><a class="sc" id="15"></a>
msgctxt "pi-tv-bu-vb-ss2:19.1"
msgid "<strong>Aṅgaṃ</strong> nāma hatthañca veṇiñca ṭhapetvā avasesaṃ aṅgaṃ nāma."
msgstr ""

#. </p><p><a class="sc" id="16"></a>
msgctxt "pi-tv-bu-vb-ss2:20.1"
msgid "Āmasanā, parāmasanā, omasanā, ummasanā, olaṅghanā, ullaṅghanā, ākaḍḍhanā, patikaḍḍhanā, abhiniggaṇhanā, abhinippīḷanā, gahaṇaṃ, chupanaṃ."
msgstr ""

#. </p><p><a class="sc" id="17"></a>
msgctxt "pi-tv-bu-vb-ss2:21.1"
msgid "<strong>Āmasanā</strong> nāma āmaṭṭhamattā."
msgstr ""

#. </p><p><a class="sc" id="18"></a>
msgctxt "pi-tv-bu-vb-ss2:22.1"
msgid "<strong>Parāmasanā</strong> nāma itocito ca sañcopanā."
msgstr ""

#. </p><p><a class="sc" id="19"></a>
msgctxt "pi-tv-bu-vb-ss2:23.1"
msgid "<strong>Omasanā</strong> nāma heṭṭhā oropanā."
msgstr ""

#. </p><p><a class="sc" id="20"></a>
msgctxt "pi-tv-bu-vb-ss2:24.1"
msgid "<strong>Ummasanā</strong> nāma uddhaṃ uccāraṇā."
msgstr ""

#. </p><p><a class="sc" id="21"></a>
msgctxt "pi-tv-bu-vb-ss2:25.1"
msgid "<strong>Olaṅghanā</strong> nāma heṭṭhā onamanā."
msgstr ""

#. </p><p><a class="sc" id="22"></a>
msgctxt "pi-tv-bu-vb-ss2:26.1"
msgid "<strong>Ullaṅghanā</strong> nāma uddhaṃ uccāraṇā."
msgstr ""

#. </p><p><a class="sc" id="23"></a>
#. VAR: āviñchanā → āviñjanā (s1-3, pts1)
msgctxt "pi-tv-bu-vb-ss2:27.1"
msgid "<strong>Ākaḍḍhanā</strong> nāma āviñchanā."
msgstr ""

#. </p><p><a class="sc" id="24"></a>
msgctxt "pi-tv-bu-vb-ss2:28.1"
msgid "<strong>Patikaḍḍhanā</strong> nāma patippaṇāmanā."
msgstr ""

#. </p><p><a class="sc" id="25"></a>
msgctxt "pi-tv-bu-vb-ss2:29.1"
msgid "<strong>Abhiniggaṇhanā</strong> nāma aṅgaṃ gahetvā nippīḷanā."
msgstr ""

#. </p><p><a class="sc" id="26"></a>
msgctxt "pi-tv-bu-vb-ss2:30.1"
msgid "<strong>Abhinippīḷanā</strong> nāma kenaci saha nippīḷanā."
msgstr ""

#. </p><p><a class="sc" id="27"></a>
msgctxt "pi-tv-bu-vb-ss2:31.1"
msgid "<strong>Gahaṇaṃ</strong> nāma gahitamattaṃ."
msgstr ""

#. </p><p><a class="sc" id="28"></a>
msgctxt "pi-tv-bu-vb-ss2:32.1"
msgid "<strong>Chupanaṃ</strong> nāma phuṭṭhamattaṃ."
msgstr ""

#. </p><p><a class="sc" id="29"></a>
msgctxt "pi-tv-bu-vb-ss2:33.1"
msgid "<strong>Saṃghādiseso</strong>ti …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:33.2"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:33.3"
msgid "tenapi vuccati “saṃghādiseso”ti."
msgstr ""

#. </p><p><a class="sc" id="30"></a>
msgctxt "pi-tv-bu-vb-ss2:34.1"
msgid "Itthī ca hoti itthisaññī sāratto ca bhikkhu ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:34.2"
msgid "Naṃ itthiyā kāyena kāyaṃ āmasati parāmasati omasati ummasati olaṅgheti ullaṅgheti ākaḍḍhati patikaḍḍhati abhiniggaṇhāti abhinippīḷeti gaṇhāti chupati, āpatti saṃghādisesassa."
msgstr ""

#. </p><p><a class="sc" id="31"></a>
msgctxt "pi-tv-bu-vb-ss2:35.1"
msgid "Itthī ca hoti vematiko sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:35.2"
msgid "Bhikkhu ca naṃ itthiyā kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:35.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:35.4"
msgid "gaṇhāti chupati, āpatti thullaccayassa."
msgstr ""

#. </p><p><a class="sc" id="32"></a>
msgctxt "pi-tv-bu-vb-ss2:36.1"
msgid "Itthī ca hoti paṇḍakasaññī sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:36.2"
msgid "Bhikkhu ca naṃ itthiyā kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:36.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:36.4"
msgid "gaṇhāti chupati, āpatti thullaccayassa."
msgstr ""

#. </p><p><a class="sc" id="33"></a>
msgctxt "pi-tv-bu-vb-ss2:37.1"
msgid "Itthī ca hoti purisasaññī sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:37.2"
msgid "Bhikkhu ca naṃ itthiyā kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:37.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:37.4"
msgid "gaṇhāti chupati, āpatti thullaccayassa."
msgstr ""

#. </p><p><a class="sc" id="34"></a>
msgctxt "pi-tv-bu-vb-ss2:38.1"
msgid "Itthī ca hoti tiracchānagatasaññī sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:38.2"
msgid "Bhikkhu ca naṃ itthiyā kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:38.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:38.4"
msgid "gaṇhāti chupati, āpatti thullaccayassa."
msgstr ""

#. </p><p><a class="sc" id="35"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.122"></a>
msgctxt "pi-tv-bu-vb-ss2:39.1"
msgid "Paṇḍako ca hoti paṇḍakasaññī sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:39.2"
msgid "Bhikkhu ca naṃ paṇḍakassa kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:39.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:39.4"
msgid "gaṇhāti chupati, āpatti thullaccayassa."
msgstr ""

#. </p><p><a class="sc" id="36"></a>
msgctxt "pi-tv-bu-vb-ss2:40.1"
msgid "Paṇḍako ca hoti vematiko sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:40.2"
msgid "Bhikkhu ca naṃ paṇḍakassa kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:40.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:40.4"
msgid "gaṇhāti chupati, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="37"></a>
msgctxt "pi-tv-bu-vb-ss2:41.1"
msgid "Paṇḍako ca hoti purisasaññī sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:41.2"
msgid "Bhikkhu ca naṃ paṇḍakassa kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:41.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:41.4"
msgid "gaṇhāti chupati, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="38"></a>
msgctxt "pi-tv-bu-vb-ss2:42.1"
msgid "Paṇḍako ca hoti tiracchānagatasaññī sāratto ca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:42.2"
msgid "Bhikkhu ca naṃ paṇḍakassa kāyena kāyaṃ āmasati parāmasati …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:42.3"
msgid "pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:42.4"
