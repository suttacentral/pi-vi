msgctxt "pi-tv-bu-vb-ss1:178.4"
msgid "Bhagavato etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:178.5"
msgid "“Anāpatti, bhikkhu, supinantenā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:178.6"
msgid "(1)"
msgstr ""

#. </p><hr><p><a class="sc" id="173"></a><a class="pts-cs" id="bu-
#. ss.1.5.2"></a><a class="ms-pa" id="ms-pa946"></a>
