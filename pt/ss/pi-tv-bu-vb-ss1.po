msgctxt "pi-tv-bu-vb-ss1:178.1"
msgid "Tena kho pana samayena aññatarassa bhikkhuno supinantena asuci mucci."
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:178.2"
msgid "Tassa kukkuccaṃ ahosi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss1:178.3"
msgid "“bhagavatā sikkhāpadaṃ paññattaṃ, kacci nu kho ahaṃ saṃghādisesaṃ āpattiṃ āpanno”ti?"
