msgstr ""

msgctxt "pi-tv-bu-vb-ss6:89.8"
msgid "anārambhaṃ saparikkamanaṃ, āpatti saṃ­ghā­di­sesassa."
msgstr ""

#. </p><p><a class="sc" id="86"></a><a class="ms-pa" id="ms-pa1502"></a>
msgctxt "pi-tv-bu-vb-ss6:90.1"
msgid "Bhikkhu samādisitvā pakkamati—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:90.2"
msgid "“kuṭiṃ me karothā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:90.3"
msgid "Tassa kuṭiṃ karonti pamāṇikaṃ sārambhaṃ aparikkamanaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:90.4"
msgid "So ce vippakate āgacchati, tena bhikkhunā sā kuṭi aññassa vā dātabbā bhinditvā vā puna kātabbā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:90.5"
msgid "No ce aññassa vā dadeyya bhinditvā vā puna kāreyya, āpatti dvinnaṃ dukkaṭānaṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:90.6"
msgid "sārambhaṃ saparikkamanaṃ, āpatti dukkaṭassa … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:90.7"
msgid "anārambhaṃ aparikkamanaṃ, āpatti dukkaṭassa … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:90.8"
msgid "anārambhaṃ saparikkamanaṃ, anāpatti."
msgstr ""

#. </p><hr><p><a class="sc" id="87"></a><a class="ms-pa" id="ms-pa1503"></a>
msgctxt "pi-tv-bu-vb-ss6:91.1"
msgid "Bhikkhu samādisitvā pakkamati—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:91.2"
msgid "“kuṭiṃ me karothā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:91.3"
msgid "Tassa kuṭiṃ karonti adesi­ta­vatthu­kaṃ pamā­ṇātik­kantaṃ sārambhaṃ aparikkamanaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:91.4"
msgid "So ce vippakate āgacchati, tena bhikkhunā sā kuṭi aññassa vā dātabbā bhinditvā vā puna kātabbā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:91.5"
msgid "No ce aññassa vā dadeyya bhinditvā vā puna kāreyya, āpatti dvinnaṃ saṃghādisesena dvinnaṃ dukkaṭānaṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:91.6"
msgid "sārambhaṃ saparikkamanaṃ, āpatti dvinnaṃ saṃghādisesena dukkaṭassa … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:91.7"
msgid "anārambhaṃ aparikkamanaṃ, āpatti dvinnaṃ saṃghādisesena dukkaṭassa … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:91.8"
msgid "anārambhaṃ saparikkamanaṃ, āpatti dvinnaṃ saṃ­ghādi­sesānaṃ."
msgstr ""

#. </p><p><a class="sc" id="88"></a><a class="ms-pa" id="ms-pa1504"></a>
msgctxt "pi-tv-bu-vb-ss6:92.1"
msgid "Bhikkhu samādisitvā pakkamati—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:92.2"
msgid "“kuṭiṃ me karothā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:92.3"
msgid "Tassa kuṭiṃ karonti desita­vatthu­kaṃ pamāṇikaṃ sārambhaṃ aparikkamanaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:92.4"
msgid "So ce vippakate āgacchati, tena bhikkhunā sā kuṭi aññassa vā dātabbā bhinditvā vā puna kātabbā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:92.5"
msgid "No ce aññassa vā dadeyya bhinditvā vā puna kāreyya, āpatti dvinnaṃ dukkaṭānaṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:92.6"
msgid "sārambhaṃ saparikkamanaṃ, āpatti dukkaṭassa … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:92.7"
msgid "anārambhaṃ aparikkamanaṃ, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="89"></a><a class="ms-pa" id="ms-pa1505"></a>
