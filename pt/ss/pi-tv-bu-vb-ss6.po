msgctxt "pi-tv-bu-vb-ss6:98.2"
msgid "Anāpatti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:98.3"
msgid "leṇe"
msgstr ""

msgctxt "pi-tv-bu-vb-ss6:98.4"
msgid "guhāya"
msgstr ""

