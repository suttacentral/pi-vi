#. lang="en"><section class="sutta" id="pi-tv-bu-vb-ss5"><article><div
#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-ss5:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p class="division">
msgctxt "pi-tv-bu-vb-ss5:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:3.1"
msgid "Saṃ­ghā­di­sesa­kaṇḍa"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-ss5:4.1"
msgid "Sañ­carit­ta­sikkhā­pada"
msgstr ""

#. </h1></div><h2>
msgctxt "pi-tv-bu-vb-ss5:5.1"
msgid ""
msgstr ""

#. </h2><h3>
msgctxt "pi-tv-bu-vb-ss5:5.2"
msgid ""
msgstr ""

#. </h3><p><a class="sc" id="1"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.135"></a><a class="pts-vp-en" id="bd.1.229"></a><a class="pts-cs-empty"
#. id="bu-ss.5.1"></a><a class="pts-cs" id="bu-ss.5.1.1"></a><a class="ms-pa"
#. id="ms-pa1220"></a>
msgctxt "pi-tv-bu-vb-ss5:5.3"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anātha­piṇḍi­kassa ārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.4"
msgid "Tena kho pana samayena āyasmā udāyī sāvatthiyaṃ kulūpako hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.5"
msgid "Bahukāni kulāni upasaṅkamati."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:5.6"
msgid "Yattha passati kumārakaṃ vā apajāpatikaṃ, kumārikaṃ vā apatikaṃ, kumārakassa mātāpitūnaṃ santike kumārikāya vaṇṇaṃ bhaṇati—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.7"
msgid "“amukassa kulassa kumārikā abhirūpā dassanīyā pāsādikā paṇḍitā byattā medhāvinī dakkhā analasā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.8"
msgid "Channā sā kumārikā imassa kumārakassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.9"
msgid "Te evaṃ vadanti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.10"
msgid "“ete kho, bhante, amhe na jānanti—‘ke vā ime kassa vā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.11"
msgid "Sace, bhante, ayyo dāpeyya āneyyāma mayaṃ taṃ kumārikaṃ imassa kumārakassā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:5.12"
msgid "Kumārikāya mātāpitūnaṃ santike kumārakassa vaṇṇaṃ bhaṇati—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.13"
msgid "“amukassa kulassa kumārako abhirūpo dassanīyo pāsādiko paṇḍito byatto medhāvī dakkho analaso."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.14"
msgid "Channāyaṃ kumārikā tassa kumārakassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.15"
msgid "Te evaṃ vadanti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.16"
msgid "“ete kho, bhante, amhe na jānanti—‘ke vā ime kassa vā’ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.17"
msgid "kismiṃ viya kumārikāya vattuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.18"
msgid "Sace, bhante, ayyo yācāpeyya dajjeyyāma mayaṃ imaṃ kumārikaṃ tassa kumārakassā”ti."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.230"></a>
msgctxt "pi-tv-bu-vb-ss5:5.19"
msgid "Eteneva upāyena āvāhānipi kārāpeti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.20"
msgid "vivāhānipi kārāpeti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:5.21"
msgid "vāreyyānipi kārāpeti."
msgstr ""

#. </p><p><a class="sc" id="2"></a><a class="pts-cs" id="bu-ss.5.1.2"></a><a
#. class="ms-pa" id="ms-pa1221"></a>
msgctxt "pi-tv-bu-vb-ss5:6.1"
msgid "Tena kho pana samayena aññatarissā purāṇagaṇakiyā dhītā abhirūpā hoti dassanīyā pāsādikā."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.2"
msgid "Tirogāmakā ājīvakasāvakā āgantvā taṃ gaṇakiṃ etadavocuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.3"
msgid "“dehāyye, imaṃ kumārikaṃ amhākaṃ kumārakassā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:6.4"
msgid "Sā evamāha—“ahaṃ khvayyo, tumhe na jānāmi—‘ke vā ime kassa vā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.5"
msgid "Ayañca me ekadhītikā, tirogāmo ca gantabbo, nāhaṃ dassāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:6.6"
msgid "Manussā te ājīvakasāvake etadavocuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.7"
msgid "“kissa tumhe, ayyo, āgatatthā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.8"
msgid "“Idha mayaṃ, ayyo, amukaṃ nāma gaṇakiṃ dhītaraṃ yācimhā amhākaṃ kumārakassa."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.9"
msgid "Sā evamāha—‘ahaṃ khvayyo, tumhe na jānāmi—ke vā ime kassa vā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.10"
msgid "Ayañca me ekadhītikā, tirogāmo ca gantabbo, nāhaṃ dassāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:6.11"
msgid "“Kissa tumhe, ayyo, taṃ gaṇakiṃ dhītaraṃ yācittha?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.12"
msgid "Nanu ayyo udāyī vattabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:6.13"
msgid "Ayyo udāyī dāpessatī”ti."
msgstr ""

#. </p><p><a class="sc" id="3"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.136"></a><a class="ms-pa" id="ms-pa1222"></a>
msgctxt "pi-tv-bu-vb-ss5:7.1"
msgid "Atha kho te ājīvakasāvakā yenāyasmā udāyī tenu­pasaṅka­miṃsu;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:7.2"
msgid "upasaṅkamitvā āyasmantaṃ udāyiṃ etadavocuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:7.3"
msgid "“idha mayaṃ, bhante, amukaṃ nāma gaṇakiṃ dhītaraṃ yācimhā amhākaṃ kumārakassa."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:7.4"
msgid "Sā evamāha—‘ahaṃ khvayyo, tumhe na jānāmi—ke vā ime kassa vāti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:7.5"
msgid "Ayañca me ekadhītikā, tirogāmo ca gantabbo, nāhaṃ dassāmī’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:7.6"
msgid "Sādhu, bhante, ayyo taṃ gaṇakiṃ dhītaraṃ dāpetu amhākaṃ kumārakassā”ti."
msgstr ""

#. </p><p><a class="sc" id="4"></a><a class="ms-pa" id="ms-pa1223"></a>
msgctxt "pi-tv-bu-vb-ss5:8.1"
msgid "Atha kho āyasmā udāyī yena sā gaṇakī tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:8.2"
msgid "upasaṅkamitvā taṃ gaṇakiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:8.3"
msgid "“kissimesaṃ dhītaraṃ na desī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:8.4"
msgid "“Ahaṃ khvayya, ime na jānāmi—‘ke vā ime kassa vā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:8.5"
msgid "Ayañca me ekadhītikā, tirogāmo ca gantabbo, nāhaṃ dassāmī”ti."
msgstr ""

#. </p><p><a class="pts-vp-en" id="bd.1.231"></a>
msgctxt "pi-tv-bu-vb-ss5:8.6"
msgid "“Dehimesaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:8.7"
msgid "Ahaṃ ime jānāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:8.8"
msgid "“Sace, bhante, ayyo jānāti, dassāmī”ti."
msgstr ""

#. </p><p><a class="pts-cs" id="bu-ss.5.1.3"></a>
msgctxt "pi-tv-bu-vb-ss5:8.9"
msgid "Atha kho sā gaṇakī tesaṃ ājīva­ka­sāvakā­naṃ dhītaraṃ adāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:8.10"
msgid "Atha kho te ājīvakasāvakā taṃ kumārikaṃ netvā māsaṃyeva suṇisabhogena bhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:8.11"
msgid "Tato aparena dāsibhogena bhuñjanti."
msgstr ""

#. </p><p><a class="sc" id="5"></a><a class="ms-pa" id="ms-pa1224"></a>
msgctxt "pi-tv-bu-vb-ss5:9.1"
msgid "Atha kho sā kumārikā mātuyā santike dūtaṃ pāhesi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.2"
msgid "“ahañhi duggatā dukkhitā, na sukhaṃ labhāmi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.3"
msgid "Māsaṃyeva maṃ suṇisabhogena bhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.4"
msgid "Tato aparena dāsibhogena bhuñjanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.5"
msgid "Āgacchatu me, mātā, maṃ nessatū”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:9.6"
msgid "Atha kho sā gaṇakī yena te ājīvakasāvakā tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.7"
msgid "upasaṅkamitvā te ājīvakasāvake etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.8"
msgid "“māyyo, imaṃ kumārikaṃ dāsibhogena bhuñjittha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.9"
msgid "Suṇisabhogena imaṃ kumārikaṃ bhuñjathā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:9.10"
msgid "Te evamāhaṃsu—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.11"
msgid "“natthamhākaṃ tayā saddhiṃ āhārūpahāro, samaṇena saddhiṃ amhākaṃ āhārūpahāro,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.12"
msgid "gaccha tvaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.13"
msgid "na mayaṃ taṃ jānāmā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:9.14"
msgid "Atha kho sā gaṇakī tehi ājīva­ka­sā­vakehi apasāditā punadeva sāvatthiṃ paccāgañchi."
msgstr ""

#. </p><p><a class="sc" id="6"></a><a class="ms-pa" id="ms-pa1225"></a>
msgctxt "pi-tv-bu-vb-ss5:10.1"
msgid "Dutiyampi kho sā kumārikā mātuyā santike dūtaṃ pāhesi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.2"
msgid "“ahañhi duggatā dukkhitā, na sukhaṃ labhāmi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.3"
msgid "Māsaṃyeva maṃ suṇisabhogena bhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.4"
msgid "Tato aparena dāsibhogena bhuñjanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.5"
msgid "Āgacchatu me, mātā, maṃ nessatū”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.6"
msgid "Atha kho sā gaṇakī yenāyasmā udāyī tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.7"
msgid "upasaṅkamitvā āyasmantaṃ udāyiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.8"
msgid "“sā kira, bhante, kumārikā duggatā dukkhitā, na sukhaṃ labhati."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.232"></a>
msgctxt "pi-tv-bu-vb-ss5:10.9"
msgid "Māsaṃyeva naṃ suṇisabhogena bhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.10"
msgid "Tato aparena dāsibhogena bhuñjanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.11"
msgid "Vadeyyātha, bhante—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.12"
msgid "‘māyyo, imaṃ kumārikaṃ dāsibhogena bhuñjittha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:10.13"
msgid "Suṇisabhogena imaṃ kumārikaṃ bhuñjathā’”ti."
msgstr ""

#. </p><p><a class="sc" id="7"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.137"></a><a class="ms-pa" id="ms-pa1226"></a>
msgctxt "pi-tv-bu-vb-ss5:11.1"
msgid "Atha kho āyasmā udāyī yena te ājīvakasāvakā tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.2"
msgid "upasaṅkamitvā te ājīvakasāvake etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.3"
msgid "“māyyo, imaṃ kumārikaṃ dāsibhogena bhujjittha."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.4"
msgid "Suṇisabhogena imaṃ kumārikaṃ bhuñjathā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:11.5"
msgid "Te evamāhaṃsu—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.6"
msgid "“natthamhākaṃ tayā saddhiṃ āhārūpahāro, gaṇakiyā saddhiṃ amhākaṃ āhārūpahāro."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.7"
msgid "Samaṇena bhavitabbaṃ abyāvaṭena."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.8"
msgid "Samaṇo assa susamaṇo,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.9"
msgid "gaccha tvaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.10"
msgid "na mayaṃ taṃ jānāmā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:11.11"
msgid "Atha kho āyasmā udāyī tehi ājīva­ka­sā­vakehi apasādito punadeva sāvatthiṃ paccāgañchi."
msgstr ""

#. </p><p><a class="sc" id="8"></a><a class="ms-pa" id="ms-pa1227"></a>
msgctxt "pi-tv-bu-vb-ss5:12.1"
msgid "Tatiyampi kho sā kumārikā mātuyā santike dūtaṃ pāhesi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.2"
msgid "“ahañhi duggatā dukkhitā, na sukhaṃ labhāmi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.3"
msgid "Māsaṃyeva maṃ suṇisabhogena bhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.4"
msgid "Tato aparena dāsibhogena bhuñjanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.5"
msgid "Āgacchatu me, mātā, maṃ nessatū”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.6"
msgid "Dutiyampi kho sā gaṇakī yenāyasmā udāyī tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.7"
msgid "upasaṅkamitvā āyasmantaṃ udāyiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.8"
msgid "“sā kira, bhante, kumārikā duggatā dukkhitā, na sukhaṃ labhati."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.9"
msgid "Māsaṃyeva naṃ suṇisabhogena bhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.10"
msgid "Tato aparena dāsibhogena bhuñjanti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.11"
msgid "Vadeyyātha, bhante—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.12"
msgid "‘māyyo, imaṃ kumārikaṃ dāsibhogena bhuñjittha, suṇisabhogena imaṃ kumārikaṃ bhuñjathā’”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:12.13"
msgid "“Paṭhamampāhaṃ tehi ājīva­ka­sā­vakehi apasādito,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.14"
msgid "gaccha tvaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:12.15"
msgid "nāhaṃ gamissāmī”ti."
msgstr ""

#. </p><p><a class="sc" id="9"></a><a class="pts-cs" id="bu-ss.5.1.4"></a><a
#. class="ms-pa" id="ms-pa1228"></a><a class="pts-vp-en" id="bd.1.233"></a>
msgctxt "pi-tv-bu-vb-ss5:13.1"
msgid "Atha kho sā gaṇakī ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:13.2"
msgid "“evaṃ duggato hotu ayyo udāyī, evaṃ dukkhito hotu ayyo udāyī, evaṃ mā sukhaṃ labhatu ayyo udāyī, yathā me kumārikā duggatā dukkhitā na sukhaṃ labhati pāpikāya sassuyā pāpakena sasurena pāpakena sāmikenā”ti."
msgstr ""

#. </p><p><a class="sc" id="10"></a><a class="ms-pa" id="ms-pa1229"></a><a
#. class="ms-pa" id="ms-pa1230"></a>
msgctxt "pi-tv-bu-vb-ss5:14.1"
msgid "Sāpi kho kumārikā ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:14.2"
msgid "“evaṃ duggato hotu ayyo udāyī, evaṃ dukkhito hotu ayyo udāyī, evaṃ mā sukhaṃ labhatu ayyo udāyī, yathāhaṃ duggatā dukkhitā na sukhaṃ labhāmi pāpikāya sassuyā pāpakena sasurena pāpakena sāmikenā”ti."
msgstr ""

#. </p><p><a class="sc" id="11"></a>
msgctxt "pi-tv-bu-vb-ss5:15.1"
msgid "Aññāpi itthiyo asantuṭṭhā sassūhi vā sasurehi vā sāmikehi vā, tā evaṃ oyācanti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:15.2"
msgid "“evaṃ duggato hotu ayyo udāyī, evaṃ dukkhito hotu ayyo udāyī, evaṃ mā sukhaṃ labhatu ayyo udāyī, yathā mayaṃ duggatā dukkhitā na sukhaṃ labhāma pāpikāhi sassūhi pāpakehi sasurehi pāpakehi sāmikehī”ti."
msgstr ""

#. </p><p><a class="sc" id="12"></a><a class="ms-pa" id="ms-pa1231"></a>
msgctxt "pi-tv-bu-vb-ss5:16.1"
msgid "Yā pana tā itthiyo santuṭṭhā sassūhi vā sasurehi vā sāmikehi vā tā evaṃ āyācanti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:16.2"
msgid "“evaṃ sukhito hotu ayyo udāyī, evaṃ sajjito hotu ayyo udāyī, evaṃ sukhamedho hotu ayyo udāyī, yathā mayaṃ sukhitā sajjitā sukhamedhā bhaddikāhi sassūhi bhaddakehi sasurehi bhaddakehi sāmikehī”ti."
msgstr ""

#. </p><p><a class="sc" id="13"></a><a class="pts-cs" id="bu-ss.5.1.5"></a><a
#. class="ms-pa" id="ms-pa1232"></a>
msgctxt "pi-tv-bu-vb-ss5:17.1"
msgid "Assosuṃ kho bhikkhū ekaccānaṃ itthīnaṃ oyācantīnaṃ ekaccānaṃ itthīnaṃ āyācantīnaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:17.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:17.3"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:17.4"
msgid "“kathañhi nāma āyasmā udāyī sañcarittaṃ samāpajjissatī”ti."
msgstr ""

#. </p><p><a class="sc" id="14"></a><a class="ms-pa" id="ms-pa1233"></a>
msgctxt "pi-tv-bu-vb-ss5:18.1"
msgid "Atha kho te bhikkhū āyasmantaṃ udāyiṃ aneka­pariyā­yena vigarahitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.234"></a>
msgctxt "pi-tv-bu-vb-ss5:18.2"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā āyasmantaṃ udāyiṃ paṭipucchi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:18.3"
msgid "“saccaṃ kira tvaṃ, udāyi, sañcarittaṃ samāpajjasī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:18.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:18.5"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:18.6"
msgid "kathañhi nāma tvaṃ, moghapurisa, sañcarittaṃ samāpajjissasi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:18.7"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:18.8"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><h3>
msgctxt "pi-tv-bu-vb-ss5:19.1"
msgid ""
msgstr ""

#. </h3><p><a class="sc" id="15"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.138"></a><a class="ms-pa" id="ms-pa1234"></a>
#. <strong class="rule">
msgctxt "pi-tv-bu-vb-ss5:19.2"
msgid "“Yo pana bhikkhu sañcarittaṃ samāpajjeyya, itthiyā vā purisamatiṃ purisassa vā itthimatiṃ, jāyattane vā jārattane vā, saṃghādiseso”ti."
msgstr ""

#. </strong>
#. </p><p class="endsection"><a class="sc" id="16"></a><a class="ms-pa"
#. id="ms-pa1235"></a>
msgctxt "pi-tv-bu-vb-ss5:20.1"
msgid "Evañcidaṃ bhagavatā bhikkhūnaṃ sikkhāpadaṃ paññattaṃ hoti."
msgstr ""

#. </p><h3>
msgctxt "pi-tv-bu-vb-ss5:21.1"
msgid ""
msgstr ""

#. </h3><p><a class="sc" id="17"></a><a class="pts-cs-empty" id="bu-
#. ss.5.2"></a><a class="pts-cs" id="bu-ss.5.2.1"></a><a class="ms-pa" id="ms-
#. pa1236"></a>
msgctxt "pi-tv-bu-vb-ss5:21.2"
msgid "Tena kho pana samayena sambahulā dhuttā uyyāne paricārentā aññatarissā vesiyā santike dūtaṃ pāhesuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:21.3"
msgid "“āgacchatu uyyāne paricāressāmā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:21.4"
msgid "Sā evamāha—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:21.5"
msgid "“ahaṃ khvayyo, tumhe na jānāmi—‘ke vā ime kassa vā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:21.6"
msgid "Ahañcamhi bahubhaṇḍā bahuparikkhārā, bahinagarañca gantabbaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:21.7"
msgid "Nāhaṃ gamissāmī”ti."
msgstr ""

#. </p><p><a class="sc" id="18"></a><a class="ms-pa" id="ms-pa1237"></a>
msgctxt "pi-tv-bu-vb-ss5:22.1"
msgid "Atha kho so dūto tesaṃ dhuttānaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.2"
msgid "Evaṃ vutte, aññataro puriso te dhutte etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.3"
msgid "“kissa tumhe, ayyo, etaṃ vesiṃ yācittha?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.4"
msgid "Nanu ayyo udāyī vattabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.5"
msgid "Ayyo udāyī uyyojessatī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:22.6"
msgid "Evaṃ vutte, aññataro upāsako taṃ purisaṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.7"
msgid "“māyyo, evaṃ avaca."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.8"
msgid "Na kappati samaṇānaṃ sakya­put­ti­yā­naṃ evarūpaṃ kātuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.9"
msgid "Nāyyo udāyī evaṃ karissatī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:22.10"
msgid "Evaṃ vutte, “karissati na karissatī”ti abbhutamakaṃsu."
msgstr ""

#. </p><p><a class="sc" id="19"></a><a class="ms-pa" id="ms-pa1238"></a><a
#. class="pts-vp-en" id="bd.1.235"></a>
msgctxt "pi-tv-bu-vb-ss5:23.1"
msgid "Atha kho te dhuttā yenāyasmā udāyī tenu­pasaṅka­miṃsu;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:23.2"
msgid "upasaṅkamitvā āyasmantaṃ udāyiṃ etadavocuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:23.3"
msgid "“idha mayaṃ, bhante, uyyāne paricārentā asukāya nāma vesiyā santike dūtaṃ pahiṇimhā—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:23.4"
msgid "‘āgacchatu uyyāne paricāressāmā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:23.5"
msgid "Sā evamāha—‘ahaṃ khvayyo, tumhe na jānāmi—ke vā ime kassa vāti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:23.6"
msgid "ahañcamhi bahubhaṇḍā bahuparikkhārā, bahinagarañca gantabbaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:23.7"
msgid "Nāhaṃ gamissāmī’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:23.8"
msgid "Sādhu, bhante, ayyo taṃ vesiṃ uyyojetū”ti."
msgstr ""

#. </p><p><a class="sc" id="20"></a><a class="ms-pa" id="ms-pa1239"></a>
msgctxt "pi-tv-bu-vb-ss5:24.1"
msgid "Atha kho āyasmā udāyī yena sā vesī tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:24.2"
msgid "upasaṅkamitvā taṃ vesiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:24.3"
msgid "“kissimesaṃ na gacchasī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:24.4"
msgid "“Ahaṃ khvayya, ime na jānāmi—‘ke vā ime kassa vā’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:24.5"
msgid "Ahañcamhi bahubhaṇḍā bahuparikkhārā, bahinagarañca gantabbaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:24.6"
msgid "Nāhaṃ gamissāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:24.7"
msgid "“Gacchimesaṃ. Ahaṃ ime jānāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:24.8"
msgid "“Sace, bhante, ayyo jānāti ahaṃ gamissāmī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:24.9"
msgid "Atha kho te dhuttā taṃ vesiṃ ādāya uyyānaṃ agamaṃsu."
msgstr ""

#. </p><p><a class="pts-cs" id="bu-ss.5.2.2"></a>
msgctxt "pi-tv-bu-vb-ss5:24.10"
msgid "Atha kho so upāsako ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:24.11"
msgid "“kathañhi nāma ayyo udāyī taṅkhaṇikaṃ sañcarittaṃ samāpajjissatī”ti."
msgstr ""

#. </p><p><a class="sc" id="21"></a><a class="ms-pa" id="ms-pa1240"></a>
msgctxt "pi-tv-bu-vb-ss5:25.1"
msgid "Assosuṃ kho bhikkhū tassa upāsakassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:25.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:25.3"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:25.4"
msgid "“kathañhi nāma āyasmā udāyī taṅkhaṇikaṃ sañcarittaṃ samāpajjissatī”ti."
msgstr ""

#. </p><p><a class="sc" id="22"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.139"></a><a class="ms-pa" id="ms-pa1241"></a>
msgctxt "pi-tv-bu-vb-ss5:26.1"
msgid "Atha kho te bhikkhū āyasmantaṃ udāyiṃ aneka­pariyā­yena vigarahitvā bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:26.2"
msgid "“saccaṃ kira tvaṃ, udāyi, taṅkhaṇikaṃ sañcarittaṃ samāpajjasī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:26.3"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss5:26.4"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:26.5"
msgid "kathañhi nāma tvaṃ, moghapurisa, taṅkhaṇikaṃ sañcarittaṃ samāpajjissasi?"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:26.6"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:26.7"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-ss5:27.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="23"></a><a class="ms-pa" id="ms-pa1242"></a><a
#. class="pts-vp-en" id="bd.1.236"></a>
#. <strong class="rule">
msgctxt "pi-tv-bu-vb-ss5:27.2"
msgid "“Yo pana bhikkhu sañcarittaṃ samāpajjeyya, itthiyā vā purisamatiṃ purisassa vā itthimatiṃ, jāyattane vā jārattane vā, antamaso taṅkhaṇikāyapi, saṃghādiseso”ti."
msgstr ""

#. </strong>
msgctxt "pi-tv-bu-vb-ss5:27.3"
msgid "(<em>5</em>:<strong>9</strong>)"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-ss5:28.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="24"></a><a class="pts-cs" id="bu-ss.5.3"></a><a
#. class="ms-pa" id="ms-pa1243"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:28.2"
msgid "Yo panāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:28.3"
msgid "yo yādiso … pe …"
msgstr ""

#. </span>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:28.4"
msgid "bhikkhūti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:28.5"
msgid "… pe … ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="25"></a><a class="ms-pa" id="ms-pa1244"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:29.1"
msgid "Sañcarittaṃ samāpajjeyyāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:29.2"
msgid "itthiyā vā pahito purisassa santike gacchati, purisena vā pahito itthiyā santike gacchati."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="26"></a><a class="ms-pa" id="ms-pa1245"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:30.1"
msgid "Itthiyā vā purisamatinti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:30.2"
msgid "purisassa matiṃ itthiyā āroceti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="27"></a><a class="ms-pa" id="ms-pa1246"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:31.1"
msgid "Purisassa vā itthimatinti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:31.2"
msgid "itthiyā matiṃ purisassa āroceti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="28"></a><a class="ms-pa" id="ms-pa1247"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:32.1"
msgid "Jāyattane vāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:32.2"
msgid "jāyā bhavissasi."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="29"></a><a class="ms-pa" id="ms-pa1248"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:33.1"
msgid "Jārattane vāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:33.2"
msgid "jārī bhavissasi."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="30"></a><a class="ms-pa" id="ms-pa1249"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:34.1"
msgid "Antamaso taṅkhaṇikāyapīti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:34.2"
msgid "muhuttikā bhavissasi."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="31"></a><a class="ms-pa" id="ms-pa1250"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:35.1"
msgid "Saṃghādisesoti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:35.2"
msgid "… pe … tenapi vuccati “saṃghādiseso”ti."
msgstr ""

#. </span>
#. </p><h2>
msgctxt "pi-tv-bu-vb-ss5:36.1"
msgid ""
msgstr ""

#. </h2><h3>
msgctxt "pi-tv-bu-vb-ss5:36.2"
msgid ""
msgstr ""

#. </h3><h4>
msgctxt "pi-tv-bu-vb-ss5:36.3"
msgid ""
msgstr ""

#. </h4><p><a class="sc" id="32"></a><a class="pts-cs-empty" id="bu-
#. ss.5.4"></a><a class="pts-cs" id="bu-ss.5.4.1"></a><a class="ms-pa" id="ms-
#. pa1251"></a>
msgctxt "pi-tv-bu-vb-ss5:36.4"
msgid "Dasa itthiyo—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:36.5"
msgid "māturakkhitā piturakkhitā ­mātāpi­tu­rak­khitā bhāturakkhitā bhagi­ni­rak­khitā ñātirakkhitā gottarakkhitā dhammarakkhitā sārakkhā saparidaṇḍā."
msgstr ""

#. </p><p><a class="sc" id="33"></a><a class="pts-vp-en" id="bd.1.237"></a><a
#. class="ms-pa" id="ms-pa1252"></a>
msgctxt "pi-tv-bu-vb-ss5:37.1"
msgid "Dasa bhariyāyo—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:37.2"
msgid "dhanakkītā chandavāsinī bhogavāsinī paṭavāsinī odapattakinī obhaṭacumbaṭā dāsī ca bhariyā ca kammakārī ca bhariyā ca dhajāhaṭā muhuttikā."
msgstr ""

#. </p><h4>
msgctxt "pi-tv-bu-vb-ss5:38.1"
msgid ""
msgstr ""

#. </h4><p><a class="sc" id="34"></a><a class="pts-cs" id="bu-ss.5.4.2"></a><a
#. class="ms-pa" id="ms-pa1253"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:38.2"
msgid "Māturakkhitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:38.3"
msgid "mātā rakkhati gopeti issariyaṃ kāreti vasaṃ vatteti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="35"></a><a class="ms-pa" id="ms-pa1254"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:39.1"
msgid "Piturakkhitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:39.2"
msgid "pitā rakkhati gopeti issariyaṃ kāreti vasaṃ vatteti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="36"></a><a class="ms-pa" id="ms-pa1255"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:40.1"
msgid "­Mātāpi­tu­rak­khitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:40.2"
msgid "mātāpitaro rakkhanti gopenti issariyaṃ kārenti vasaṃ vattenti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="37"></a><a class="ms-pa" id="ms-pa1256"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:41.1"
msgid "Bhāturakkhitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:41.2"
msgid "bhātā rakkhati gopeti issariyaṃ kāreti vasaṃ vatteti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="38"></a><a class="ms-pa" id="ms-pa1257"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:42.1"
msgid "Bhagi­ni­rak­khitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:42.2"
msgid "bhaginī rakkhati gopeti issariyaṃ kāreti vasaṃ vatteti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="39"></a><a class="ms-pa" id="ms-pa1258"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:43.1"
msgid "Ñātirakkhitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:43.2"
msgid "ñātakā rakkhanti gopenti issariyaṃ kārenti vasaṃ vattenti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="40"></a><a class="ms-pa" id="ms-pa1259"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:44.1"
msgid "Gottarakkhitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:44.2"
msgid "sagottā rakkhanti gopenti issariyaṃ kārenti vasaṃ vattenti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="41"></a><a class="ms-pa" id="ms-pa1260"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:45.1"
msgid "Dhammarakkhitā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:45.2"
msgid "sahadhammikā rakkhanti gopenti issariyaṃ kārenti vasaṃ vattenti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="42"></a><a class="ms-pa" id="ms-pa1261"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:46.1"
msgid "Sārakkhā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:46.2"
msgid "gabbhepi pariggahitā hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:46.3"
msgid "mayhaṃ esāti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss5:46.4"
msgid "Antamaso mālā­guḷa­parik­khit­tāpi."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="43"></a><a class="ms-pa" id="ms-pa1262"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ss5:47.1"
msgid "Saparidaṇḍā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ss5:47.2"
msgid "kehici daṇḍo ṭhapito hoti—yo itthannāmaṃ itthiṃ gacchati ettako daṇḍoti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="44"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.140"></a><a class="pts-cs" id="bu-ss.5.4.3"></a><a class="ms-pa"
