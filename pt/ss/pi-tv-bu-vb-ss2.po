msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:136.3"
msgid "“sādiyi tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss2:136.4"
msgid "“Nāhaṃ, bhagavā, sādiyin”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ss2:136.5"
msgid "“Anāpatti, bhikkhu, asādiyantassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ss2:136.6"
msgid "(11)"
msgstr ""

