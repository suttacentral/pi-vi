msgid "pañca ceva amūlakā;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:99.3"
msgid "Dubbhikkhe kuramaṃsañca,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:99.4"
msgid "pūva­sak­kha­li­modakā."
msgstr ""

#. </p><p><a class="sc" id="95"></a><a class="ms-pa" id="ms.285"></a>
msgctxt "pi-tv-bu-vb-pj2:100.1"
msgid "Cha­parik­khā­ra­thavikā,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:100.2"
msgid "bhisivaṃsā na nikkhame;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:100.3"
msgid "Khādanīyañca vissāsaṃ,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:100.4"
msgid "sasaññāyapare duve."
msgstr ""

#. </p><p><a class="sc" id="96"></a><a class="ms-pa" id="ms.286"></a>
msgctxt "pi-tv-bu-vb-pj2:101.1"
msgid "Satta nāvaharāmāti,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:101.2"
msgid "satta ceva avāharuṃ;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:101.3"
msgid "Saṃghassa avaharuṃ satta,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:101.4"
msgid "pupphehi apare duve."
msgstr ""

#. </p><p><a class="sc" id="97"></a><a class="ms-pa" id="ms.287"></a>
msgctxt "pi-tv-bu-vb-pj2:102.1"
msgid "Tayo ca vuttavādino,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:102.2"
msgid "maṇi tīṇi atikkame;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:102.3"
msgid "Sūkarā ca migā macchā,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:102.4"
msgid "yānañcāpi pavattayi."
msgstr ""

#. </p><p><a class="sc" id="98"></a><a class="ms-pa" id="ms.288"></a>
msgctxt "pi-tv-bu-vb-pj2:103.1"
msgid "Duve pesī duve dārū,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:103.2"
msgid "paṃsukūlaṃ duve dakā;"
msgstr ""

#. <br>
#. <a class="pts-vp-pi" id="pts-vp-pi3.56"></a>"
msgctxt "pi-tv-bu-vb-pj2:103.3"
msgid "Anu­pubba­vidhā­nena,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:103.4"
msgid "tadañño na paripūrayi."
msgstr ""

#. </p><p><a class="sc" id="99"></a><a class="ms-pa" id="ms.289"></a>
msgctxt "pi-tv-bu-vb-pj2:104.1"
msgid "Sāvatthiyā caturo muṭṭhī,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:104.2"
msgid "dve vighāsā duve tiṇā;"
msgstr ""

#. <br><a class="pts-vp-en" id="bd.1.94"></a>
msgctxt "pi-tv-bu-vb-pj2:104.3"
msgid "Saṃghassa bhājayuṃ satta,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:104.4"
msgid "satta ceva assāmikā."
msgstr ""

#. </p><p><a class="sc" id="100"></a><a class="ms-pa" id="ms.290"></a>
msgctxt "pi-tv-bu-vb-pj2:105.1"
msgid "Dārudakā mattikā dve tiṇāni,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:105.2"
msgid "Saṃghassa satta avahāsi seyyaṃ;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:105.3"
msgid "Sassāmikaṃ na cāpi nīhareyya,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:105.4"
msgid "Hareyya sassāmikaṃ tāvakālikaṃ."
msgstr ""

#. </p><p><a class="sc" id="101"></a><a class="ms-pa" id="ms.291"></a>
msgctxt "pi-tv-bu-vb-pj2:106.1"
msgid "Campā rājagahe ceva,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:106.2"
msgid "vesāliyā ca ajjuko;"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:106.3"
msgid "Bārāṇasī ca kosambī,"
msgstr ""

#. <br>
msgctxt "pi-tv-bu-vb-pj2:106.4"
msgid "sāgalā daḷhikena cāti."
msgstr ""

#. </p></div><h3>
msgctxt "pi-tv-bu-vb-pj2:107.1"
msgid "2.2. Vinītavatthu"
msgstr ""

#. </h3><p><a class="sc" id="102"></a><a class="pts-cs-empty" id="Bu-
#. Pj.2.7"></a><a class="pts-cs" id="Bu-Pj.2.7.1"></a><a class="ms-pa"
#. id="ms.292"></a>
msgctxt "pi-tv-bu-vb-pj2:108.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū raja­kattha­ra­ṇaṃ gantvā rajaka­bhaṇḍi­kaṃ avahariṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:108.2"
msgid "Tesaṃ kukkuccaṃ ahosi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:108.3"
msgid "“bhagavatā sikkhāpadaṃ paññattaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:108.4"
msgid "Kacci nu kho mayaṃ pārājikaṃ āpattiṃ āpannā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:108.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:108.6"
msgid "“Āpattiṃ tumhe, bhikkhave, āpannā pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:108.7"
msgid "(1)"
msgstr ""

#. </p><hr><p><a class="sc" id="103"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.2"></a><a class="ms-pa" id="ms.293"></a>
msgctxt "pi-tv-bu-vb-pj2:109.1"
msgid "Tena kho pana samayena aññataro bhikkhu raja­kattha­ra­ṇaṃ gantvā mahagghaṃ dussaṃ passitvā theyyacittaṃ uppādesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:109.2"
msgid "Tassa kukkuccaṃ ahosi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:109.3"
msgid "“bhagavatā sikkhāpadaṃ paññattaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:109.4"
msgid "kacci nu kho ahaṃ pārājikaṃ āpattiṃ āpanno”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:109.5"
msgid "Bhagavato etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:109.6"
msgid "“Anāpatti, bhikkhu, cittuppāde”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:109.7"
msgid "(2)"
msgstr ""

#. </p><hr><p><a class="sc" id="104"></a><a class="ms-pa" id="ms.294"></a>
msgctxt "pi-tv-bu-vb-pj2:110.1"
msgid "Tena kho pana samayena aññataro bhikkhu raja­kattha­ra­ṇaṃ gantvā mahagghaṃ dussaṃ passitvā theyyacitto āmasi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:110.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:110.3"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:110.4"
msgid "Āpatti dukkaṭassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:110.5"
msgid "(3)"
msgstr ""

#. </p><hr><p><a class="sc" id="105"></a><a class="ms-pa" id="ms.295"></a>
msgctxt "pi-tv-bu-vb-pj2:111.1"
msgid "Tena kho pana samayena aññataro bhikkhu raja­kattha­ra­ṇaṃ gantvā mahagghaṃ dussaṃ passitvā theyyacitto phandāpesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:111.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:111.3"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:111.4"
msgid "Āpatti thullaccayassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:111.5"
msgid "(4)"
msgstr ""

#. </p><hr><p><a class="sc" id="106"></a><a class="ms-pa" id="ms.296"></a>
msgctxt "pi-tv-bu-vb-pj2:112.1"
msgid "Tena kho pana samayena aññataro bhikkhu raja­kattha­ra­ṇaṃ gantvā mahagghaṃ dussaṃ passitvā theyyacitto ṭhānā cāvesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:112.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:112.3"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:112.4"
msgid "(5)"
msgstr ""

#. </p><hr><p><a class="sc" id="107"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.3"></a><a class="ms-pa" id="ms.297"></a><a class="pts-vp-en"
#. id="bd.1.95"></a>
msgctxt "pi-tv-bu-vb-pj2:113.1"
msgid "Tena kho pana samayena aññataro piṇḍacāriko bhikkhu mahagghaṃ utta­rattha­ra­ṇaṃ passitvā theyyacittaṃ uppādesi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:113.2"
msgid "theyyacitto āmasi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:113.3"
msgid "theyyacitto phandāpesi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:113.4"
msgid "theyyacitto ṭhānā cāvesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:113.5"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:113.6"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:113.7"
msgid "(6–‍9)"
msgstr ""

#. </p><hr><p><a class="sc" id="108"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.4"></a><a class="ms-pa" id="ms.298"></a>
msgctxt "pi-tv-bu-vb-pj2:114.1"
msgid "Tena kho pana samayena aññataro bhikkhu divā bhaṇḍaṃ passitvā nimittaṃ akāsi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.2"
msgid "“rattiṃ avaharissāmī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.3"
msgid "So taṃ maññamāno taṃ avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.4"
msgid "taṃ maññamāno aññaṃ avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.5"
msgid "aññaṃ maññamāno taṃ avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.6"
msgid "aññaṃ maññamāno aññaṃ avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.7"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.8"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:114.9"
msgid "(10–‍13)"
msgstr ""

#. </p><p><a class="sc" id="109"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.57"></a> <a class="ms-pa" id="ms.299"></a>
msgctxt "pi-tv-bu-vb-pj2:115.1"
msgid "Tena kho pana samayena aññataro bhikkhu divā bhaṇḍaṃ passitvā nimittaṃ akāsi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:115.2"
msgid "“rattiṃ avaharissāmī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:115.3"
msgid "So taṃ maññamāno attano bhaṇḍaṃ avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:115.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:115.5"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:115.6"
msgid "Āpatti dukkaṭassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:115.7"
msgid "(14)"
msgstr ""

#. </p><hr><p><a class="sc" id="110"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.5"></a><a class="ms-pa" id="ms.300"></a>
msgctxt "pi-tv-bu-vb-pj2:116.1"
msgid "Tena kho pana samayena aññataro bhikkhu aññassa bhaṇḍaṃ haranto sīse bhāraṃ theyyacitto āmasi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.2"
msgid "theyyacitto phandāpesi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.3"
msgid "theyyacitto khandhaṃ oropesi … pe …"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:116.4"
msgid "khandhe bhāraṃ theyyacitto āmasi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.5"
msgid "theyyacitto phandāpesi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.6"
msgid "theyyacitto kaṭiṃ oropesi … pe …"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:116.7"
msgid "kaṭiyā bhāraṃ theyyacitto āmasi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.8"
msgid "theyyacitto phandāpesi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.9"
msgid "theyyacitto hatthena aggahesi … pe …"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:116.10"
msgid "hatthe bhāraṃ theyyacitto bhūmiyaṃ nikkhipi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.11"
msgid "theyyacitto bhūmito aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.12"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.13"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:116.14"
msgid "(15–‍25)"
msgstr ""

#. </p><hr><p><a class="sc" id="111"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.6"></a><a class="ms-pa" id="ms.301"></a>
msgctxt "pi-tv-bu-vb-pj2:117.1"
msgid "Tena kho pana samayena aññataro bhikkhu ajjhokāse cīvaraṃ pattharitvā vihāraṃ pāvisi."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.96"></a>
msgctxt "pi-tv-bu-vb-pj2:117.2"
msgid "Aññataro bhikkhu—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.3"
msgid "“māyidaṃ cīvaraṃ nassī”ti, paṭisāmesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.4"
msgid "So nikkhamitvā taṃ bhikkhuṃ pucchi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.5"
msgid "“āvuso, mayhaṃ cīvaraṃ kena avahaṭan”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.6"
msgid "So evamāha—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.7"
msgid "“mayā avahaṭan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.8"
msgid "“So taṃ ādiyi, assamaṇosi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.9"
msgid "Tassa kukkuccaṃ ahosi … pe …."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.10"
msgid "Bhagavato etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.11"
msgid "“Kiṃcitto tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:117.12"
msgid "“Niruttipatho ahaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:117.13"
msgid "“Anāpatti, bhikkhu, niruttipathe”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:117.14"
msgid "(26)"
msgstr ""

#. </p><hr><p><a class="sc" id="112"></a><a class="ms-pa" id="ms.302"></a>
msgctxt "pi-tv-bu-vb-pj2:118.1"
msgid "Tena kho pana samayena aññataro bhikkhu pīṭhe cīvaraṃ nikkhipitvā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.2"
msgid "Pīṭhe nisīdanaṃ nikkhipitvā …"
msgstr ""

#. <span class="note" hidden>I have added one set of ellipses points that
#. seems to be missing from the Pali of MS.1.302.</span>
msgctxt "pi-tv-bu-vb-pj2:118.3"
msgid "heṭṭhāpīṭhe pattaṃ nikkhipitvā vihāraṃ pāvisi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.4"
msgid "Aññataro bhikkhu—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.5"
msgid "“māyaṃ patto nassī”ti paṭisāmesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.6"
msgid "So nikkhamitvā taṃ bhikkhuṃ pucchi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.7"
msgid "“āvuso, mayhaṃ patto kena avahaṭo”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.8"
msgid "So evamāha—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.9"
msgid "“mayā avahaṭo”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.10"
msgid "So taṃ ādiyi, “assamaṇosi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.11"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.12"
msgid "“anāpatti, bhikkhu, niruttipathe”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:118.13"
msgid "(27–‍29)"
msgstr ""

#. </p><hr><p><a class="sc" id="113"></a><a class="ms-pa"
#. id="ms.303"></a><span class="bhikkhuni">
msgctxt "pi-tv-bu-vb-pj2:119.1"
msgid "Tena kho pana samayena aññatarā bhikkhunī vatiyā cīvaraṃ pattharitvā vihāraṃ pāvisi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.2"
msgid "Aññatarā bhikkhunī—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.3"
msgid "“māyidaṃ cīvaraṃ nassī”ti paṭisāmesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.4"
msgid "Sā nikkhamitvā taṃ bhikkhuniṃ pucchi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.5"
msgid "“ayye, mayhaṃ cīvaraṃ kena avahaṭan”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.6"
msgid "Sā evamāha—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.7"
msgid "“mayā avahaṭan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.8"
msgid "“Sā taṃ ādiyi, assamaṇīsi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.9"
msgid "Tassā kukkuccaṃ ahosi … pe …."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.10"
msgid "Atha kho sā bhikkhunī bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.11"
msgid "Bhikkhuniyo bhikkhūnaṃ etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.12"
msgid "Bhikkhū bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:119.13"
msgid "“anāpatti, bhikkhave, niruttipathe”ti."
msgstr ""

#. </span>
msgctxt "pi-tv-bu-vb-pj2:119.14"
msgid "(30)"
msgstr ""

#. </p><hr><p><a class="sc" id="114"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.7"></a><a class="pts-vp-pi" id="pts-vp-pi3.58"></a> <a class="ms-pa"
#. id="ms.304"></a>
msgctxt "pi-tv-bu-vb-pj2:120.1"
msgid "Tena kho pana samayena aññataro bhikkhu vāta­maṇḍa­li­kāya ukkhittaṃ sāṭakaṃ passitvā sāmikānaṃ dassāmīti, aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:120.2"
msgid "Sāmikā taṃ bhikkhuṃ codesuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:120.3"
msgid "“assamaṇosi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:120.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:120.5"
msgid "“kiṃcitto tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
#. <a class="pts-vp-en" id="bd.1.97"></a>
msgctxt "pi-tv-bu-vb-pj2:120.6"
msgid "“Atheyyacitto ahaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:120.7"
msgid "“Anāpatti, bhikkhu, atheyya­cittassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:120.8"
msgid "(31)"
msgstr ""

#. </p><hr><p><a class="sc" id="115"></a><a class="ms-pa" id="ms.305"></a>
msgctxt "pi-tv-bu-vb-pj2:121.1"
msgid "Tena kho pana samayena aññataro bhikkhu vāta­maṇḍa­li­kāya ukkhittaṃ veṭhanaṃ passitvā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:121.2"
msgid "“pure sāmikā passantī”ti theyyacitto aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:121.3"
msgid "Sāmikā taṃ bhikkhuṃ codesuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:121.4"
msgid "“assamaṇosi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:121.5"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:121.6"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:121.7"
msgid "(32)"
msgstr ""

#. </p><hr><p><a class="sc" id="116"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.8"></a><a class="ms-pa" id="ms.306"></a>
msgctxt "pi-tv-bu-vb-pj2:122.1"
msgid "Tena kho pana samayena aññataro bhikkhu susānaṃ gantvā abhinne sarīre paṃsukūlaṃ aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.2"
msgid "Tasmiñca sarīre peto adhivattho hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.3"
msgid "Atha kho so peto taṃ bhikkhuṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.4"
msgid "“mā, bhante, mayhaṃ sāṭakaṃ aggahesī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.5"
msgid "So bhikkhu anādiyanto agamāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.6"
msgid "Atha kho taṃ sarīraṃ uṭṭhahitvā tassa bhikkhuno piṭṭhito piṭṭhito anubandhi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.7"
msgid "Atha kho so bhikkhu vihāraṃ pavisitvā dvāraṃ thakesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.8"
msgid "Atha kho taṃ sarīraṃ tattheva paripati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.9"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.10"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

#. <strong class="rule">
msgctxt "pi-tv-bu-vb-pj2:122.11"
msgid "Na ca, bhikkhave, abhinne sarīre paṃsukūlaṃ gahetabbaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:122.12"
msgid "Yo gaṇheyya, āpatti dukkaṭassā”ti."
msgstr ""

#. </strong>
msgctxt "pi-tv-bu-vb-pj2:122.13"
msgid "(33)"
msgstr ""

#. </p><hr><p><a class="sc" id="117"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.9"></a><a class="ms-pa" id="ms.307"></a>
msgctxt "pi-tv-bu-vb-pj2:123.1"
msgid "Tena kho pana samayena aññataro bhikkhu saṃghassa cīvare bhājīyamāne theyyacitto kusaṃ saṅkāmetvā cīvaraṃ aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:123.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:123.3"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:123.4"
msgid "(34)"
msgstr ""

#. </p><hr><p><a class="sc" id="118"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.10"></a><a class="ms-pa" id="ms.308"></a>
msgctxt "pi-tv-bu-vb-pj2:124.1"
msgid "Tena kho pana samayena āyasmā ānando jantāghare aññatarassa bhikkhuno antaravāsakaṃ attano maññamāno nivāsesi."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.98"></a>
msgctxt "pi-tv-bu-vb-pj2:124.2"
msgid "Atha kho so bhikkhu āyasmantaṃ ānandaṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:124.3"
msgid "“kissa me tvaṃ, āvuso ānanda, antaravāsakaṃ nivāsesī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:124.4"
msgid "“Sakasaññī ahaṃ, āvuso”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:124.5"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:124.6"
msgid "“Anāpatti, bhikkhave, sakasaññissā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:124.7"
msgid "(35)"
msgstr ""

#. </p><hr><p><a class="sc" id="119"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.11"></a><a class="ms-pa" id="ms.309"></a>
msgctxt "pi-tv-bu-vb-pj2:125.1"
msgid "Tena kho pana samayena sambahulā bhikkhū gijjhakūṭā pabbatā orohantā sīhavighāsaṃ passitvā pacāpetvā paribhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:125.2"
msgid "Tesaṃ kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:125.3"
msgid "“anāpatti, bhikkhave, sīhavighāse”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:125.4"
msgid "(36)"
msgstr ""

#. </p><hr><p><a class="sc" id="120"></a><a class="ms-pa" id="ms.310"></a>
msgctxt "pi-tv-bu-vb-pj2:126.1"
msgid "Tena kho pana samayena sambahulā bhikkhū gijjhakūṭā pabbatā orohantā byag­gha­vighā­saṃ passitvā …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:126.2"
msgid "dīpivighāsaṃ passitvā …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:126.3"
msgid "tarac­cha­vighā­saṃ passitvā …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:126.4"
msgid "kokavighāsaṃ passitvā pacāpetvā paribhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:126.5"
msgid "Tesaṃ kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:126.6"
msgid "“anāpatti, bhikkhave, tiracchā­na­gata­parig­gahe”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:126.7"
msgid "(37–‍40)"
msgstr ""

#. </p><hr><p><a class="sc" id="121"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.12"></a><a class="pts-vp-pi" id="pts-vp-pi3.59"></a> <a class="ms-
#. pa" id="ms.311"></a>
msgctxt "pi-tv-bu-vb-pj2:127.1"
msgid "Tena kho pana samayena aññataro bhikkhu saṃghassa odane bhājīyamāne—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:127.2"
msgid "“aparassa bhāgaṃ dehī”ti amūlakaṃ aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:127.3"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:127.4"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:127.5"
msgid "Āpatti sam­pajā­na­musā­vāde pācittiyassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:127.6"
msgid "(41)"
msgstr ""

#. </p><hr><p><a class="sc" id="122"></a><a class="ms-pa" id="ms.312"></a>
msgctxt "pi-tv-bu-vb-pj2:128.1"
msgid "Tena kho pana samayena aññataro bhikkhu saṃghassa khādanīye bhājīyamāne …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:128.2"
msgid "saṃghassa pūve bhājīyamāne …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:128.3"
msgid "saṃghassa ucchumhi bhājīyamāne …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:128.4"
msgid "saṃghassa timbarūsake bhājīyamāne—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:128.5"
msgid "“aparassa bhāgaṃ dehī”ti amūlakaṃ aggahesi."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.99"></a>
msgctxt "pi-tv-bu-vb-pj2:128.6"
msgid "Tassa kukkuccaṃ ahosi … pe …."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:128.7"
msgid "“Anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:128.8"
msgid "Āpatti sam­pajā­na­musā­vāde pācittiyassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:128.9"
msgid "(42–‍45)"
msgstr ""

#. </p><hr><p><a class="sc" id="123"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.13"></a><a class="ms-pa" id="ms.313"></a>
msgctxt "pi-tv-bu-vb-pj2:129.1"
msgid "Tena kho pana samayena aññataro bhikkhu dubbhikkhe odanīyagharaṃ pavisitvā pattapūraṃ odanaṃ theyyacitto avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:129.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:129.3"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:129.4"
msgid "(46)"
msgstr ""

#. </p><hr><p><a class="sc" id="124"></a><a class="ms-pa" id="ms.314"></a>
msgctxt "pi-tv-bu-vb-pj2:130.1"
msgid "Tena kho pana samayena aññataro bhikkhu dubbhikkhe sūnagharaṃ pavisitvā pattapūraṃ maṃsaṃ theyyacitto avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:130.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:130.3"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:130.4"
msgid "(47)"
msgstr ""

#. </p><hr><p><a class="sc" id="125"></a><a class="ms-pa" id="ms.315"></a>
msgctxt "pi-tv-bu-vb-pj2:131.1"
msgid "Tena kho pana samayena aññataro bhikkhu dubbhikkhe pūvagharaṃ pavisitvā pattapūraṃ pūvaṃ theyyacitto avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:131.2"
msgid "pattapūrā sakkhaliyo theyyacitto avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:131.3"
msgid "pattapūre modake theyyacitto avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:131.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:131.5"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:131.6"
msgid "(48–‍50)"
msgstr ""

#. </p><hr><p><a class="sc" id="126"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.14"></a><a class="ms-pa" id="ms.316"></a>
msgctxt "pi-tv-bu-vb-pj2:132.1"
msgid "Tena kho pana samayena aññataro bhikkhu divā parikkhāraṃ passitvā nimittaṃ akāsi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.2"
msgid "“rattiṃ avaharissāmī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.3"
msgid "So taṃ maññamāno taṃ avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.4"
msgid "taṃ maññamāno aññaṃ avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.5"
msgid "aññaṃ maññamāno taṃ avahari … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.6"
msgid "aññaṃ maññamāno aññaṃ avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.7"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.8"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:132.9"
msgid "(51–‍54)"
msgstr ""

#. </p><hr><p><a class="sc" id="127"></a><a class="ms-pa" id="ms.317"></a>
msgctxt "pi-tv-bu-vb-pj2:133.1"
msgid "Tena kho pana samayena aññataro bhikkhu divā parikkhāraṃ passitvā nimittaṃ akāsi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:133.2"
msgid "“rattiṃ avaharissāmī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:133.3"
msgid "So taṃ maññamāno attano parikkhāraṃ avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:133.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:133.5"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:133.6"
msgid "Āpatti dukkaṭassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:133.7"
msgid "(55)"
msgstr ""

#. </p><hr><p><a class="sc" id="128"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.15"></a><a class="ms-pa" id="ms.318"></a>
msgctxt "pi-tv-bu-vb-pj2:134.1"
msgid "Tena kho pana samayena aññataro bhikkhu pīṭhe thavikaṃ passitvā—"
msgstr ""

#. <a class="pts-vp-en" id="bd.1.100"></a>
msgctxt "pi-tv-bu-vb-pj2:134.2"
msgid "“ito gaṇhanto pārājiko bhavissāmī”ti saha pīṭhakena saṅkāmetvā aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:134.3"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:134.4"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:134.5"
msgid "(56)"
msgstr ""

#. </p><hr><p><a class="sc" id="129"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.16"></a><a class="ms-pa" id="ms.319"></a>
msgctxt "pi-tv-bu-vb-pj2:135.1"
msgid "Tena kho pana samayena aññataro bhikkhu saṃghassa bhisiṃ theyyacitto avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:135.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:135.3"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:135.4"
msgid "(57)"
msgstr ""

#. </p><hr><p><a class="sc" id="130"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.17"></a><a class="ms-pa" id="ms.320"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.60"></a>
msgctxt "pi-tv-bu-vb-pj2:136.1"
msgid "Tena kho pana samayena aññataro bhikkhu cīvaravaṃse cīvaraṃ theyyacitto avahari."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:136.2"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:136.3"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:136.4"
msgid "(58)"
msgstr ""

#. </p><hr><p><a class="sc" id="131"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.18"></a><a class="ms-pa" id="ms.321"></a>
msgctxt "pi-tv-bu-vb-pj2:137.1"
msgid "Tena kho pana samayena aññataro bhikkhu vihāre cīvaraṃ avaharitvā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:137.2"
msgid "“ito nikkhamanto pārājiko bhavissāmī”ti vihārā na nikkhami … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:137.3"
msgid "bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:137.4"
msgid "“Nikkhami vā so, bhikkhave, moghapuriso na vā nikkhami, āpatti pārājikassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:137.5"
msgid "(59)"
msgstr ""

#. </p><hr><p><a class="sc" id="132"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.19"></a><a class="ms-pa" id="ms.322"></a>
msgctxt "pi-tv-bu-vb-pj2:138.1"
msgid "Tena kho pana samayena dve bhikkhū sahāyakā honti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:138.2"
msgid "Eko bhikkhu gāmaṃ piṇḍāya pāvisi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:138.3"
msgid "Dutiyo bhikkhu saṃghassa khādanīye bhājīyamāne sahāyakassa bhāgaṃ gahetvā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:138.4"
msgid "tassa vissasanto paribhuñji."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:138.5"
msgid "So jānitvā taṃ codesi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:138.6"
msgid "“assamaṇosi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:138.7"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:138.8"
msgid "“kiṃcitto tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:138.9"
msgid "“Vissāsaggāho ahaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:138.10"
msgid "“Anāpatti, bhikkhu, vissāsaggāhe”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:138.11"
msgid "(60)"
msgstr ""

#. </p><hr><p><a class="sc" id="133"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.20"></a><a class="ms-pa" id="ms.323"></a>
msgctxt "pi-tv-bu-vb-pj2:139.1"
msgid "Tena kho pana samayena sambahulā bhikkhū cīvarakammaṃ karonti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:139.2"
msgid "Saṃghassa khādanīye bhājīyamāne sabbesaṃ paṭivisā āharitvā upanikkhittā honti."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.101"></a>
msgctxt "pi-tv-bu-vb-pj2:139.3"
msgid "Aññataro bhikkhu aññatarassa bhikkhuno paṭivisaṃ attano maññamāno paribhuñji."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:139.4"
msgid "So jānitvā taṃ codesi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:139.5"
msgid "“assamaṇosi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:139.6"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:139.7"
msgid "“kiṃcitto tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:139.8"
msgid "“Sakasaññī ahaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:139.9"
msgid "“Anāpatti, bhikkhu, sakasaññissā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:139.10"
msgid "(61)"
msgstr ""

#. </p><hr><p><a class="sc" id="134"></a><a class="ms-pa" id="ms.324"></a>
msgctxt "pi-tv-bu-vb-pj2:140.1"
msgid "Tena kho pana samayena sambahulā bhikkhū cīvarakammaṃ karonti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:140.2"
msgid "Saṃghassa khādanīye bhājīyamāne aññatarassa bhikkhuno pattena aññatarassa bhikkhuno paṭiviso āharitvā upanikkhitto hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:140.3"
msgid "Pattasāmiko bhikkhu attano maññamāno paribhuñji."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:140.4"
msgid "So jānitvā taṃ codesi—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:140.5"
msgid "“assamaṇosi tvan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:140.6"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:140.7"
msgid "“anāpatti, bhikkhu, sakasaññissā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:140.8"
msgid "(62)"
msgstr ""

#. </p><hr><p><a class="sc" id="135"></a><a class="pts-cs" id="Bu-
#. Pj.2.7.21"></a><a class="ms-pa" id="ms.325"></a>
msgctxt "pi-tv-bu-vb-pj2:141.1"
msgid "Tena kho pana samayena ambacorakā ambaṃ pātetvā bhaṇḍikaṃ ādāya agamaṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.2"
msgid "Sāmikā te corake anubandhiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.3"
msgid "Corakā sāmike passitvā bhaṇḍikaṃ pātetvā palāyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.4"
msgid "Bhikkhū paṃsu­kūla­saññino paṭiggahāpetvā paribhuñjiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.5"
msgid "Sāmikā te bhikkhū codesuṃ—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.6"
msgid "“assamaṇāttha tumhe”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.7"
msgid "Tesaṃ kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.8"
msgid "bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:141.9"
msgid "“Kiṃcittā tumhe, bhikkhave”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:141.10"
msgid "“Paṃsu­kūla­saññino mayaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj2:141.11"
msgid "“Anāpatti, bhikkhave, paṃsu­kūla­saññissā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj2:141.12"
msgid "(63)"
msgstr ""

