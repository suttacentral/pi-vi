msgctxt "pi-tv-bu-vb-pj1:141.2"
msgid "yakkhiniyā methunaṃ dhammaṃ paṭisevi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:141.3"
msgid "petiyā methunaṃ dhammaṃ paṭisevi …"
msgstr ""

#. <a class="pts-vp-en" id="bd.1.58"></a>
msgctxt "pi-tv-bu-vb-pj1:141.4"
msgid "paṇḍakassa methunaṃ dhammaṃ paṭisevi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:141.5"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:141.6"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:141.7"
msgid "(37–‍40)"
msgstr ""

#. </p><hr><p><a class="sc" id="173"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.15"></a><a class="ms-pa" id="ms.173"></a>
msgctxt "pi-tv-bu-vb-pj1:142.1"
msgid "Tena kho pana samayena aññataro bhikkhu upahatindriyo hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:142.2"
msgid "So—“nāhaṃ vediyāmi sukhaṃ vā dukkhaṃ vā, anāpatti me bhavissatī”ti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:142.3"
msgid "methunaṃ dhammaṃ paṭisevi. … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:142.4"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:142.5"
msgid "“Vediyi vā so, bhikkhave, moghapuriso na vā vediyi, āpatti pārājikassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:142.6"
msgid "(41)"
msgstr ""

#. </p><hr><p><a class="sc" id="174"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.16"></a><a class="ms-pa" id="ms.174"></a>
msgctxt "pi-tv-bu-vb-pj1:143.1"
msgid "Tena kho pana samayena aññataro bhikkhu—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:143.2"
msgid "“itthiyā methunaṃ dhammaṃ paṭisevissāmī”ti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:143.3"
msgid "chupitamatte vippaṭisārī ahosi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:143.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:143.5"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:143.6"
msgid "Āpatti saṃ­ghā­di­sesassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:143.7"
msgid "(42)"
msgstr ""

#. </p><hr><p><a class="sc" id="175"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.17"></a><a class="ms-pa" id="ms.175"></a>
msgctxt "pi-tv-bu-vb-pj1:144.1"
msgid "Tena kho pana samayena aññataro bhikkhu bhaddiye jātiyāvane divāvihāragato nipanno hoti."
msgstr ""

#. <span class="note" hidden><i lang="pi">Aṅgamaṅgāni</i>, “various bodily
#. parts.” The point seems to be that the wind element caused stiffness
#. throughout the body.</span>
msgctxt "pi-tv-bu-vb-pj1:144.2"
msgid "Tassa aṅgamaṅgāni vātū­patthad­dhāni honti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:144.3"
msgid "Aññatarā itthī passitvā aṅgajāte abhinisīditvā yāvadatthaṃ katvā pakkāmi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:144.4"
msgid "Bhikkhū kilinnaṃ passitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. <a class="pts-vp-pi" id="pts-vp-pi3.38"></a><span class="parallel">
msgctxt "pi-tv-bu-vb-pj1:144.5"
msgid "“Pañcahi, bhikkhave, ākārehi aṅgajātaṃ kammaniyaṃ hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:144.6"
msgid "rāgena, vaccena, passāvena, vātena, uccāliṅ­ga­pāṇaka­daṭ­ṭhena."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:144.7"
msgid "Imehi kho, bhikkhave, pañcahākārehi aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:144.8"
msgid "Aṭṭhānametaṃ, bhikkhave, anavakāso yaṃ tassa bhikkhuno rāgena aṅgajātaṃ kammaniyaṃ assa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:144.9"
msgid "Arahaṃ so, bhikkhave, bhikkhu."
msgstr ""

#. </span>
msgctxt "pi-tv-bu-vb-pj1:144.10"
msgid "Anāpatti, bhikkhave, tassa bhikkhuno”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:144.11"
msgid "(43)"
msgstr ""

#. </p><hr><p><a class="sc" id="176"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.18"></a><a class="ms-pa" id="ms.176"></a>
msgctxt "pi-tv-bu-vb-pj1:145.1"
msgid "Tena kho pana samayena aññataro bhikkhu sāvatthiyā andhavane divāvihāragato nipanno hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:145.2"
msgid "Aññatarā gopālikā passitvā aṅgajāte abhinisīdi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:145.3"
msgid "So bhikkhu pavesanaṃ sādiyi, paviṭṭhaṃ sādiyi, ṭhitaṃ sādiyi, uddharaṇaṃ sādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:145.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:145.5"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:145.6"
msgid "(44)"
msgstr ""

#. </p><hr><p><a class="sc" id="177"></a><a class="ms-pa" id="ms.177"></a>
msgctxt "pi-tv-bu-vb-pj1:146.1"
msgid "Tena kho pana samayena aññataro bhikkhu sāvatthiyā andhavane divāvihāragato nipanno hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:146.2"
msgid "Aññatarā ajapālikā passitvā …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:146.3"
msgid "aññatarā kaṭṭhahārikā passitvā …"
msgstr ""

#. <a class="pts-vp-en" id="bd.1.59"></a>
msgctxt "pi-tv-bu-vb-pj1:146.4"
msgid "aññatarā gomayahārikā passitvā aṅgajāte abhinisīdi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:146.5"
msgid "So bhikkhu pavesanaṃ sādiyi, paviṭṭhaṃ sādiyi, ṭhitaṃ sādiyi, uddharaṇaṃ sādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:146.6"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:146.7"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:146.8"
msgid "(45–‍47)"
msgstr ""

#. </p><hr><p><a class="sc" id="178"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.19"></a><a class="ms-pa" id="ms.178"></a>
msgctxt "pi-tv-bu-vb-pj1:147.1"
msgid "Tena kho pana samayena aññataro bhikkhu vesāliyaṃ mahāvane divāvihāragato nipanno hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:147.2"
msgid "Aññatarā itthī passitvā aṅgajāte abhinisīditvā yāvadatthaṃ katvā sāmantā hasamānā ṭhitā hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:147.3"
msgid "So bhikkhu paṭibujjhitvā taṃ itthiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:147.4"
msgid "“tuyhidaṃ kamman”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:147.5"
msgid "“Āma, mayhaṃ kamman”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:147.6"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:147.7"
msgid "“sādiyi tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:147.8"
msgid "“Nāhaṃ, bhagavā, jānāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:147.9"
msgid "“ Anāpatti, bhikkhu, ajānantassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:147.10"
msgid "(48)"
msgstr ""

#. </p><hr><p><a class="sc" id="179"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.20"></a><a class="ms-pa" id="ms.179"></a>
msgctxt "pi-tv-bu-vb-pj1:148.1"
msgid "Tena kho pana samayena aññataro bhikkhu vesāliyaṃ mahāvane divāvihāragato rukkhaṃ apassāya nipanno hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:148.2"
msgid "Aññatarā itthī passitvā aṅgajāte abhinisīdi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:148.3"
msgid "So bhikkhu sahasā vuṭṭhāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:148.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:148.5"
msgid "“sādiyi tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:148.6"
msgid "“Nāhaṃ, bhagavā, sādiyin”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:148.7"
msgid "“Anāpatti, bhikkhu, asādiyantassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:148.8"
msgid "(49)"
msgstr ""

#. </p><hr><p><a class="sc" id="180"></a><a class="ms-pa" id="ms.180"></a>
msgctxt "pi-tv-bu-vb-pj1:149.1"
msgid "Tena kho pana samayena aññataro bhikkhu vesāliyaṃ mahāvane divāvihāragato rukkhaṃ apassāya nipanno hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:149.2"
msgid "Aññatarā itthī passitvā aṅgajāte abhinisīdi."
