msgid "Yāni vā panaññānipi atthi buddha­ve­vaca­nāni vā dhamma­ve­vaca­nāni vā saṃgha­ve­vaca­nāni vā sikkhā­ve­vaca­nāni vā vinaya­ve­vaca­nāni vā pāti­mokkha­ve­vaca­nāni vā uddesa­ve­vaca­nāni vā upaj­jhā­ya­ve­vaca­nāni vā ācari­ya­ve­vaca­nāni vā sad­dhi­vihā­ri­ka­ve­vaca­nāni vā antevā­si­ka­ve­vaca­nāni vā samā­nu­paj­jhā­ya­ka­ve­vaca­nāni vā samā­nā­cari­ya­ka­ve­vaca­nāni vā sab­rahma­cāri­ve­vaca­nāni vā gihivevacanāni vā upāsa­ka­ve­vaca­nāni vā ārāmi­ka­ve­vaca­nāni vā sāma­ṇera­ve­vaca­nāni vā ­titthi­ya­ve­vaca­nāni vā ­titthi­ya­sāvaka­ve­vaca­nāni vā assama­ṇa­ve­vaca­nāni vā asakya­put­ti­ya­ve­vaca­nāni vā,"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:68.2"
msgid "tehi ākārehi tehi liṅgehi tehi nimittehi vadati viññāpeti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:68.3"
msgid "Evaṃ kho, bhikkhave, dubbal­yā­vi­kammañ­ceva hoti sikkhā ca paccakkhātā."
msgstr ""

#. </p><p><a class="sc" id="102"></a><a class="pts-cs" id="Bu-Pj.1.8.4"></a><a
#. class="ms-pa" id="ms.102"></a>
msgctxt "pi-tv-bu-vb-pj1:69.1"
msgid "Kathañca, bhikkhave, apaccakkhātā hoti sikkhā?"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.2"
msgid "Idha, bhikkhave, yehi ākārehi yehi liṅgehi yehi nimittehi sikkhā paccakkhātā hoti tehi ākārehi tehi liṅgehi tehi nimittehi ummattako sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.3"
msgid "Ummattakassa santike sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.47"></a>
msgctxt "pi-tv-bu-vb-pj1:69.4"
msgid "Khittacitto sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.5"
msgid "Khittacittassa santike sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.6"
msgid "Vedanāṭṭo sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.7"
msgid "Vedanāṭṭassa santike sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.8"
msgid "Devatāya santike sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.9"
msgid "Tiracchā­na­gatassa santike sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.10"
msgid "Ariyakena milakkhassa santike sikkhaṃ paccakkhāti, so ca na paṭivijānāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.11"
msgid "Milakkhakena ariyassa santike sikkhaṃ paccakkhāti, so ca na paṭivijānāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.12"
msgid "Ariyakena ariyassa santike sikkhaṃ paccakkhāti, so ca na paṭivijānāti, apaccakkhātā hoti sikkhā."
msgstr ""

#. <a class="pts-vp-pi" id="pts-vp-pi3.28"></a>
msgctxt "pi-tv-bu-vb-pj1:69.13"
msgid "Milakkhakena milakkhassa santike sikkhaṃ paccakkhāti, so ca na paṭivijānāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.14"
msgid "Davāya sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.15"
msgid "Ravāya sikkhaṃ paccakkhāti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.16"
msgid "Asāvetukāmo sāveti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.17"
msgid "Sāvetukāmo na sāveti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.18"
msgid "Aviññussa sāveti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.19"
msgid "Viññussa na sāveti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.20"
msgid "Sabbaso vā pana na sāveti, apaccakkhātā hoti sikkhā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:69.21"
msgid "Evaṃ kho, bhikkhave, apaccakkhātā hoti sikkhā."
msgstr ""

#. </p><p><a class="sc" id="103"></a><a class="pts-cs" id="Bu-Pj.1.8.5"></a><a
#. class="ms-pa" id="ms.103"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pj1:70.1"
msgid "Methunadhammo nāma"
msgstr ""

#. </strong>
#. <span class="gloss">
msgctxt "pi-tv-bu-vb-pj1:70.2"
msgid "yo so asaddhammo gāmadhammo vasaladhammo duṭṭhullaṃ odakantikaṃ rahassaṃ dvayaṃ­d­vayasa­māpatti,"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:70.3"
msgid "eso methunadhammo nāma."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="104"></a><a class="ms-pa" id="ms.104"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pj1:71.1"
msgid "Paṭisevati nāma"
msgstr ""

#. </strong>
#. <span class="gloss">
msgctxt "pi-tv-bu-vb-pj1:71.2"
msgid "yo nimittena nimittaṃ aṅgajātena aṅgajātaṃ antamaso tilaphala­mattampi paveseti, eso paṭisevati nāma."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="105"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pj1:72.1"
msgid "Antamaso tiracchā­na­gatā­yapīti"
msgstr ""

#. </strong>
#. <a class="pts-vp-en" id="bd.1.48"></a>
#. <span class="gloss">
msgctxt "pi-tv-bu-vb-pj1:72.2"
msgid "tiracchā­na­ga­titthi­yāpi methunaṃ dhammaṃ paṭisevitvā assamaṇo hoti asakyaputtiyo, pageva manussitthiyā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:72.3"
msgid "Tena vuccati—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:72.4"
msgid "“antamaso tiracchā­na­gatā­yapī”ti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="106"></a><a class="ms-pa" id="ms.106"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pj1:73.1"
msgid "Pārājiko hotīti"
msgstr ""

#. </strong>
#. <span class="gloss">
msgctxt "pi-tv-bu-vb-pj1:73.2"
msgid "seyyathāpi nāma puriso sīsacchinno abhabbo tena sarīra­bandha­nena jīvituṃ;"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:73.3"
msgid "evameva bhikkhu methunaṃ dhammaṃ paṭisevitvā assamaṇo hoti asakyaputtiyo."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:73.4"
msgid "Tena vuccati—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:73.5"
msgid "“pārājiko hotī”ti."
msgstr ""

#. </span>
#. </p><p><a class="sc" id="107"></a><a class="ms-pa" id="ms.107"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pj1:74.1"
msgid "Asaṃvāsoti"
msgstr ""

#. </strong>
#. <span class="gloss"><strong class="term">
msgctxt "pi-tv-bu-vb-pj1:74.2"
msgid "saṃvāso nāma"
msgstr ""

#. </strong>
#. <span class="gloss">
msgctxt "pi-tv-bu-vb-pj1:74.3"
msgid "ekakammaṃ ekuddeso samasikkhatā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:74.4"
msgid "eso saṃvāso nāma."
msgstr ""

#. </span>
msgctxt "pi-tv-bu-vb-pj1:74.5"
msgid "So tena saddhiṃ natthi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:74.6"
msgid "Tena vuccati—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:74.7"
msgid "“asaṃvāso”ti."
msgstr ""

#. </span>
#. </p><h2>
msgctxt "pi-tv-bu-vb-pj1:75.1"
msgid ""
msgstr ""

#. </h2><h3>
msgctxt "pi-tv-bu-vb-pj1:75.2"
msgid ""
msgstr ""

#. </h3><h4>
msgctxt "pi-tv-bu-vb-pj1:75.3"
msgid ""
msgstr ""

#. </h4><p><a class="sc" id="108"></a><a class="pts-cs-empty" id="Bu-
#. Pj.1.9"></a> <a class="pts-cs" id="Bu-Pj.1.9.1"></a> <a class="ms-pa"
#. id="ms.108"></a>
msgctxt "pi-tv-bu-vb-pj1:75.4"
msgid "Tisso itthiyo—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:75.5"
msgid "manussitthī, amanussitthī, tiracchā­na­gatit­thī."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:75.6"
msgid "Tayo ubhatob­yañ­janakā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:75.7"
msgid "manus­subha­tob­yañ­janako, amanus­subha­tob­yañ­janako, tiracchā­na­ga­tubha­tob­yañ­janako."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:75.8"
msgid "Tayo paṇḍakā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:75.9"
msgid "manussapaṇḍako, amanus­sa­paṇḍako, tiracchā­na­gata­paṇḍako."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:75.10"
msgid "Tayo purisā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:75.11"
msgid "manussapuriso, amanussapuriso, tiracchā­na­gata­puriso."
msgstr ""

#. </p><h4>
msgctxt "pi-tv-bu-vb-pj1:76.1"
msgid ""
msgstr ""

#. </h4><p><a class="sc" id="109"></a><a class="ms-pa" id="ms.109"></a>
msgctxt "pi-tv-bu-vb-pj1:76.2"
msgid "Manussitthiyā tayo magge methunaṃ dhammaṃ paṭisevantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.3"
msgid "vaccamagge, passāvamagge, mukhe."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.4"
msgid "Amanussitthiyā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.5"
msgid "tiracchā­na­ga­titthiyā tayo magge methunaṃ dhammaṃ paṭisevantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.6"
msgid "vaccamagge, passāvamagge, mukhe."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.7"
msgid "Manus­subha­tob­yañ­jana­kassa …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.8"
msgid "amanus­subha­tob­yañ­jana­kassa …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.9"
msgid "tiracchā­na­ga­tubha­tob­yañ­jana­kassa tayo magge methunaṃ dhammaṃ paṭisevantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.10"
msgid "vaccamagge, passāvamagge, mukhe."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.11"
msgid "­Manus­sa­paṇḍa­kassa dve magge methunaṃ dhammaṃ paṭisevantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.12"
msgid "vaccamagge, mukhe."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.13"
msgid "Amanus­sa­paṇḍa­kassa …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.14"
msgid "tiracchā­na­gata­paṇḍa­kassa …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.15"
msgid "­manus­sa­purisassa …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.16"
msgid "amanus­sa­purisassa …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.17"
msgid "tiracchā­na­gata­purisassa dve magge methunaṃ dhammaṃ paṭisevantassa āpatti pārājikassa—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:76.18"
msgid "vaccamagge, mukhe."
msgstr ""

#. </p><h4>
msgctxt "pi-tv-bu-vb-pj1:77.1"
msgid ""
msgstr ""

