msgctxt "pi-tv-bu-vb-pj1:149.5"
msgid "“sādiyi tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:149.6"
msgid "“Nāhaṃ, bhagavā, sādiyin”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:149.7"
msgid "“Anāpatti, bhikkhu, asādiyantassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:149.8"
msgid "(50)"
msgstr ""

#. </p><hr><p><a class="sc" id="181"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.21"></a><a class="pts-vp-en" id="bd.1.60"></a> <a class="ms-pa"
#. id="ms.181"></a>
msgctxt "pi-tv-bu-vb-pj1:150.1"
msgid "Tena kho pana samayena aññataro bhikkhu vesāliyaṃ mahāvane kūṭā­gāra­sālāyaṃ divāvihāragato dvāraṃ vivaritvā nipanno hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:150.2"
msgid "Tassa aṅgamaṅgāni vātū­patthad­dhāni honti."
msgstr ""

#. <a class="pts-vp-pi" id="pts-vp-pi3.39"></a>
msgctxt "pi-tv-bu-vb-pj1:150.3"
msgid "Tena kho pana samayena sambahulā itthiyo gandhañca mālañca ādāya ārāmaṃ āgamaṃsu vihā­ra­pekkhi­kāyo."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:150.4"
msgid "Atha kho tā itthiyo taṃ bhikkhuṃ passitvā aṅgajāte abhinisīditvā yāvadatthaṃ katvā, purisūsabho vatāyanti vatvā gandhañca mālañca āropetvā pakkamiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:150.5"
msgid "Bhikkhū kilinnaṃ passitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

#. </p><p><span class="parallel">
msgctxt "pi-tv-bu-vb-pj1:150.6"
msgid "“Pañcahi, bhikkhave, ākārehi aṅgajātaṃ kammaniyaṃ hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:150.7"
msgid "rāgena, vaccena, passāvena, vātena, uccāliṅ­ga­pāṇaka­daṭ­ṭhena."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:150.8"
msgid "Imehi kho, bhikkhave, pañcahākārehi aṅgajātaṃ kammaniyaṃ hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:150.9"
msgid "Aṭṭhānametaṃ, bhikkhave, anavakāso, yaṃ tassa bhikkhuno rāgena aṅgajātaṃ kammaniyaṃ assa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:150.10"
msgid "Arahaṃ so, bhikkhave, bhikkhu."
msgstr ""

#. </span>
msgctxt "pi-tv-bu-vb-pj1:150.11"
msgid "Anāpatti, bhikkhave, tassa bhikkhuno."
msgstr ""

#. <strong class="rule">
msgctxt "pi-tv-bu-vb-pj1:150.12"
msgid "Anujānāmi, bhikkhave, divā paṭi­sal­lī­yan­tena dvāraṃ saṃvaritvā paṭisallīyitun”ti."
msgstr ""

#. </strong>
msgctxt "pi-tv-bu-vb-pj1:150.13"
msgid "(51)"
msgstr ""

#. </p><hr><p><a class="sc" id="182"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.22"></a><a class="ms-pa" id="ms.182"></a>
msgctxt "pi-tv-bu-vb-pj1:151.1"
msgid "Tena kho pana samayena aññataro bhārukacchako bhikkhu supinante purāṇa­duti­yikāya methunaṃ dhammaṃ paṭisevitvā—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:151.2"
msgid "“assamaṇo ahaṃ, vibbhamissāmī”ti,"
msgstr ""

#. <a class="pts-vp-en" id="bd.1.61"></a>
msgctxt "pi-tv-bu-vb-pj1:151.3"
msgid "bhārukacchaṃ gacchanto antarāmagge āyasmantaṃ upāliṃ passitvā etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:151.4"
msgid "Āyasmā upāli evamāha—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:151.5"
msgid "“anāpatti, āvuso, supinantenā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:151.6"
msgid "(52)"
msgstr ""

#. </p><hr><p><a class="sc" id="183"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.23"></a><a class="ms-pa" id="ms.183"></a>
msgctxt "pi-tv-bu-vb-pj1:152.1"
msgid "Tena kho pana samayena rājagahe supabbā nāma upāsikā mudhappasannā hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.2"
msgid "Sā evaṃdiṭṭhikā hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.3"
msgid "“yā methunaṃ dhammaṃ deti sā aggadānaṃ detī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.4"
msgid "Sā bhikkhuṃ passitvā etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.5"
msgid "“ehi, bhante, methunaṃ dhammaṃ paṭisevā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:152.6"
msgid "“Alaṃ, bhagini, netaṃ kappatī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:152.7"
msgid "“Ehi, bhante, ūruntarikāya ghaṭṭehi,"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.8"
msgid "evaṃ te anāpatti bhavissatī”ti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.9"
msgid "ehi, bhante, nābhiyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.10"
msgid "ehi, bhante, udaravaṭṭiyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.11"
msgid "ehi, bhante, upakacchake ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.12"
msgid "ehi, bhante, gīvāyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.13"
msgid "ehi, bhante, kaṇṇacchidde ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.14"
msgid "ehi, bhante, kesavaṭṭiyaṃ ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.15"
msgid "ehi, bhante, aṅgu­lanta­rikāya ghaṭṭehi …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.16"
msgid "“ehi, bhante, hatthena upakkamitvā mocessāmi,"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.17"
msgid "evaṃ te anāpatti bhavissatī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.18"
msgid "So bhikkhu tathā akāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.19"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.20"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.21"
msgid "Āpatti saṃ­ghā­di­sesassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:152.22"
msgid "(53–‍61)"
msgstr ""

#. </p><hr><p><a class="sc" id="184"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.24"></a><a class="ms-pa" id="ms.184"></a>
msgctxt "pi-tv-bu-vb-pj1:153.1"
msgid "Tena kho pana samayena sāvatthiyaṃ saddhā nāma upāsikā mudhappasannā hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.2"
msgid "Sā evaṃdiṭṭhikā hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.3"
msgid "“yā methunaṃ dhammaṃ deti sā aggadānaṃ detī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.4"
msgid "Sā bhikkhuṃ passitvā etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.5"
msgid "“ehi, bhante, methunaṃ dhammaṃ paṭisevā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:153.6"
msgid "“Alaṃ, bhagini, netaṃ kappatī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:153.7"
msgid "“Ehi, bhante, ūruntarikāya ghaṭṭehi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.8"
msgid "ehi, bhante, hatthena upakkamitvā mocessāmi,"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.9"
msgid "evaṃ te anāpatti bhavissatī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.10"
msgid "So bhikkhu tathā akāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.11"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.12"
msgid "“anāpatti, bhikkhu, pārājikassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.13"
msgid "Āpatti saṃ­ghā­di­sesassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:153.14"
msgid "(62–‍70)"
msgstr ""

#. </p><hr><p><a class="sc" id="185"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.25"></a><a class="ms-pa" id="ms.185"></a><a class="pts-vp-en"
#. id="bd.1.62"></a><a class="pts-vp-pi" id="pts-vp-pi3.40"></a><span
#. class="bhikkhuni">
msgctxt "pi-tv-bu-vb-pj1:154.1"
msgid "Tena kho pana samayena vesāliyaṃ ­liccha­vi­kumārakā bhikkhuṃ gahetvā bhikkhuniyā vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:154.2"
msgid "sikkhamānāya vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:154.3"
msgid "sāmaṇeriyā vippaṭipādesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:154.4"
msgid "Ubho sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:154.5"
msgid "Ubho nāsetabbā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:154.6"
msgid "Ubho na sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:154.7"
msgid "Ubhinnaṃ anāpatti."
msgstr ""

#. </span>
msgctxt "pi-tv-bu-vb-pj1:154.8"
msgid "(71–‍76)"
msgstr ""

#. </p><hr><p><a class="sc" id="186"></a><a class="ms-pa" id="ms.186"></a>
msgctxt "pi-tv-bu-vb-pj1:155.1"
msgid "Tena kho pana samayena vesāliyaṃ ­liccha­vi­kumārakā bhikkhuṃ gahetvā vesiyā vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:155.2"
msgid "paṇḍake vippaṭipādesuṃ …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:155.3"
msgid "gihiniyā vippaṭipādesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:155.4"
msgid "Bhikkhu sādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:155.5"
msgid "Bhikkhu nāsetabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:155.6"
msgid "Bhikkhu na sādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:155.7"
msgid "Bhikkhussa anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:155.8"
msgid "(77–‍82)"
msgstr ""

#. </p><hr><p><a class="sc" id="187"></a><a class="ms-pa" id="ms.187"></a>
msgctxt "pi-tv-bu-vb-pj1:156.1"
msgid "Tena kho pana samayena vesāliyaṃ ­liccha­vi­kumārakā bhikkhū gahetvā aññamaññaṃ vippaṭipādesuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:156.2"
msgid "Ubho sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:156.3"
msgid "Ubho nāsetabbā."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:156.4"
msgid "Ubho na sādiyiṃsu."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:156.5"
msgid "Ubhinnaṃ anāpatti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:156.6"
msgid "(83–‍84)"
msgstr ""

#. </p><hr><p><a class="sc" id="188"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.26"></a><a class="ms-pa" id="ms.188"></a>
msgctxt "pi-tv-bu-vb-pj1:157.1"
msgid "Tena kho pana samayena aññataro vuḍḍha­pabba­jito bhikkhu purāṇa­duti­yikāya dassanaṃ agamāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:157.2"
msgid "Sā—“ehi, bhante, vibbhamā”ti aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:157.3"
msgid "So bhikkhu paṭikkamanto uttāno paripati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:157.4"
msgid "Sā ubbhajitvā aṅgajāte abhinisīdi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:157.5"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:157.6"
msgid "“sādiyi tvaṃ, bhikkhū”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:157.7"
msgid "“Nāhaṃ, bhagavā, sādiyin”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pj1:157.8"
msgid "“Anāpatti, bhikkhu, asādiyantassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:157.9"
msgid "(85)"
msgstr ""

#. </p><hr><p><a class="sc" id="189"></a><a class="pts-cs" id="Bu-
#. Pj.1.10.27"></a><a class="ms-pa" id="ms.189"></a>
msgctxt "pi-tv-bu-vb-pj1:158.1"
msgid "Tena kho pana samayena aññataro bhikkhu araññe viharati."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:158.2"
msgid "Migapotako tassa passāvaṭṭhānaṃ āgantvā passāvaṃ pivanto mukhena aṅgajātaṃ aggahesi."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:158.3"
msgid "So bhikkhu sādiyi."
msgstr ""

#. <a class="pts-vp-en" id="bd.1.63"></a>
msgctxt "pi-tv-bu-vb-pj1:158.4"
msgid "Tassa kukkuccaṃ ahosi … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:158.5"
msgid "“āpattiṃ tvaṃ, bhikkhu, āpanno pārājikan”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pj1:158.6"
msgid "(86)"
msgstr ""

#. </p><hr><p class="endsutta"><a class="sc" id="190"></a><a class="ms-pa"
#. id="ms.190"></a>
msgctxt "pi-tv-bu-vb-pj1:159.1"
msgid "Paṭha­ma­pā­rāji­kaṃ samattaṃ."
msgstr ""

