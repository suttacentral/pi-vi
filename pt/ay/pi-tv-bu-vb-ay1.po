msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ay1:3.1"
msgid "Aniyatakaṇḍa"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-ay1:4.1"
msgid "1. Paṭhamaaniyatasikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.187"></a><a class="pts-vp-en" id="bd.1.330"></a><a class="ms-pa"
#. id="ms-pa1802"></a>
msgctxt "pi-tv-bu-vb-ay1:5.1"
msgid "Ime kho panāyasmanto dve aniyatā dhammā"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:5.2"
msgid "uddesaṃ āgacchanti."
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-ay1:6.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="2"></a><a class="pts-cs" id="bu-an.1.1"></a><a
#. class="ms-pa" id="ms-pa1803"></a>
msgctxt "pi-tv-bu-vb-ay1:6.2"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.3"
msgid "Tena kho pana samayena āyasmā udāyī sāvatthiyaṃ kulūpako hoti, bahukāni kulāni upasaṅkamati."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.4"
msgid "Tena kho pana samayena āyasmato udāyissa upaṭṭhākakulassa kumārikā aññatarassa kulassa kumārakassa dinnā hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.5"
msgid "Atha kho āyasmā udāyī pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya yena taṃ kulaṃ tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.6"
msgid "upasaṅkamitvā manusse pucchi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.7"
msgid "“kahaṃ itthannāmā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.8"
msgid "Te evamāhaṃsu—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.9"
msgid "“dinnā, bhante, amukassa kulassa kumārakassā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.10"
msgid "Tampi kho kulaṃ āyasmato udāyissa upaṭṭhākaṃ hoti."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.11"
msgid "Atha kho āyasmā udāyī yena taṃ kulaṃ tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.12"
msgid "upasaṅkamitvā manusse pucchi—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.13"
msgid "“kahaṃ itthannāmā”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.14"
msgid "Te evamāhaṃsu—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.15"
msgid "“esāyya, ovarake nisinnā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.16"
msgid "Atha kho āyasmā udāyī yena sā kumārikā tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:6.17"
msgid "upasaṅkamitvā tassā kumārikāya saddhiṃ eko ekāya raho paṭicchanne āsane alaṃkammaniye nisajjaṃ kappesi kālayuttaṃ samullapanto kālayuttaṃ dhammaṃ bhaṇanto."
msgstr ""

#. </p><p><a class="sc" id="3"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.188"></a><a class="ms-pa" id="ms-pa1804"></a><a class="pts-vp-en"
#. id="bd.1.331"></a>
msgctxt "pi-tv-bu-vb-ay1:7.1"
msgid "Tena kho pana samayena visākhā migāramātā bahuputtā hoti bahunattā arogaputtā aroganattā abhimaṅgalasammatā."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.2"
msgid "Manussā yaññesu chaṇesu ussavesu visākhaṃ migāramātaraṃ paṭhamaṃ bhojenti."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.3"
msgid "Atha kho visākhā migāramātā nimantitā taṃ kulaṃ agamāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.4"
msgid "Addasā kho visākhā migāramātā āyasmantaṃ udāyiṃ tassā kumārikāya saddhiṃ ekaṃ ekāya raho paṭicchanne āsane alaṃkammaniye nisinnaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.5"
msgid "Disvāna āyasmantaṃ udāyiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.6"
msgid "“idaṃ, bhante, nacchannaṃ nappatirūpaṃ yaṃ ayyo mātugāmena saddhiṃ eko ekāya raho paṭicchanne āsane alaṃkammaniye nisajjaṃ kappeti."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.7"
msgid "Kiñcāpi, bhante, ayyo anatthiko tena dhammena, api ca dussaddhāpayā appasannā manussā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.8"
msgid "Evampi kho āyasmā udāyī visākhāya migāramātuyā vuccamāno nādiyi."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.9"
msgid "Atha kho visākhā migāramātā nikkhamitvā bhikkhūnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.10"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.11"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:7.12"
msgid "“kathañhi nāma āyasmā udāyī mātugāmena saddhiṃ eko ekāya raho paṭicchanne āsane alaṃkammaniye nisajjaṃ kappessatī”ti."
msgstr ""

#. </p><p><a class="sc" id="4"></a><a class="ms-pa" id="ms-pa1805"></a>
msgctxt "pi-tv-bu-vb-ay1:8.1"
msgid "Atha kho te bhikkhū āyasmantaṃ udāyiṃ anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:8.2"
msgid "“saccaṃ kira tvaṃ, udāyi, mātugāmena saddhiṃ eko ekāya raho paṭicchanne āsane alaṃkammaniye nisajjaṃ kappesī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ay1:8.3"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-ay1:8.4"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:8.5"
msgid "kathañhi nāma tvaṃ, moghapurisa, mātugāmena saddhiṃ eko ekāya raho paṭicchanne āsane alaṃkammaniye nisajjaṃ kappessasi."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:8.6"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:8.7"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-ay1:9.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="5"></a><a class="ms-pa" id="ms-pa1806"></a>
#. <a class="pts-vp-en" id="bd.1.332"></a>
#. <strong class="rule">
msgctxt "pi-tv-bu-vb-ay1:9.2"
msgid "“Yo pana bhikkhu mātugāmena saddhiṃ eko ekāya raho paṭicchanne āsane alaṃkammaniye nisajjaṃ kappeyya, tamenaṃ saddheyyavacasā upāsikā disvā tiṇṇaṃ dhammānaṃ aññatarena vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:9.3"
msgid "pārājikena vā saṃghādisesena vā pācittiyena vā, nisajjaṃ bhikkhu paṭijānamāno tiṇṇaṃ dhammānaṃ aññatarena kāretabbo—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:9.4"
msgid "pārājikena vā saṃghādisesena vā pācittiyena vā, yena vā sā saddheyyavacasā upāsikā vadeyya, tena so bhikkhu kāretabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:9.5"
msgid "Ayaṃ dhammo aniyato”ti."
msgstr ""

#. </strong>
msgctxt "pi-tv-bu-vb-ay1:9.6"
msgid "(<em>1</em>:<strong>18</strong>)"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-ay1:10.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="6"></a><a class="pts-cs-empty" id="bu-
#. an.1.2"></a><a class="pts-cs" id="bu-an.1.2.1"></a><a class="ms-pa" id="ms-
#. pa1807"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:10.2"
msgid "Yo panāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:10.3"
msgid "yo yādiso … pe …"
msgstr ""

#. </span><strong class="term">
msgctxt "pi-tv-bu-vb-ay1:10.4"
msgid "bhikkhūti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:10.5"
msgid "… pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:10.6"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. </span></p><p><a class="sc" id="7"></a><a class="ms-pa" id="ms-pa1808"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:11.1"
msgid "Mātugāmo nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:11.2"
msgid "manussitthī, na yakkhī na petī na tiracchānagatā."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:11.3"
msgid "Antamaso tadahujātāpi dārikā, pageva mahattarī."
msgstr ""

#. </span></p><p><a class="sc" id="8"></a><a class="ms-pa" id="ms-pa1809"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:12.1"
msgid "Saddhinti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:12.2"
msgid "ekato."
msgstr ""

#. </span></p><p><a class="sc" id="9"></a><a class="ms-pa" id="ms-pa1810"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:13.1"
msgid "Eko ekāyāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:13.2"
msgid "bhikkhu ceva hoti mātugāmo ca."
msgstr ""

#. </span></p><p><a class="sc" id="10"></a><a class="ms-pa" id="ms-
#. pa1811"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:14.1"
msgid "Raho nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:14.2"
msgid "cakkhussa raho, sotassa raho."
msgstr ""

#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:14.3"
msgid "Cakkhussa raho nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:14.4"
msgid "na sakkā hoti akkhiṃ vā nikhaṇīyamāne bhamukaṃ vā ukkhipīyamāne sīsaṃ vā ukkhipīyamāne passituṃ."
msgstr ""

#. </span><strong class="term">
msgctxt "pi-tv-bu-vb-ay1:14.5"
msgid "Sotassa raho nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:14.6"
msgid "na sakkā hoti pakatikathā sotuṃ."
msgstr ""

#. </span></span></p><p><a class="sc" id="11"></a><a class="pts-vp-pi"
#. id="pts-vp-pi3.189"></a><a class="ms-pa" id="ms-pa1812"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:15.1"
msgid "Paṭicchannaṃ nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:15.2"
msgid "āsanaṃ kuṭṭena vā kavāṭena vā kilañjena vā sāṇipākārena vā rukkhena vā thambhena vā kotthaḷiyā vā yena kenaci paṭicchannaṃ hoti."
msgstr ""

#. </span></p><p><a class="sc" id="12"></a><a class="ms-pa" id="ms-
#. pa1813"></a><a class="pts-vp-en" id="bd.1.333"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:16.1"
msgid "Alaṃkammaniyeti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:16.2"
msgid "sakkā hoti methunaṃ dhammaṃ paṭisevituṃ."
msgstr ""

#. </span></p><p><a class="sc" id="13"></a><a class="ms-pa" id="ms-
#. pa1814"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:17.1"
msgid "Nisajjaṃ kappeyyāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:17.2"
msgid "mātugāme nisinne bhikkhu upanisinno vā hoti upanipanno vā."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:17.3"
msgid "Bhikkhu nisinne mātugāmo upanisinno vā hoti upanipanno vā."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:17.4"
msgid "Ubho vā nisinnā honti ubho vā nipannā."
msgstr ""

#. </span></p><p><a class="sc" id="14"></a><a class="ms-pa" id="ms-
#. pa1815"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:18.1"
msgid "Saddheyyavacasā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:18.2"
msgid "āgataphalā abhisametāvinī viññātasāsanā."
msgstr ""

#. </span></p><p><a class="sc" id="15"></a><a class="ms-pa" id="ms-
#. pa1816"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:19.1"
msgid "Upāsikā nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:19.2"
msgid "buddhaṃ saraṇaṃ gatā, dhammaṃ saraṇaṃ gatā, saṃghaṃ saraṇaṃ gatā."
msgstr ""

#. </span></p><p><a class="sc" id="16"></a><a class="ms-pa" id="ms-
#. pa1817"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-ay1:20.1"
msgid "Disvāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-ay1:20.2"
msgid "passitvā."
msgstr ""

#. </span></p><p><a class="sc" id="17"></a><a class="pts-cs" id="bu-
#. an.1.2.2"></a><a class="ms-pa" id="ms-pa1818"></a><strong class="term">
msgctxt "pi-tv-bu-vb-ay1:21.1"
msgid "Tiṇṇaṃ dhammānaṃ aññatarena vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:21.2"
msgid "pārājikena vā saṃghādisesena vā pācittiyena vā."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:21.3"
msgid "Nisajjaṃ bhikkhu paṭijānamāno tiṇṇaṃ dhammānaṃ aññatarena kāretabbo—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:21.4"
msgid "pārājikena vā saṃghādisesena vā pācittiyena vā."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:21.5"
msgid "Yena vā sā saddheyyavacasā upāsikā vadeyya tena so bhikkhu kāretabbo."
msgstr ""

#. </strong></p><div class="gloss"><p><a class="sc" id="18"></a><a class="ms-
#. pa" id="ms-pa1819"></a>
msgctxt "pi-tv-bu-vb-ay1:22.1"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.2"
msgid "“ayyo mayā diṭṭho nisinno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.3"
msgid "so ca taṃ paṭijānāti, āpattiyā kāretabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.4"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.5"
msgid "“ayyo mayā diṭṭho nisinno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.6"
msgid "so ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.7"
msgid "“saccāhaṃ nisinno, no ca kho methunaṃ dhammaṃ paṭisevin”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.8"
msgid "nisajjāya kāretabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.9"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

#. <a class="pts-vp-en" id="bd.1.334"></a>
msgctxt "pi-tv-bu-vb-ay1:22.10"
msgid "“ayyo mayā diṭṭho nisinno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.11"
msgid "so ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.12"
msgid "“nāhaṃ nisinno, api ca kho nipanno”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.13"
msgid "nipajjāya kāretabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.14"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.15"
msgid "“ayyo mayā diṭṭho nisinno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.16"
msgid "so ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.17"
msgid "“nāhaṃ nisinno, api ca kho ṭhito”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:22.18"
msgid "na kāretabbo."
msgstr ""

#. </p><p><a class="sc" id="19"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.190"></a><a class="ms-pa" id="ms-pa1820"></a>
msgctxt "pi-tv-bu-vb-ay1:23.1"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.2"
msgid "“ayyo mayā diṭṭho nipanno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.3"
msgid "so ca taṃ paṭijānāti, āpattiyā kāretabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.4"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.5"
msgid "“ayyo mayā diṭṭho nipanno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.6"
msgid "so ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.7"
msgid "“saccāhaṃ nipanno, no ca kho methunaṃ dhammaṃ paṭisevin”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.8"
msgid "nipajjāya kāretabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.9"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.10"
msgid "“ayyo mayā diṭṭho nipanno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.11"
msgid "so ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.12"
msgid "“nāhaṃ nipanno, api ca kho nisinno”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.13"
msgid "nisajjāya kāretabbo."
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.14"
msgid "Sā ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.15"
msgid "“ayyo mayā diṭṭho nipanno mātugāmassa methunaṃ dhammaṃ paṭisevanto”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.16"
msgid "so ce evaṃ vadeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.17"
msgid "“nāhaṃ nipanno, api ca kho ṭhito”ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-ay1:23.18"
msgid "na kāretabbo."
msgstr ""

