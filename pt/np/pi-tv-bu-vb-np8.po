#. lang="pi"><section class="sutta" id="pi-tv-bu-vb-np8"><article><div
#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-np8:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p class="division">
msgctxt "pi-tv-bu-vb-np8:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:4.1"
msgid "Cīvaravagga"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:4.2"
msgid ""
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-np8:5.1"
msgid "8. Upakkhaṭasikkhāpada"
msgstr ""

#. </h1></div><h2>
msgctxt "pi-tv-bu-vb-np8:6.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="1"></a><a class="pts-cs" id="bu-np.8.1"></a><a
#. class="pts-vp-en" id="bd.2.53"></a><a class="wt-pa" id="2015"></a>
msgctxt "pi-tv-bu-vb-np8:6.2"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:6.3"
msgid "Tena kho pana samayena aññataro puriso pajāpatiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:6.4"
msgid "“ayyaṃ upanandaṃ cīvarena acchādessāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:6.5"
msgid "Assosi kho aññataro piṇḍacāriko bhikkhu tassa purisassa imaṃ vācaṃ bhāsamānassa."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:6.6"
msgid "Atha kho so bhikkhu yenāyasmā upanando sakyaputto tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:6.7"
msgid "upasaṅkamitvā āyasmantaṃ upanandaṃ sakyaputtaṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:6.8"
msgid "“mahāpuññosi tvaṃ, āvuso upananda."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:6.9"
msgid "Amukasmiṃ okāse aññataro puriso pajāpatiṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:6.10"
msgid "‘ayyaṃ upanandaṃ cīvarena acchādessāmī’”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:6.11"
msgid "“Atthāvuso, maṃ so upaṭṭhāko”ti."
msgstr ""

#. </p><p><a class="sc" id="2"></a><a class="wt-pa" id="2016"></a>
msgctxt "pi-tv-bu-vb-np8:7.1"
msgid "Atha kho āyasmā upanando sakyaputto yena so puriso tenupasaṅkami;"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:7.2"
msgid "upasaṅkamitvā taṃ purisaṃ etadavoca—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:7.3"
msgid "“saccaṃ kira maṃ tvaṃ, āvuso, cīvarena acchādetukāmosī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:7.4"
msgid "“Api meyya, evaṃ hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:7.5"
msgid "‘ayyaṃ upanandaṃ cīvarena acchādessāmī’”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:7.6"
msgid "“Sace kho maṃ tvaṃ, āvuso, cīvarena acchādetukāmosi, evarūpena cīvarena acchādehi."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:7.7"
msgid "Kyāhaṃ tena acchannopi karissāmi yāhaṃ na paribhuñjissāmī”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:7.8"
msgid "Atha kho so puriso ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:7.9"
msgid "“mahicchā ime samaṇā sakyaputtiyā asantuṭṭhā."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:7.10"
msgid "Nayime sukarā cīvarena acchādetuṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:7.11"
msgid "Kathañhi nāma ayyo upanando mayā pubbe appavārito maṃ upasaṅkamitvā cīvare vikappaṃ āpajjissatī”ti."
msgstr ""

#. </p><p><a class="sc" id="3"></a><a class="pts-vp-en" id="bd.2.54"></a><a
#. class="wt-pa" id="2017"></a>
msgctxt "pi-tv-bu-vb-np8:8.1"
msgid "Assosuṃ kho bhikkhū tassa purisassa ujjhāyantassa khiyyantassa vipācentassa."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:8.2"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:8.3"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:8.4"
msgid "“kathañhi nāma āyasmā upanando sakyaputto pubbe appavārito gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpajjissatī”ti."
msgstr ""

#. </p><p><a class="sc" id="4"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.216"></a><a class="wt-pa" id="2018"></a>
msgctxt "pi-tv-bu-vb-np8:9.1"
msgid "Atha kho te bhikkhū āyasmantaṃ upanandaṃ sakyaputtaṃ anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:9.2"
msgid "“saccaṃ kira tvaṃ, upananda, pubbe appavārito gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpajjasī”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:9.3"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:9.4"
msgid "“Ñātako te, upananda, aññātako”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:9.5"
msgid "“Aññātako, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:9.6"
msgid "“Aññātako, moghapurisa, aññātakassa na jānāti patirūpaṃ vā appatirūpaṃ vā santaṃ vā asantaṃ vā."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:9.7"
msgid "Tattha nāma tvaṃ, moghapurisa, pubbe appavārito aññātakaṃ gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpajjissasi."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:9.8"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:9.9"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-np8:10.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="5"></a><a class="wt-pa" id="2019"></a>
#. <strong class="rule">
msgctxt "pi-tv-bu-vb-np8:10.2"
msgid "“Bhikkhuṃ paneva uddissa aññātakassa gahapatissa vā gahapatāniyā vā cīvaracetāpannaṃ upakkhaṭaṃ hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:10.3"
msgid "‘iminā cīvaracetāpannena cīvaraṃ cetāpetvā itthannāmaṃ bhikkhuṃ cīvarena acchādessāmī’ti;"
msgstr ""

#. <a class="pts-vp-en" id="bd.2.55"></a>
msgctxt "pi-tv-bu-vb-np8:10.4"
msgid "tatra ce so bhikkhu pubbe appavārito upasaṅkamitvā cīvare vikappaṃ āpajjeyya—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:10.5"
msgid "‘sādhu vata maṃ āyasmā iminā cīvaracetāpannena evarūpaṃ vā evarūpaṃ vā cīvaraṃ cetāpetvā acchādehī’ti,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:10.6"
msgid "kalyāṇakamyataṃ upādāya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. </strong>
msgctxt "pi-tv-bu-vb-np8:10.7"
msgid "(<em>8</em>:<strong>27</strong>)"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-np8:11.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="6"></a><a class="pts-cs" id="bu-np.8.2"></a><a
#. class="pts-cs" id="bu-np.8.2.1"></a><a class="wt-pa" id="2020"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:11.2"
msgid "Bhikkhuṃ paneva uddissāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:11.3"
msgid "bhikkhussatthāya, bhikkhuṃ ārammaṇaṃ karitvā, bhikkhuṃ acchādetukāmo."
msgstr ""

#. </span></p><p><a class="sc" id="7"></a><a class="wt-pa" id="2021"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:12.1"
msgid "Aññātako nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:12.2"
msgid "mātito vā pitito vā yāva sattamā pitāmahayugā asambaddho."
msgstr ""

#. </span></p><p><a class="sc" id="8"></a><a class="wt-pa" id="2022"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:13.1"
msgid "Gahapati nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:13.2"
msgid "yo koci agāraṃ ajjhāvasati."
msgstr ""

#. </span></p><p><a class="sc" id="9"></a><a class="wt-pa" id="2023"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:14.1"
msgid "Gahapatānī nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:14.2"
msgid "yā kāci agāraṃ ajjhāvasati."
msgstr ""

#. </span></p><p><a class="sc" id="10"></a><a class="wt-pa" id="2024"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:15.1"
msgid "Cīvaracetāpannaṃ nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:15.2"
msgid "hiraññaṃ vā suvaṇṇaṃ vā muttā vā maṇi vā pavāḷo vā phaliko vā paṭako vā suttaṃ vā kappāso vā."
msgstr ""

#. </span></p><p><a class="sc" id="11"></a><a class="wt-pa" id="2025"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:16.1"
msgid "Iminā cīvaracetāpannenāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:16.2"
msgid "paccupaṭṭhitena."
msgstr ""

#. </span></p><p><a class="sc" id="12"></a><a class="wt-pa" id="2026"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:17.1"
msgid "Cetāpetvāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:17.2"
msgid "parivattetvā."
msgstr ""

#. </span></p><p><a class="sc" id="13"></a><a class="wt-pa" id="2027"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:18.1"
msgid "Acchādessāmīti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:18.2"
msgid "dassāmi."
msgstr ""

#. </span></p><p><a class="sc" id="14"></a><a class="wt-pa" id="2028"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:19.1"
msgid "Tatra ce so bhikkhūti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:19.2"
msgid "yaṃ bhikkhuṃ uddissa cīvaracetāpannaṃ upakkhaṭaṃ hoti so bhikkhu."
msgstr ""

#. </span></p><p><a class="sc" id="15"></a><a class="wt-pa" id="2029"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:20.1"
msgid "Pubbe appavāritoti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:20.2"
msgid "pubbe avutto hoti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:20.3"
msgid "“kīdisena te, bhante, cīvarena attho,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:20.4"
msgid "kīdisaṃ te cīvaraṃ cetāpemī”ti?"
msgstr ""

#. </span></p><p><a class="sc" id="16"></a><a class="wt-pa" id="2030"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:21.1"
msgid "Upasaṅkamitvāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:21.2"
msgid "gharaṃ gantvā yattha katthaci upasaṅkamitvā."
msgstr ""

#. </span></p><p><a class="sc" id="17"></a><a class="wt-pa" id="2031"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:22.1"
msgid "Cīvare vikappaṃ āpajjeyyāti"
msgstr ""

#. </strong><a class="pts-vp-en" id="bd.2.56"></a><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:22.2"
msgid "āyataṃ vā hotu vitthataṃ vā appitaṃ vā saṇhaṃ vā."
msgstr ""

#. </span></p><p><a class="sc" id="18"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.217"></a><a class="wt-pa" id="2032"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:23.1"
msgid "Iminā cīvaracetāpannenāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:23.2"
msgid "paccupaṭṭhitena."
msgstr ""

#. </span></p><p><a class="sc" id="19"></a><a class="wt-pa" id="2033"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:24.1"
msgid "Evarūpaṃ vā evarūpaṃ vāti."
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:24.2"
msgid "Āyataṃ vā vitthataṃ vā appitaṃ vā saṇhaṃ vā."
msgstr ""

#. </span></p><p><a class="sc" id="20"></a><a class="wt-pa" id="2034"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:25.1"
msgid "Cetāpetvāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:25.2"
msgid "parivattetvā."
msgstr ""

#. </span></p><p><a class="sc" id="21"></a><a class="wt-pa" id="2035"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:26.1"
msgid "Acchādehīti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:26.2"
msgid "dajjehi."
msgstr ""

#. </span></p><p><a class="sc" id="22"></a><a class="wt-pa" id="2036"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np8:27.1"
msgid "Kalyāṇakamyataṃ upādāyāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np8:27.2"
msgid "sādhatthiko mahagghatthiko."
msgstr ""

#. </span></p><hr><p>
msgctxt "pi-tv-bu-vb-np8:27.3"
msgid "Tassa vacanena āyataṃ vā vitthataṃ vā appitaṃ vā saṇhaṃ vā cetāpeti, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:27.4"
msgid "Paṭilābhena nissaggiyaṃ hoti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np8:27.5"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā puggalassa vā."
msgstr ""

#. <span class="kamma">
msgctxt "pi-tv-bu-vb-np8:27.6"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ …"
msgstr ""

#. <span class="add">
msgctxt "pi-tv-bu-vb-np8:27.7"
msgid "pe …"
msgstr ""

#. </span>
msgctxt "pi-tv-bu-vb-np8:27.8"
msgid "“idaṃ me, bhante, cīvaraṃ pubbe appavārito aññātakaṃ gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpannaṃ nissaggiyaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:27.9"
msgid "imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:27.10"
msgid "dadeyyāti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:27.11"
msgid "dadeyyunti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:27.12"
msgid "āyasmato dammīti."
msgstr ""

#. </span></p><h2>
msgctxt "pi-tv-bu-vb-np8:28.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="23"></a><a class="pts-cs" id="bu-np.8.2.2"></a><a
#. class="wt-pa" id="2037"></a>
msgctxt "pi-tv-bu-vb-np8:28.2"
msgid "Aññātake aññātakasaññī pubbe appavārito gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpajjati, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:28.3"
msgid "Aññātake vematiko pubbe appavārito gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpajjati, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:28.4"
msgid "Aññātake ñātakasaññī pubbe appavārito gahapatikaṃ upasaṅkamitvā cīvare vikappaṃ āpajjati, nissaggiyaṃ pācittiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="24"></a><a class="wt-pa" id="2038"></a>
msgctxt "pi-tv-bu-vb-np8:29.1"
msgid "Ñātake aññātakasaññī, āpatti dukkaṭassa."
msgstr ""

#. <a class="pts-vp-en" id="bd.2.57"></a>
msgctxt "pi-tv-bu-vb-np8:29.2"
msgid "Ñātake vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-np8:29.3"
msgid "Ñātake ñātakasaññī, anāpatti."
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-np8:30.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="25"></a><a class="pts-cs" id="bu-np.8.2.3"></a><a
#. class="wt-pa" id="2039"></a>
msgctxt "pi-tv-bu-vb-np8:30.2"
msgid "Anāpatti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:30.3"
msgid "ñātakānaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:30.4"
msgid "pavāritānaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:30.5"
msgid "aññassatthāya,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:30.6"
msgid "attano dhanena,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:30.7"
msgid "mahagghaṃ cetāpetukāmassa appagghaṃ cetāpeti,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:30.8"
msgid "ummattakassa,"
msgstr ""

msgctxt "pi-tv-bu-vb-np8:30.9"
msgid "ādikammikassāti."
msgstr ""

#. </p><hr><p class="endsutta"><a class="sc" id="26"></a><a class="wt-pa"
#. id="2040"></a>
msgctxt "pi-tv-bu-vb-np8:31.1"
msgid "Upakkhaṭasikkhāpadaṃ niṭṭhitaṃ aṭṭhamaṃ."
msgstr ""

