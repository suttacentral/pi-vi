#. lang="pi"><section class="sutta" id="pi-tv-bu-vb-np13"><article><div
#. class="hgroup"><p class="division">
msgctxt "pi-tv-bu-vb-np13:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. </p><p class="division">
msgctxt "pi-tv-bu-vb-np13:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np13:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np13:4.1"
msgid "Kosiyavagga"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:4.2"
msgid ""
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-np13:5.1"
msgid "13. Dvebhāgasikkhāpada"
msgstr ""

#. </h1></div><h2>
msgctxt "pi-tv-bu-vb-np13:6.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="1"></a>
msgctxt "pi-tv-bu-vb-np13:6.2"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.3"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū—“bhagavatā paṭikkhittaṃ suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpetun”ti, te thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpenti."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.4"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.5"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:6.6"
msgid "“kathañhi nāma chabbaggiyā bhikkhū thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpessantī”ti."
msgstr ""

#. </p><p><a class="sc" id="2"></a><a class="wt-pa" id="2122"></a>
msgctxt "pi-tv-bu-vb-np13:7.1"
msgid "Atha kho te bhikkhū chabbaggiye bhikkhū anekapariyāyena vigarahitvā bhagavato etamatthaṃ ārocesuṃ … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.2"
msgid "“saccaṃ kira tumhe, bhikkhave, thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpethā”ti?"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np13:7.3"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-np13:7.4"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.5"
msgid "kathañhi nāma tumhe, moghapurisā, thokaññeva odātaṃ ante ādiyitvā tatheva suddhakāḷakānaṃ eḷakalomānaṃ santhataṃ kārāpessatha."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.6"
msgid "Netaṃ, moghapurisā, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:7.7"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-np13:8.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="3"></a><a class="wt-pa" id="2123"></a>
#. <strong class="rule">
msgctxt "pi-tv-bu-vb-np13:8.2"
msgid "“Navaṃ pana bhikkhunā santhataṃ kārayamānena dve bhāgā suddhakāḷakānaṃ eḷakalomānaṃ ādātabbā tatiyaṃ odātānaṃ catutthaṃ gocariyānaṃ."
msgstr ""

#. <a class="pts-vp-en" id="bd.2.77"></a>
msgctxt "pi-tv-bu-vb-np13:8.3"
msgid "Anādā ce bhikkhu dve bhāge suddhakāḷakānaṃ eḷakalomānaṃ tatiyaṃ odātānaṃ catutthaṃ gocariyānaṃ navaṃ santhataṃ kārāpeyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. </strong>
msgctxt "pi-tv-bu-vb-np13:8.4"
msgid "(<em>13</em>:<strong>32</strong>)"
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-np13:9.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="4"></a><a class="pts-cs" id="bu-np.13.2"></a><a
#. class="wt-pa" id="2124"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np13:9.2"
msgid "Navaṃ nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np13:9.3"
msgid "karaṇaṃ upādāya vuccati."
msgstr ""

#. </span></p><p><a class="sc" id="5"></a><a class="wt-pa" id="2125"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np13:10.1"
msgid "Santhataṃ nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np13:10.2"
msgid "santharitvā kataṃ hoti avāyimaṃ."
msgstr ""

#. </span></p><p><a class="sc" id="6"></a><a class="wt-pa" id="2126"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np13:11.1"
msgid "Kārayamānenāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np13:11.2"
msgid "karonto vā kārāpento vā."
msgstr ""

#. </span></p><p><a class="sc" id="7"></a><a class="wt-pa" id="2127"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np13:12.1"
msgid "Dve bhāgā suddhakāḷakānaṃ eḷakalomānaṃ ādātabbāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np13:12.2"
msgid "dhārayitvā dve tulā ādātabbā."
msgstr ""

#. </span></p><p><a class="sc" id="8"></a><a class="wt-pa" id="2128"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np13:13.1"
msgid "Tatiyaṃ odātānanti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np13:13.2"
msgid "tulaṃ odātānaṃ."
msgstr ""

#. </span></p><p><a class="sc" id="9"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np13:14.1"
msgid "Catutthaṃ gocariyānanti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np13:14.2"
msgid "tulaṃ gocariyānaṃ."
msgstr ""

#. </span></p><p><a class="sc" id="10"></a><a class="pts-vp-pi" id="pts-vp-
#. pi3.227"></a><a class="wt-pa" id="2130"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-np13:15.1"
msgid "Anādā ce bhikkhu dve bhāge suddhakāḷakānaṃ eḷakalomānaṃ tatiyaṃ odātānaṃ catutthaṃ gocariyānanti."
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-np13:15.2"
msgid "Anādiyitvā dve tule suddhakāḷakānaṃ eḷakalomānaṃ tulaṃ odātānaṃ tulaṃ gocariyānaṃ navaṃ santhataṃ karoti vā kārāpeti vā payoge dukkaṭaṃ,"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.3"
msgid "paṭilābhena nissaggiyaṃ hoti."
msgstr ""

#. </span></p><hr><p>
msgctxt "pi-tv-bu-vb-np13:15.4"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā puggalassa vā."
msgstr ""

#. <span class="kamma">
msgctxt "pi-tv-bu-vb-np13:15.5"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ …"
msgstr ""

#. <span class="add">
msgctxt "pi-tv-bu-vb-np13:15.6"
msgid "pe …"
msgstr ""

#. </span>
msgctxt "pi-tv-bu-vb-np13:15.7"
msgid "“idaṃ me, bhante, santhataṃ anādiyitvā dve tule suddhakāḷakānaṃ eḷakalomānaṃ tulaṃ odātānaṃ tulaṃ gocariyānaṃ kārāpitaṃ nissaggiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.8"
msgid "Imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.9"
msgid "dadeyyāti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.10"
msgid "dadeyyunti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:15.11"
msgid "āyasmato dammīti."
msgstr ""

#. </span></p><h2>
msgctxt "pi-tv-bu-vb-np13:16.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="11"></a><a class="wt-pa" id="2131"></a>
msgctxt "pi-tv-bu-vb-np13:16.2"
msgid "Attanā vippakataṃ attanā pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:16.3"
msgid "Attanā vippakataṃ parehi pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:16.4"
msgid "Parehi vippakataṃ attanā pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:16.5"
msgid "Parehi vippakataṃ parehi pariyosāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

#. </p><p><a class="sc" id="12"></a><a class="wt-pa" id="2132"></a>
msgctxt "pi-tv-bu-vb-np13:17.1"
msgid "Aññassatthāya karoti vā kārāpeti vā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-np13:17.2"
msgid "Aññena kataṃ paṭilabhitvā paribhuñjati, āpatti dukkaṭassa."
msgstr ""

#. </p><h2>
msgctxt "pi-tv-bu-vb-np13:18.1"
msgid ""
msgstr ""

#. </h2><p><a class="sc" id="13"></a><a class="wt-pa" id="2133"></a>
msgctxt "pi-tv-bu-vb-np13:18.2"
msgid "Anāpatti—"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:18.3"
msgid "tulaṃ odātānaṃ tulaṃ gocariyānaṃ ādiyitvā karoti,"
msgstr ""

#. <a class="pts-vp-en" id="bd.2.78"></a>
msgctxt "pi-tv-bu-vb-np13:18.4"
msgid "bahutaraṃ odātānaṃ bahutaraṃ gocariyānaṃ ādiyitvā karoti,"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:18.5"
msgid "suddhaṃ odātānaṃ suddhaṃ gocariyānaṃ ādiyitvā karoti,"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:18.6"
msgid "vitānaṃ vā bhūmattharaṇaṃ vā sāṇipākāraṃ vā bhisiṃ vā bibbohanaṃ vā karoti,"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:18.7"
msgid "ummattakassa,"
msgstr ""

msgctxt "pi-tv-bu-vb-np13:18.8"
msgid "ādikammikassāti."
msgstr ""

#. </p><hr><p class="endsutta"><a class="sc" id="14"></a><a class="wt-pa"
#. id="2134"></a>
msgctxt "pi-tv-bu-vb-np13:19.1"
msgid "Dvebhāgasikkhāpadaṃ niṭṭhitaṃ tatiyaṃ."
msgstr ""

