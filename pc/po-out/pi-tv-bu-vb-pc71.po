msgid "Theravāda Vinaya"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pc71:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. </p><p>
msgctxt "pi-tv-bu-vb-pc71:3.1"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession (<i lang=\"pi\">pācittiya</i>)"

#. </p><p>
msgctxt "pi-tv-bu-vb-pc71:4.1"
msgid "Sahadhammikavagga"
msgstr ""

#. </p><h1>
msgctxt "pi-tv-bu-vb-pc71:5.1"
msgid "71. Sahadhammikasikkhāpada"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="pts-vp-pi" id="pts-vp-pi4.141"></a>
msgctxt "pi-tv-bu-vb-pc71:6.1"
msgid "Tena samayena buddho bhagavā kosambiyaṃ viharati ghositārāme."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:6.2"
msgid "Tena kho pana samayena āyasmā channo anācāraṃ ācarati."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:6.3"
msgid "Bhikkhū evamāhaṃsu—"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:6.4"
msgid "“māvuso channa, evarūpaṃ akāsi."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:6.5"
msgid "Netaṃ kappatī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:6.6"
msgid "So evaṃ vadeti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:6.7"
msgid "“na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paripucchāmī”ti."
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "pi-tv-bu-vb-pc71:7.1"
msgid "Ye te bhikkhū appicchā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.3"
msgid "te ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.4"
msgid "“kathañhi nāma āyasmā channo bhikkhūhi sahadhammikaṃ vuccamāno evaṃ vakkhati—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.5"
msgid "na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paripucchāmī’”ti … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.7"
msgid "“saccaṃ kira tvaṃ, channa, bhikkhūhi sahadhammikaṃ vuccamāno evaṃ vadesi—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.8"
msgid "na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paripucchāmī’”ti?"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.9"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.10"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.12"
msgid "kathañhi nāma tvaṃ, moghapurisa, bhikkhūhi sahadhammikaṃ vuccamāno evaṃ vakkhasi—‘"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.13"
msgid "na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paripucchāmī’ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.14"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:7.16"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. </p><p><a class="sc" id="3"></a>
msgctxt "pi-tv-bu-vb-pc71:8.1"
msgid "<strong class=\"rule\">“Yo pana bhikkhu bhikkhūhi sahadhammikaṃ vuccamāno evaṃ vadeyya—‘"
msgstr ""

#. <span class="var" hidden>yāva na aññaṃ → yāva naññaṃ (s1-3)</span>
msgctxt "pi-tv-bu-vb-pc71:8.2"
msgid "na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paripucchāmī’ti, pācittiyaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:8.3"
msgid "Sikkhamānena, bhikkhave, bhikkhunā aññātabbaṃ paripucchitabbaṃ paripañhitabbaṃ."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:8.4"
msgid "Ayaṃ tattha sāmīcī”</strong>ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:8.5"
msgid "(<em>71</em>:<strong>120</strong>)"
msgstr ""

#. </p><p><a class="sc" id="4"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:9.1"
msgid "Yo panāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:9.1"
msgid "yo yādiso … pe …"
msgstr ""

#. </span>

#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:9.3"
msgid "bhikkhūti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:9.3"
msgid "… pe …"
msgstr ""

#. </span>

msgctxt "pi-tv-bu-vb-pc71:9.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. </p><p><a class="sc" id="5"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:10.1"
msgid "Bhikkhūhīti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:10.1"
msgid "aññehi bhikkhūhi."
msgstr ""

#. </span>

#. </p><p><a class="sc" id="6"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:11.1"
msgid "Sahadhammikaṃ nāma"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:11.1"
msgid "yaṃ bhagavatā paññattaṃ sikkhāpadaṃ etaṃ sahadhammikaṃ nāma."
msgstr ""

#. </span>
#. <span class="var" hidden>ti → imāni padāni s1-3 potthakesu na </span><span class="var" hidden>vadeti → vadeyyāti (bj, mr)</span>
msgctxt "pi-tv-bu-vb-pc71:11.2"
msgid "Tena vuccamāno evaṃ vadeti—“"
msgstr ""

#. <span class="var" hidden>byattaṃ → imāni padāni s1-3 potthakesu na</span>
msgctxt "pi-tv-bu-vb-pc71:11.3"
msgid "na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paripucchāmī”ti."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:11.4"
msgid "Paṇḍitaṃ byattaṃ medhāviṃ bahussutaṃ dhammakathikaṃ paripucchāmīti bhaṇati, āpatti pācittiyassa."
msgstr ""

#. </p><p><a class="sc" id="7"></a><a class="pts-vp-pi" id="pts-vp-pi4.142"></a>
msgctxt "pi-tv-bu-vb-pc71:12.1"
msgid "Upasampanne upasampannasaññī evaṃ vadeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:12.2"
msgid "Upasampanne vematiko evaṃ vadeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:12.3"
msgid "Upasampanne anupasampannasaññī evaṃ vadeti, āpatti pācittiyassa."
msgstr ""

#. </p><p><a class="sc" id="8"></a>
msgctxt "pi-tv-bu-vb-pc71:13.1"
msgid "Apaññattena vuccamāno—"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:13.2"
msgid "“idaṃ na sallekhāya na dhutatthāya na pāsādikatāya na apacayāya na vīriyārambhāya saṃvattatī”ti evaṃ vadeti, “na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paṇḍitaṃ medhāviṃ bahussutaṃ dhammakathikaṃ paripucchāmī”ti bhaṇati, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="9"></a>
msgctxt "pi-tv-bu-vb-pc71:14.1"
msgid "Anupasampannena paññattena vā apaññattena vā vuccamāno—"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:14.2"
msgid "“idaṃ na sallekhāya na dhutatthāya na pāsādikatāya na apacayāya na vīriyārambhāya saṃvattatī”ti evaṃ vadeti, “na tāvāhaṃ, āvuso, etasmiṃ sikkhāpade sikkhissāmi yāva na aññaṃ bhikkhuṃ byattaṃ vinayadharaṃ paṇḍitaṃ medhāviṃ bahussutaṃ dhammakathikaṃ paripucchāmī”ti bhaṇati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:14.3"
msgid "Anupasampanne upasampannasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:14.4"
msgid "Anupasampanne vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:14.5"
msgid "Anupasampanne anupasampannasaññī, āpatti dukkaṭassa."
msgstr ""

#. </p><p><a class="sc" id="10"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:15.1"
msgid "Sikkhamānenāti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:15.1"
msgid "sikkhitukāmena."
msgstr ""

#. </span>

#. </p><p><a class="sc" id="11"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:16.1"
msgid "Aññātabbanti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:16.1"
msgid "jānitabbaṃ."
msgstr ""

#. </span>

#. </p><p><a class="sc" id="12"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:17.1"
msgid "Paripucchitabbanti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:17.1"
msgid "“idaṃ, bhante, kathaṃ;"
msgstr ""

#. </span>

msgctxt "pi-tv-bu-vb-pc71:17.2"
msgid "imassa vā kvattho”ti?"
msgstr ""

#. </p><p><a class="sc" id="13"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:18.1"
msgid "Paripañhitabbanti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:18.1"
msgid "cintetabbaṃ tulayitabbaṃ."
msgstr ""

#. </span>

#. </p><p><a class="sc" id="14"></a>
#. <strong class="term">
msgctxt "pi-tv-bu-vb-pc71:19.1"
msgid "Ayaṃ tattha sāmīcīti"
msgstr ""

#. </strong><span class="gloss">
msgctxt "pi-tv-bu-vb-pc71:19.1"
msgid "ayaṃ tattha anudhammatā."
msgstr ""

#. </span>

#. </p><p><a class="sc" id="15"></a>
msgctxt "pi-tv-bu-vb-pc71:20.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pi-tv-bu-vb-pc71:20.2"
msgid "“jānissāmi sikkhissāmī”ti bhaṇati, ummattakassa, ādikammikassāti."
msgstr ""

#. </p><p class="endsutta"><a class="sc" id="16"></a>
msgctxt "pi-tv-bu-vb-pc71:21.1"
msgid "Sahadhammikasikkhāpadaṃ niṭṭhitaṃ paṭhamaṃ."
msgstr ""

